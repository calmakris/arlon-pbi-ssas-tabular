            {
              "name": "AOP Var",
              "expression": [
                "IF (",
                "    NOT ISBLANK( [Expenses Amount] )",
                "        && NOT ISBLANK( [Expenses Plan] ),",
                "    [Expenses Amount] - [Expenses Plan]",
                ")"
              ],
              "ordinal": 4
            }

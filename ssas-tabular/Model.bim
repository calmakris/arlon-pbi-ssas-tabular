              "        SUM( 'Sales Order'[CustomerOpenMarginAmountUSD] )   ",
              "    ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Backorder Snapshot Amount PD",
            "description": "Snapshot of the amount of open sales order line items yesterday",
            "expression": [
              " ",
              "CALCULATE(",
              "    SUM('Backorder Snapshots'[Outgoing Amount]),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] = [Report Date] - 1",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Time-Based"
          },
          {
            "name": "Backorder Snapshot Amount PD Var",
            "description": "Difference between the snapshots of open sales order line item amount on current and previous day",
            "expression": " [Sales Order Open Amount] - [Backorder Snapshot Amount PD]",
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Time-Based"
          },
          {
            "name": "Goods Receipt Linear Footage",
            "description": "Total footage of postings with movement types 101, 102, 531, and 532. The footage is based on the conversion factor in the alternative unit of measure section in the material master in SAP.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Goods Movement'[Linear Feet] ),",
              "    KEEPFILTERS('Goods Movement'[Movement Type] IN {\"101\", \"531\", \"102\", \"532\"})",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Goods Movement"
          },
          {
            "name": "Goods Receipt Square Footage",
            "description": "Total square footage of postings with movement types 101, 102, 531, and 532. The dimensions for square footage are based on the width in the material master and the length conversion factor in the alternative unit of measure in the material master in SAP.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Goods Movement'[Square Feet] ),",
              "    KEEPFILTERS('Goods Movement'[Movement Type] IN {\"101\", \"531\", \"102\", \"532\"})",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Goods Movement"
          },
          {
            "name": "Goods Issue Linear Footage",
            "description": "Total footage of postings with movement types 261, 262, 551, and 552.",
            "expression": [
              "",
              "CALCULATE(",
              "    ABS( SUM( 'Goods Movement'[Linear Feet] ) ),",
              "    KEEPFILTERS('Goods Movement'[Movement Type] IN {\"261\", \"551\", \"262\", \"552\"}),",
              "    KEEPFILTERS('Goods Movement'[Storage Bin] IN {\"600\", \"700\", \"733\", \"734\", \"735\", \"736\", \"88\", \"182\", \"782\"}),",
              "    KEEPFILTERS('Goods Movement'[Reason Code for Movement] <> \"0000\")",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Goods Movement"
          },
          {
            "name": "Goods Issue Scrap Rate",
            "description": "Goods issue linear footage divided by the goods receipt linear footage of transactions in 'Goods Movement' table",
            "expression": " DIVIDE( [Goods Issue Linear Footage], [Goods Receipt Linear Footage] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Goods Movement"
          },
          {
            "name": "Goods Movement Linear Footage",
            "description": "Total linear footage of transactions in the 'Goods Movement' table. The footage is based on the conversion factor in the alternative unit of measure section in the material master in SAP.",
            "expression": " SUM ('Goods Movement'[Linear Feet])",
            "formatString": "#,0",
            "displayFolder": "Goods Movement"
          },
          {
            "name": "Goods Movement Square Footage",
            "description": "Total square footage of transactions in the 'Goods Movement' table. The dimensions for square footage are based on the width in the material master and the length conversion factor in the alternative unit of measure in the material master in SAP.",
            "expression": " SUM ('Goods Movement'[Square Feet])",
            "formatString": "#,0",
            "displayFolder": "Goods Movement"
          },
          {
            "name": "Goods Movement Quantity",
            "description": "Total quantity of transactions in the 'Goods Movement' table",
            "expression": " SUM( 'Goods Movement'[Quantity])",
            "formatString": "#,0.00",
            "displayFolder": "Goods Movement"
          },
          {
            "name": "Goods Movement Amount",
            "description": "Total amount of transactions in the 'Goods Movement' table",
            "expression": " SUM( 'Goods Movement'[Amount] )",
            "formatString": "#,0",
            "displayFolder": "Goods Movement"
          },
          {
            "name": "Sales Order Allocated Amount",
            "description": "Total amount of items allocated in the sales order (or, items with delivery notes without a set actual PGI date)",
            "expression": [
              " ",
              "CALCULATE(",
              "    IF(",
              "        HASONEVALUE( '_Currency to Use'[Currency] ),",
              "        SWITCH(",
              "            VALUES( '_Currency to Use'[Currency] ),",
              "            \"USD\", SUM( 'Sales Order'[AllocatedAmountUSD] ),",
              "            \"Local\", SUM( 'Sales Order'[AllocatedAmount] ),",
              "            SUM( 'Sales Order'[AllocatedAmountUSD] )",
              "        ),",
              "        SUM( 'Sales Order'[AllocatedAmountUSD] )   ",
              "    ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Sales Order Shipped Amount",
            "description": [
              "Total amount of items shipped (or, delivery notes with actual PGI date set) in the sales order line item.",
              "",
              "This value is toggleable to local currency using the '_Currency to Use' table."
            ],
            "expression": [
              "CALCULATE(",
              "    IF(",
              "        HASONEVALUE( '_Currency to Use'[Currency] ),",
              "        SWITCH(",
              "            VALUES( '_Currency to Use'[Currency] ),",
              "            \"USD\", SUM( 'Sales Order'[ShippedAmountUSD] ),",
              "            \"Local\", SUM( 'Sales Order'[ShippedAmount] ),",
              "            SUM( 'Sales Order'[ShippedAmountUSD] )",
              "        ),",
              "        SUM( 'Sales Order'[ShippedAmountUSD] )   ",
              "    ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "CONV Square Feet per Man Hour Target",
            "description": "These values are provided by Converting at the beginning of the fiscal year. If none provided, the default value used is 5,000.",
            "expression": [
              " ",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"Sqftmh\",",
              "        TREATAS(",
              "            VALUES( 'Company Code'[Company Code] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )",
              "RETURN",
              "    IF(",
              "        ISBLANK( TargetVal ),",
              "        5000,",
              "        TargetVal",
              "    )",
              " "
            ],
            "formatString": "#,0",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Linear Footage PD",
            "description": [
              "Linear footage of goods receipt transactions posted by Converting yesterday.",
              "",
              "If this is used without the date field, this measure requires a 'Time Intelligence[Time Calculation]' filter to be active."
            ],
            "expression": [
              "",
              "CALCULATE( ",
              "    [CONV Linear Footage],",
              "    PREVIOUSDAY('Calendar'[Date])",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Converting\\_Time-Based"
          },
          {
            "name": "CONV Scrap Footage PD",
            "description": [
              "Linear footage of goods issue and scrap transactions posted by Converting yesterday.",
              "",
              "If this is used without the date field, this measure requires a 'Time Intelligence[Time Calculation]' filter to be active.",
              ""
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    [CONV Scrap Footage],",
              "    PREVIOUSDAY( 'Calendar'[Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Converting\\_Time-Based"
          },
          {
            "name": "CONV Scrap Rate PD",
            "description": [
              "Linear footage of goods issue and scrap transactions divided by the linear footage of goods receipt transactions from Converting machines posted in Converting storage bins yesterday.",
              "",
              "If this is used without the date field, this measure requires a 'Time Intelligence[Time Calculation]' filter to be active."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    [CONV Scrap Rate],",
              "    PREVIOUSDAY( 'Calendar'[Date] )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Converting\\_Time-Based"
          },
          {
            "name": "S-Block Quantity",
            "description": "Total quantity of inventory on S block",
            "expression": " SUM( 'Blocked Inventory'[SBlock Quantity] )",
            "formatString": "#,0.00",
            "displayFolder": "Inventory\\_Blocked"
          },
          {
            "name": "Q-Block Quantity",
            "description": "Total quantity of inventory on Q block",
            "expression": " SUM( 'Blocked Inventory'[QBlock Quantity] )",
            "formatString": "#,0.00",
            "displayFolder": "Inventory\\_Blocked"
          },
          {
            "name": "Expired Quantity",
            "description": "Total quantity of inventory with expiration/SLED date on or before yesterday.",
            "expression": [
              "",
              "VAR Qty =",
              "    CALCULATE(",
              "        SUM( 'Expired Inventory'[Stock Quantity] ),",
              "        FILTER(",
              "            ALL( 'Expired Inventory'[Expiration Date] ),",
              "            'Expired Inventory'[Expiration Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "VAR Amt =",
              "    CALCULATE(",
              "        SUM( 'Expired Inventory'[Stock Amount] ),",
              "        FILTER(",
              "            ALL( 'Expired Inventory'[Expiration Date] ),",
              "            'Expired Inventory'[Expiration Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "    IF(",
              "        HASONEVALUE( '_Value to Use'[Value] ),",
              "        SWITCH(",
              "            VALUES( '_Value to Use'[Value] ),",
              "            \"Quantity\", Qty,",
              "            \"Amount\", Amt,",
              "            Qty",
              "        ),",
              "        Qty",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory\\_Blocked\\_Expired"
          },
          {
            "name": "Sales Order Amount %Grand MTD",
            "description": "Total sales order amount divided by the total sales order amount of all customers and materials",
            "expression": [
              "",
              "DIVIDE(",
              "    [Sales Order Amount MTD],",
              "    CALCULATE(",
              "        [Sales Order Amount MTD],",
              "        ALL( 'Customer Master' ),",
              "        ALL( 'Material Master' )",
              "    )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Order\\_Time-Based"
          },
          {
            "name": "Gross Sales Budget MTD Var%",
            "expression": [
              "",
              "VAR VarVal = IF( OR( ISBLANK([Gross Sales MTD]), ISBLANK([Gross Sales Budget MTD]) ), BLANK(), [Gross Sales MTD] - [Gross Sales Budget MTD] )",
              "",
              "RETURN",
              "    DIVIDE(",
              "        VarVal,",
              "        [Gross Sales Budget MTD]",
              "    )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Gross Sales Budget FYTD Var%",
            "expression": [
              "",
              "VAR VarVal = IF( OR( ISBLANK([Gross Sales FYTD]), ISBLANK([Gross Sales Budget FYTD]) ), BLANK(), [Gross Sales FYTD] - [Gross Sales Budget FYTD] )",
              "",
              "RETURN",
              "    DIVIDE(",
              "        VarVal,",
              "        [Gross Sales Budget FYTD]",
              "    )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Gross Sales YTD",
            "description": "Date grouping: 'Sales Invoice Documents'[Posting Date]",
            "expression": " CALCULATE( [Gross Sales], DATESYTD( 'Calendar'[Date] ) )",
            "formatString": "#,0",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Gross Margin YTD",
            "description": "Calendar year-to-date total of gross margin",
            "expression": " CALCULATE( [Gross Margin], DATESYTD( 'Calendar'[Date] ) )",
            "formatString": "#,0",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Gross Margin% YTD",
            "description": "Calendar year-to-date total of gross margin divided by calendar year-to-date total of gross sales",
            "expression": " CALCULATE( [Gross Margin%], DATESYTD( 'Calendar'[Date] ) )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Gross Sales YTD PY",
            "description": "Date grouping: 'Sales Invoice Documents'[Posting Date]",
            "expression": " CALCULATE( [Gross Sales YTD], SAMEPERIODLASTYEAR( 'Calendar'[Date] ) )",
            "formatString": "#,0",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Gross Margin YTD PY",
            "description": "Calendar year-to-date total of gross margin of previous calendar year up to the same current period to the day",
            "expression": " CALCULATE( [Gross Margin YTD], SAMEPERIODLASTYEAR( 'Calendar'[Date] ) )",
            "formatString": "#,0",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Gross Margin% YTD PY",
            "description": "Calendar year-to-date total of gross margin divided by calendar year-to-date total of gross sales of previous calendar year up to the same current period to the day",
            "expression": " CALCULATE( [Gross Margin% YTD], SAMEPERIODLASTYEAR( 'Calendar'[Date] ) )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Gross Sales YTD PY Var%",
            "description": "Date grouping: 'Sales Invoice Documents'[Posting Date]",
            "expression": " DIVIDE( [Gross Sales YTD PY Var], [Gross Sales YTD PY] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Gross Sales YTD PY Var",
            "description": "Date grouping: 'Sales Invoice Documents'[Posting Date]",
            "expression": " IF( NOT( OR(ISBLANK([Gross Sales YTD]), ISBLANK([Gross Sales YTD PY]) ) ), [Gross Sales YTD] - [Gross Sales YTD PY], BLANK() )",
            "formatString": "#,0",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Unrestricted Stock Quantity",
            "description": "Total quantity of materials on physical inventory that has no block category",
            "expression": [
              "",
              "VAR Qty = SUM('Inventory'[Unrestricted Stock Quantity])",
              "VAR Amt = SUM('Inventory'[Unrestricted Stock Amount (USD)])",
              "RETURN",
              "    IF(",
              "        HASONEVALUE( '_Value to Use'[Value] ),",
              "        SWITCH(",
              "            VALUES( '_Value to Use'[Value] ),",
              "            \"Quantity\", Qty,",
              "            \"Amount\", Amt,",
              "            Qty",
              "        ),",
              "        Qty",
              "    )",
              ""
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory"
          },
          {
            "name": "Safety Stock Quantity",
            "description": "Average safety stock quantity by material and plant. If this measure is grouped by material only and not plant, this value is the average safety stock across all the plants.",
            "expression": [
              "",
              "AVERAGEX(",
              "    VALUES( 'Inventory'[MaterialPlantKey] ),",
              "    AVERAGE('Inventory'[SafetyStockQty])",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory"
          },
          {
            "name": "Available Inventory Quantity after Allocations and Open Sales Orders",
            "description": "Difference between unrestricted stock and allocated and open quantities in sales orders",
            "expression": " [Unrestricted Stock Quantity] - [Sales Order Allocated Quantity] - [Sales Order Open Internal Quantity]",
            "formatString": "#,0.00",
            "displayFolder": "Inventory"
          },
          {
            "name": "Purchase Order Goods Receipt Quantity",
            "description": "Total quantity of materials received for the purchase order line item",
            "expression": " SUM('Purchase Order'[Goods Receipt Quantity])",
            "formatString": "#,0.00",
            "displayFolder": "Purchase Order"
          },
          {
            "name": "Cumulative Expired Amount",
            "description": [
              "Cumulative stock amount by expiration date. For example, if 'Calendar' table is filtered to Jan 2023, this value is the expired amount of materials with expiration/SLED date on or before Jan 31, 2023/",
              "",
              "This measure is best used with fields in the 'Calendar' table."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Expired Inventory'[Stock Amount] ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Expired Inventory'[Expiration Date] ),",
              "    FILTER(",
              "        ALL( 'Calendar'[Date] ),",
              "        'Calendar'[Date] <= MAX( 'Calendar'[Date] )",
              "            && 'Calendar'[Date] <> BLANK()",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Blocked\\_Expired"
          },
          {
            "name": "Earned Labor Budget MTD",
            "description": "Monthly earned labor budget entered in the ZBUDGET table in SAP.",
            "expression": [
              "",
              "CALCULATE(",
              "    [Earned Labor Budget],",
              "    DATESMTD( 'Calendar'[Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Earned Overhead Budget MTD",
            "description": "Monthly earned overhead budget entered in the ZBUDGET table in SAP.",
            "expression": [
              "",
              "CALCULATE(",
              "    [Earned Overhead Budget],",
              "    DATESMTD( 'Calendar'[Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Absorption Budget MTD",
            "description": "Monthly earned labor and overhead budget entered in the ZBUDGET table in SAP.",
            "expression": " [Earned Labor Budget MTD] + [Earned Overhead Budget MTD]",
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Earned Labor Budget FYTD",
            "description": "Fiscal year-to-date total of the monthly earned labor budget entered in ZBUDGET table in SAP",
            "expression": [
              "",
              "CALCULATE(",
              "    [Earned Labor Budget],",
              "    DATESYTD( 'Calendar'[Date], \"09/30\" )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Earned Overhead Budget FYTD",
            "description": "Fiscal year-to-date total of the monthly earned overhead budget entered in ZBUDGET table in SAP",
            "expression": [
              "",
              "CALCULATE(",
              "    [Earned Overhead Budget],",
              "    DATESYTD( 'Calendar'[Date], \"09/30\" )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Absorption Budget FYTD",
            "description": "Fiscal year-to-date",
            "expression": " [Earned Labor Budget FYTD] + [Earned Overhead Budget FYTD]",
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Earned Labor MTD",
            "description": "Month-to-date total cost posted in the earned labor and setup cost elements",
            "expression": [
              " CALCULATE( [Earned Labor], DATESMTD( 'Calendar'[Date] ) )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Earned Overhead MTD",
            "description": "Month-to-date total cost posted in the earned overhead cost elements",
            "expression": [
              " CALCULATE( [Earned Overhead], DATESMTD( 'Calendar'[Date] ) )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Inventory Amount",
            "description": "Total standard cost of materials on inventory",
            "expression": [
              "",
              "-- valuated inventory",
              "SUM( 'Inventory'[Total Stock Amount (USD)] ) +",
              "-- open stock transfer purchase orders",
              "CALCULATE(",
              "    [Purchase Order In Transit Amount],",
              "    KEEPFILTERS( 'Purchase Order'[Receiving Plant] <> \"1001\" )",
              ") +",
              "-- open WIP process orders",
              "[Open WIP Amount]"
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory"
          },
          {
            "name": "Purchase Order In Transit Amount",
            "description": "Total amount of materials that shipped but not received yet in stock transfer purchase orders",
            "expression": [
              " SUM( 'Inventory in Transit'[In Transit Amount] )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Purchase Order"
          },
          {
            "name": "Open WIP Amount",
            "description": "Total standard cost of materials in process orders that are not TECO'd, closed, nor deleted yet",
            "expression": " SUM( 'Inventory Open WIP'[Net Cost Amount] )",
            "formatString": "#,0",
            "displayFolder": "Inventory"
          },
          {
            "name": "Inventory Snapshot Amount",
            "description": "Latest snapshot of the inventory amount depending on the date filter/context. If there is no date filter/context, then this value is yesterday's snapshot.",
            "expression": [
              "",
              "VAR RefDate =",
              "    CALCULATE(",
              "        MAX('Calendar'[Date]),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "    CALCULATE(",
              "        SUM( 'Inventory Snapshots'[Amount] ),",
              "        KEEPFILTERS( 'Calendar'[Date] = RefDate )",
              "    )"
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Time-Based"
          },
          {
            "name": "Inventory Amount Closing Prior Day",
            "description": "Inventory snapshot amount yesterday",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Inventory Snapshots'[Amount] ),",
              "    FILTER( ",
              "        'Calendar',",
              "        'Calendar'[Date] = [Prior Report Date]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Time-Based"
          },
          {
            "name": "Inventory Amount Closing Prior Day Var",
            "description": "Difference between the inventory snapshot amount on current day and yesterday",
            "expression": [
              "",
              "IF(",
              "    OR( ISBLANK( [Inventory Snapshot Amount] ), ISBLANK( [Inventory Amount Closing Prior Day] ) ),",
              "    BLANK(),",
              "    [Inventory Snapshot Amount] - [Inventory Amount Closing Prior Day]",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Time-Based"
          },
          {
            "name": "Process Order Count",
            "description": "Count of process orders by actual/scheduled finished date",
            "expression": [
              "",
              "CALCULATE(",
              "    DISTINCTCOUNT( 'Process Order'[Process Order Number] ),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Report Date]",
              "    ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Process Order'[EndDateKey] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Process Order"
          },
          {
            "name": "Process Order Ordered Quantity",
            "description": "Total ordered quantity in the process order by actual/scheduled finish date",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Process Order'[OrderQuantity] ),",
              "    FILTER(",

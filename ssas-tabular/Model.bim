              "IF( ",
              "    AND( ",
              "        NOT(ISBLANK([MTD Sales Order Amount])), ",
              "        NOT(ISBLANK([MTD PY Sales Order Amount])) ",
              "    ), ",
              "    [MTD Sales Order Amount] - [MTD PY Sales Order Amount], ",
              "    BLANK() ",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "MTD PY Var% Sales Order Amount",
            "expression": " DIVIDE( [MTD PY Var Sales Order Amount], [MTD Sales Order Amount] )",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "FYTD Sales Order Amount",
            "expression": " CALCULATE( [Sales Order Amount], DATESYTD( 'Calendar'[Date], \"09/30\" ) )",
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "FYTD PY Sales Order Amount",
            "expression": " CALCULATE( [FYTD Sales Order Amount], SAMEPERIODLASTYEAR( 'Calendar'[Date] ) )",
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "FYTD PY Var Sales Order Amount",
            "expression": [
              " ",
              "IF( ",
              "    AND( ",
              "        NOT(ISBLANK([FYTD Sales Order Amount])), ",
              "        NOT(ISBLANK([FYTD PY Sales Order Amount])) ",
              "    ), ",
              "    [FYTD Sales Order Amount] - [FYTD PY Sales Order Amount], ",
              "    BLANK() ",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "FYTD PY Var% Sales Order Amount",
            "expression": " DIVIDE( [FYTD PY Var Sales Order Amount], [FYTD Sales Order Amount] )",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "SO Internal Open Quantity",
            "expression": [
              " ",
              "CALCULATE(",
              "    SUM( 'Sales Order Documents'[Internal Open Quantity] ),",
              "    USERELATIONSHIP( 'Sales Order Documents'[First Delivery Date], 'Calendar'[Date] )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "SO Customer Open Quantity",
            "expression": [
              " ",
              "CALCULATE(",
              "    SUM( 'Sales Order Documents'[Customer Open Quantity] ) + 0,",
              "    USERELATIONSHIP( 'Sales Order Documents'[First Delivery Date], 'Calendar'[Date] )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "SO Internal Open Amount",
            "expression": [
              " ",
              "CALCULATE(",
              "    SUM( 'Sales Order Documents'[Internal Open Amount] ),",
              "    USERELATIONSHIP( 'Sales Order Documents'[First Delivery Date], 'Calendar'[Date] )",
              ")"
            ],
            "formatString": "\\$#,0;(\\$#,0);\\$#,0",
            "displayFolder": "Sales Orders",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"Currency\"><Currency LCID=\"1033\" DisplayName=\"$ English (United States)\" Symbol=\"$\" PositivePattern=\"0\" NegativePattern=\"0\" /></Format>"
              }
            ]
          },
          {
            "name": "SO Customer Open Amount",
            "expression": [
              " ",
              "CALCULATE(",
              "    SUM( 'Sales Order Documents'[Customer Open Amount] ) + 0,",
              "    USERELATIONSHIP( 'Sales Order Documents'[First Delivery Date], 'Calendar'[Date] )",
              ")"
            ],
            "formatString": "\\$#,0;(\\$#,0);\\$#,0",
            "displayFolder": "Sales Orders",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"Currency\"><Currency LCID=\"1033\" DisplayName=\"$ English (United States)\" Symbol=\"$\" PositivePattern=\"0\" NegativePattern=\"0\" /></Format>"
              }
            ]
          },
          {
            "name": "SO Ordered Quantity",
            "expression": "SUM( 'Sales Order Documents'[Order Quantity] )",
            "formatString": "#,0.00",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "SO Allocated Quantity",
            "expression": " SUM( 'Sales Order Documents'[Allocated Quantity] )",
            "formatString": "#,0.00",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "SO Shipped Quantity",
            "expression": " SUM( 'Sales Order Documents'[Shipped Quantity] )",
            "formatString": "#,0.00",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "In Stock Measure",
            "description": "Applies to open orders only; Indicates if an item is available in stock (FIFO by first scheduled ship date)",
            "expression": " IF(COUNTROWS( FILTER( 'Sales Order Documents', 'Sales Order Documents'[In Stock] = FALSE ) ) > 0, 0, 1)",
            "displayFolder": "Sales Orders"
          }
        ]

            "dataType": "int64",
            "isDataTypeInferred": true,
            "isHidden": true,
            "isAvailableInMdx": false
          },
          {
            "type": "calculated",
            "name": "Marketing",
            "dataType": "int64",
            "isDataTypeInferred": true,
            "isHidden": true,
            "isAvailableInMdx": false
          }
        ],
        "partitions": [
          {
            "name": "CalculatedTable 1",
            "source": {
              "type": "calculated",
              "expression": "{1}"
            }
          }
        ],
        "measures": [
          {
            "name": "Gross Sales",
            "description": "Total revenue amount of billing documents posted in SAP. This value is toggleable to local currency using the \"_Currency To Use\" table.",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice'[GrossSalesAmount] ),",
              "        \"Local\", SUMX( 'Sales Invoice', 'Sales Invoice'[GrossSalesAmount] * 'Sales Invoice'[Exchange Rate] ),",
              "        SUM('Sales Invoice'[GrossSalesAmount] )",
              "    ),",
              "    SUM('Sales Invoice'[GrossSalesAmount] )    ",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Net Sales",
            "description": "Difference between gross sales and discounts applied to gross sales",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice'[NetSales] ),",
              "        \"Local\", SUMX( 'Sales Invoice',  'Sales Invoice'[NetSales] * 'Sales Invoice'[Exchange Rate] ),",
              "        SUM('Sales Invoice'[NetSales] )",
              "    ),",
              "    SUM('Sales Invoice'[NetSales] )    ",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Gross Margin",
            "description": "Difference between gross sales and standard cost in the billing document. This value is toggleable to local currency using the \"_Currency To Use\" table.",
            "expression": "SUM( 'Sales Invoice'[GrossMargin] )",
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Net Margin",
            "description": "Difference between net sales and standard cost in the billing document. This value is toggleable to local currency using the \"_Currency To Use\" table.",
            "expression": " [Net Sales] - [Frozen Standard Cost]",
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Net Margin %",
            "description": "Net margin divided by net sales. Net margin is the difference between net sales and standard cost in the billing document.",
            "expression": " DIVIDE( [Net Margin], [Net Sales] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Complaint Count",
            "description": "Distinct count of complaints entered (by complaint number, not by line)",
            "expression": [
              " DISTINCTCOUNT( 'Complaint'[Complaint Number] )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Complaints"
          },
          {
            "name": "Product Quality Index",
            "description": [
              "Total amount of product quality complaints entered divided by the gross sales. For example, in Jan 2023, this value is the total amount of complaints entered in Jan 2023 divided by the gross sales in Jan 2023.",
              "",
              "Product quality complaints are complaints entered with reason codes marked as 'Q' (quality)."
            ],
            "expression": " DIVIDE( [Product Quality Complaint Amount], [Gross Sales] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Complaints\\_Product Quality"
          },
          {
            "name": "Hold Tag Count",
            "description": "Count of hold tags by unique notification number",
            "expression": " DISTINCTCOUNT( 'Hold Tag'[Notification Number] )",
            "formatString": "#,0",
            "displayFolder": "Hold Tag"
          },
          {
            "name": "Expenses Amount",
            "description": "Total amount posted in the expenses related cost elements per cost center",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM(Expenses[Amount]) + 0,",
              "    ALL( Plant[Plant] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Expenses"
          },
          {
            "name": "Expenses Plan",
            "description": "Monthly budget values per cost center and cost element. Operations submit their expenses budget to IT for upload annually.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( TARGET_Expenses[TargetValue]),",
              "    FILTER(",
              "        ALL( TARGET_Expenses[CalendarTimeKey] ),",
              "        CONTAINS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            'Calendar'[Year_Month_Sort_Key], TARGET_Expenses[CalendarTimeKey]",
              "        )",
              "    ),",
              "    FILTER(",
              "        ALL( TARGET_Expenses[CostCenterKey] ),",
              "        CONTAINS(",
              "            VALUES( 'Cost Center'[Cost Center] ),",
              "            'Cost Center'[Cost Center], TARGET_Expenses[CostCenterKey]",
              "        )",
              "    ),",
              "    FILTER(",
              "        ALL( TARGET_Expenses[CostElementKey] ),",
              "        CONTAINS(",
              "            VALUES( 'Cost Element'[Cost Element] ),",
              "            'Cost Element'[Cost Element], TARGET_Expenses[CostElementKey]",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Expenses"
          },
          {
            "name": "Service Quality Index",
            "description": [
              "Total credit issue amount of service complaints divided by the gross sales. For example , in Jan 2023, this value is the total credit amount issued in Jan 2023 divided by the gross sales in Jan 2023.",
              "",
              "Service are complaints entered with reason codes marked as 'S' (service).",
              ""
            ],
            "expression": " DIVIDE( [Service Quality Complaint Amount], [Gross Sales] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Complaints\\_Service Quality"
          },
          {
            "name": "Customer Accommodation Index",
            "description": [
              "Total credit issue amount of customer accommodation complaints divided by the gross sales. For example , in Jan 2023, this value is the total credit amount issued in Jan 2023 divided by the gross sales in Jan 2023.",
              "",
              "Customer accommodation are complaints entered with reason codes marked as 'S1' (accommodation)."
            ],
            "expression": " DIVIDE( [Customer Accommodation Complaint Amount], [Gross Sales] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Complaints\\_Customer Accommodation"
          },
          {
            "name": "Date 12 Months Ago",
            "expression": " EOMONTH( EDATE( [Report Date], -12), -1) + 1",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "Sales Order Count",
            "description": "Count of sales order entered (by unique order number)",
            "expression": " DISTINCTCOUNT(  'Sales Order'[Order Number] )",
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Gross Sales Budget",
            "description": "Monthly gross sales plan per sales territory as provided by Commercial Leadership Team (CLT)",
            "expression": [
              " ",
              "CALCULATE(",
              "    SUM( TARGET_Sales[GrossSales] ),",
              "    TREATAS(",
              "        VALUES( 'Customer Master'[Sales Person Code] ),",
              "        TARGET_Sales[SalesPersonKey]",
              "    ),",
              "    TREATAS(",
              "        VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "        TARGET_Sales[CalendarTimeKey]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Backorder Amount",
            "description": [
              "Total sales order open amount scheduled to ship in the past and current month.",
              "",
              "Scheduled ship date is the first date in the schedule line."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    [Sales Order Open Amount],",
              "    ALL( 'Calendar'[Date] ),",
              "    FILTER(",
              "        ALL( 'Sales Order'[First Scheduled Ship Date] ),",
              "        'Sales Order'[First Scheduled Ship Date] <= [End of Month]",
              "    )",
              ")",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Sales Order Margin",
            "description": [
              "Difference between the ordered amount and standard cost in the sales order.",
              "",
              "This value is toggleable to local currency using the '_Currency To Use' table."
            ],
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Order'[MarginAmountUSD] ),",
              "        \"Local\", SUM( 'Sales Order'[MarginAmount] ),",
              "        SUM( 'Sales Order'[MarginAmountUSD] )",
              "    ),",
              "    SUM( 'Sales Order'[MarginAmountUSD] )  ",
              ") + 0"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Backorder Margin",
            "description": [
              "Total gross margin of open sales order items scheduled to ship in the past and current month.",
              "",
              "Scheduled ship date is the first date in the schedule line."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    [Sales Order Open Margin],",
              "    ALL( 'Calendar'[Date] ),",
              "    FILTER(",
              "        ALL( 'Sales Order'[First Scheduled Ship Date] ),",
              "        'Sales Order'[First Scheduled Ship Date] <= [End of Month]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Quantity Sold",
            "description": "Total of quantity in the billing documents posted in SAP.",
            "expression": " SUM( 'Sales Invoice'[Quantity] )",
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Yesterday",
            "expression": " TODAY() - 1",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "Beginning of Month",
            "expression": " EOMONTH( [Report Date], -1 ) + 1",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "End of Month",
            "expression": " EOMONTH( [Report Date], 0 )",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",

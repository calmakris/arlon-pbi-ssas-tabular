              }
            ]
          },
          {
            "name": "GRIR Quantity",
            "description": "Total quantity in the cleared and open accounting documents posted under the 0022877000 G/L account",
            "expression": " SUM( 'Goods and Invoice Receipt'[Quantity] )",
            "formatString": "#,0.00",
            "displayFolder": "GRIR"
          },
          {
            "name": "GRIR Open Amount",
            "description": [
              "Total amount of open accounting documents posted under the 0022877000 G/L account.",
              "",
              "This value is in USD by default but it is toggleable to local currency using the '_Currency to Use' table."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    IF(",
              "        HASONEVALUE( '_Currency to Use'[Currency] ),",
              "        SWITCH(",
              "            VALUES( '_Currency to Use'[Currency] ),",
              "            \"USD\", [GRIR Amount in Local Currency2],",
              "            \"Local\", [GRIR Amount in Local Currency],",
              "            [GRIR Amount in Local Currency2]",
              "        ),",
              "        [GRIR Amount in Local Currency2]",
              "    ),",
              "    FILTER(",
              "        'Goods and Invoice Receipt',",
              "        'Goods and Invoice Receipt'[Clearing Date] > MAX( 'Calendar'[Date] )",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "GRIR"
          },
          {
            "name": "GRIR Open Quantity",
            "description": [
              "Total quantity of open accounting documents posted under the 0022877000 G/L account.",
              ""
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    [GRIR Quantity],",
              "    FILTER(",
              "        'Goods and Invoice Receipt',",
              "        'Goods and Invoice Receipt'[Clearing Date] > MAX( 'Calendar'[Date] )",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "GRIR"
          },
          {
            "name": "End of Next Month",
            "expression": " EOMONTH( [Report Date], 1 )",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "Earned Labor",
            "description": "Total cost posted in the earned labor and setup cost elements",
            "expression": [
              " ",
              "CALCULATE(",
              "    SUM( Absorption[Amount] ),",
              "    KEEPFILTERS( 'Cost Element'[Sub Category] IN {\"Labor\", \"Setup\"} )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption"
          },
          {
            "name": "Earned Overhead",
            "description": "Total cost posted in the earned overhead cost elements",
            "expression": [
              " ",
              "CALCULATE(",
              "    SUM( Absorption[Amount] ),",
              "    KEEPFILTERS( 'Cost Element'[Sub Category] = \"Overhead\" )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption"
          },
          {
            "name": "Absorption",
            "description": "Total cost posted in the earned labor, setup, and overhead cost elements",
            "expression": " [Earned Labor] + [Earned Overhead]",
            "formatString": "#,0",
            "displayFolder": "Absorption"
          },
          {
            "name": "Sales Order Lines Automated",
            "description": "Number of sales order line items entered by BATCHUSER",
            "expression": " COUNTROWS( FILTER( 'Sales Order', 'Sales Order'[Entered By] = \"BATCHUSER\" ) )",
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Sales Order Line Count",
            "description": "Count of sales order line items entered",
            "expression": " COUNTROWS( 'Sales Order' )",
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Sales Order Lines Automated Entry%",
            "description": "Number of sales order line items entered by BATCHUSER divided by sales order line count entered by Customer Service and BATCHUSER",
            "expression": [
              " ",
              "DIVIDE( ",
              "    [Sales Order Lines Automated],",
              "    [Sales Order Line Count Automated and Manual]",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Sales Order Lines Manual",
            "description": "Number of sales order line items entered by Customer Service",
            "expression": [
              "",
              "CALCULATE(",
              "    [Sales Order Line Count],",
              "    KEEPFILTERS( 'DIM_UsersSAP'[Department] = \"CUSTOMER SERVICE\" )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Sales Order Automation Efficiency%",
            "description": "Count of sales order line items entered automatically but without any manual changes divided by the count of sales order line items entered automatically",
            "expression": " DIVIDE( [Sales Order Lines Automated without Changes], [Sales Order Lines Automated] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Sales Order Lines Manual Entry%",
            "description": "Number of sales order line items entered by Customer Service divided by sales order line count entered by Customer Service and BATCHUSER",
            "expression": " DIVIDE( [Sales Order Lines Manual], [Sales Order Line Count Automated and Manual] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Order"
          },
          {
            "name": "CONV Production Hours",
            "description": "The number of hours across all converting employees when they are producing material",
            "expression": " SUM( 'Converting Man Hours'[Production Hours] )",
            "formatString": "#,0.00",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Square Feet per Man Hour",
            "description": "Total number of square footage over total production hours in Converting",
            "expression": "DIVIDE( [CONV Square Footage], [CONV Production Hours] )",
            "formatString": "#,0",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Availability",
            "description": [
              "Uptime divided by the difference between runtime and non production seconds.",
              "",
              "Uptime is when Converting is running production. Runtime is the total duration of the entire day. Non-production is the total duration of break time, lunch, and other activities marked as non-production in the Converting runtime log using the downtime integer field."
            ],
            "expression": [
              " ",
              "DIVIDE(",
              "    [CONV Uptime Seconds],",
              "    [CONV Runtime Seconds] - [CONV Non Production Seconds]",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Converting\\_OEE"
          },
          {
            "name": "Upstream Actual Production Footage",
            "description": "Total footage of production records (excl FEs) in the machine logs in Casting and Coating departments",
            "expression": [
              " ",
              "CALCULATE(",
              "    [Upstream Actual Footage],",
              "    FILTER(",
              "        'Upstream Log Simplified',",
              "        'Upstream Log Simplified'[Category] = \"Production\"",
              "            && LEFT('Upstream Log Simplified'[Causes], 4) = \"Roll\"",
              "            && NOT(CONTAINSSTRING('Upstream Log Simplified'[Causes], \"FE\"))",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Ideal Production Footage",
            "description": "Sum of ideal footage of production records excluding FEs. Ideal footage is based on the duration of the run (in minutes) multplied by the specification linespeed.",
            "expression": [
              " ",
              "CALCULATE(",
              "    [Upstream Ideal Footage],",
              "    FILTER(",
              "        'Upstream Log Simplified',",
              "        'Upstream Log Simplified'[Category] = \"Production\"",
              "            && LEFT('Upstream Log Simplified'[Causes], 4) = \"Roll\"",
              "            && NOT(CONTAINSSTRING('Upstream Log Simplified'[Causes], \"FE\"))",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Uptime Minutes",
            "description": "Sum of duration between 'Start Time' and 'Stop Time' in minutes of production records excluding FEs",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Upstream Log Simplified'[DurationMinute] ),",
              "    FILTER(",
              "        'Upstream Log Simplified',",
              "        'Upstream Log Simplified'[Category] = \"Production\"",
              "            && NOT(CONTAINSSTRING('Upstream Log Simplified'[Causes], \"FE\"))",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Runtime Minutes",
            "description": "Sum of duration between 'Start Time' and 'Stop Time' in minutes of all records except end of production, EOS, and FEs",
            "expression": [
              "",
              "VAR CAST = ",
              "    CALCULATE(",
              "        SUM( 'Upstream Log Simplified'[DurationMinute] ),",
              "        FILTER(",
              "            'Upstream Log Simplified',",
              "            'Upstream Log Simplified'[Category] <> \"End of Production\"",
              "                && 'Upstream Log Simplified'[Further Detail] <> \"EOS\"",
              "                && NOT(CONTAINSSTRING('Upstream Log Simplified'[Causes], \"FE\"))",
              "        )",
              "    )",
              "",
              "VAR COAT = ",
              "    CALCULATE(",
              "        SUM( 'Upstream Log Simplified'[DurationMinute] ),",
              "        FILTER(",
              "            'Upstream Log Simplified',",
              "            'Upstream Log Simplified'[Category] <> \"End of Production\"",
              "                && 'Upstream Log Simplified'[Further Detail] <> \"EOS\"",
              "                && 'Upstream Log Simplified'[Non Uptime Reason] <> \"PLANNED DT\"",
              "                && NOT(CONTAINSSTRING('Upstream Log Simplified'[Causes], \"FE\"))",
              "        )",
              "    )",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(Machine[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(Machine[Department]), COAT,",
              "    CAST",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Actual Footage",
            "description": "Total footage recorded in the machine logs in Casting and Coating departments",
            "expression": " SUM('Upstream Log Simplified'[Footage])",
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Ideal Footage",
            "description": "Sum of ideal footage. Ideal footage is derived from the specification linespeed and duration between 'Start Time' and 'Stop Time' in minutes",
            "expression": "SUM('Upstream Log Simplified'[IdealFootage])",
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Downtime Hours",
            "description": "Sum of duration between 'Start Time' and 'Stop Time' in minutes of unplanned clean up and unplanned downtime records",
            "expression": [
              "",
              "CALCULATE(",
              "    DIVIDE( SUM( 'Upstream Log Simplified'[DurationMinute] ), 60 ) + 0,",
              "    FILTER(",
              "        'Upstream Log Simplified',",
              "        'Upstream Log Simplified'[Category] IN { \"Clean Up\", \"Downtime\" }",
              "            && LEFT('Upstream Log Simplified'[Causes], 9) = \"Unplanned\"",
              "    )",
              ")"
            ],
            "formatString": "#,0.00 \"hr\";-#,0.00 \"hr\";0 \"hr\"",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "CAST Clean Up Count",
            "expression": "COUNTROWS(FILTER('Upstream Log Simplified', 'Upstream Log Simplified'[Category] = \"Clean Up\" && 'Upstream Log Simplified'[Causes] IN { \"Planned\", \"Unplanned\", \"Unplanned - Casting\" })) + 0",
            "formatString": "#,0",
            "isHidden": true,
            "displayFolder": "Upstream Log"
          },
          {
            "name": "COAT Clean Up Count",
            "expression": "COUNTROWS(FILTER('Upstream Log', 'Upstream Log'[Category] = \"Clean Up\" && 'Upstream Log'[Causes] IN { \"Planned\", \"Unplanned\" })) + 0",
            "formatString": "#,0",
            "isHidden": true,
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Planned CU Count",
            "description": "Number of planned clean up records. For casting, the clean ups from 'Upstream Logs - Simplified' table is counted where consecutively similar clean ups are combined into one clean up. For coating, it is the raw count of clean ups in 'Upstream Logs - Raw'.",
            "expression": [
              "",
              "VAR CAST = CALCULATE( [CAST Clean Up Count], 'Upstream Log Simplified'[Causes] = \"Planned\" )",
              "VAR COAT = CALCULATE( [COAT Clean Up Count], 'Upstream Log'[Causes] = \"Planned\" )",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(Machine[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(Machine[Department]), COAT,",
              "    CAST + COAT",
              ")",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Unplanned CU Count",
            "description": "Number of unplanned clean up records. For casting, the clean ups from 'Upstream Logs - Simplified' table is counted where consecutively similar clean ups are combined into one clean up. For coating, it is the raw count of clean ups in 'Upstream Logs - Raw'",
            "expression": [
              "",
              "VAR CAST = CALCULATE( [CAST Clean Up Count], 'Upstream Log Simplified'[Causes] = \"Unplanned - Casting\" )",
              "VAR COAT = CALCULATE( [COAT Clean Up Count], 'Upstream Log'[Causes] = \"Unplanned\" )",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(Machine[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(Machine[Department]), COAT,",
              "    CAST + COAT",
              ")",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Performance",
            "description": "Upstream actual production footage divided by ideal production footage",
            "expression": [
              " ",
              "CALCULATE(",
              "    DIVIDE( [Upstream Actual Production Footage], [Upstream Ideal Production Footage] ),",
              "    ALLSELECTED(",
              "        'Upstream Log Simplified'[Category],",
              "        'Upstream Log Simplified'[Causes],",
              "        'Upstream Log Simplified'[Further Detail],",
              "        'Upstream Log Simplified'[Comments],",
              "        'Upstream Log Simplified'[Non Uptime Reason],",
              "        'Upstream Log Simplified'[Specification Number],",
              "        'Upstream Log Simplified'[Efficiency Hitter]",
              "    )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Availability",
            "description": "Upstream uptime duration minutes divided by runtime duration minutes",
            "expression": [
              " ",
              "CALCULATE(",
              "    DIVIDE( [Upstream Uptime Minutes], [Upstream Runtime Minutes] ),",
              "    ALLSELECTED(",
              "        'Upstream Log Simplified'[Category],",
              "        'Upstream Log Simplified'[Causes],",
              "        'Upstream Log Simplified'[Further Detail],",
              "        'Upstream Log Simplified'[Comments],",
              "        'Upstream Log Simplified'[Non Uptime Reason],",
              "        'Upstream Log Simplified'[Specification Number],",
              "        'Upstream Log Simplified'[Efficiency Hitter]",
              "    )",
              ")",
              ""
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Overall Equipment Efficiency",
            "description": "Product of upstream availability, performance, and quality",
            "expression": "[Upstream Availability] * [Upstream Performance] * [Upstream Quality]",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream First Pass Reliability",
            "description": "Number of planned clean ups divided by the total planned and unplanned clean ups",
            "expression": " DIVIDE( [Upstream Planned CU Count], [Upstream Planned CU Count] + [Upstream Unplanned CU Count] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Changeover Time",
            "description": "Average duration of clean up records in 'Upstream Logs - Simplified'",
            "expression": [
              "",
              "CALCULATE(",
              "    AVERAGE( 'Upstream Log Simplified'[DurationMinute] ),",
              "    FILTER(",
              "        'Upstream Log Simplified',",
              "        'Upstream Log Simplified'[Category] = \"Clean Up\"",
              "    )",
              ")"
            ],
            "formatString": "#,0 \"min\"",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "CAST Overall Equipment Efficiency (Target)",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"OEE_CAST\",",
              "        TREATAS(",
              "            VALUES( 'Company Code'[Company Code] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.85,",
              "        TargetVal",
              "    )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "isHidden": true,
            "displayFolder": "Upstream Log"
          },
          {
            "name": "COAT Overall Equipment Efficiency (Target)",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"OEE_COAT\",",
              "        TREATAS(",
              "            VALUES( 'Company Code'[Company Code] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.85,",
              "        TargetVal",
              "    )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "isHidden": true,
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Overall Equipment Efficiency Target",
            "description": "Monthly target data supplied by Engineering once every fiscal year",
            "expression": [
              "",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED('Machine'[Department]), [CAST Overall Equipment Efficiency (Target)],",
              "    \"COAT\" IN ALLSELECTED(Machine[Department]), [COAT Overall Equipment Efficiency (Target)],",
              "    DIVIDE( ([CAST Overall Equipment Efficiency (Target)] + [COAT Overall Equipment Efficiency (Target)]), 2 )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "CAST First Pass Reliability (Target)",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"FPass_CAST\",",
              "        TREATAS(",
              "            VALUES( 'Company Code'[Company Code] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.95,",
              "        TargetVal",
              "    )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "isHidden": true,
            "displayFolder": "Upstream Log"
          },
          {
            "name": "COAT First Pass Reliability (Target)",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"FPass_COAT\",",
              "        TREATAS(",
              "            VALUES( 'Company Code'[Company Code] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.95,",
              "        TargetVal",
              "    )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "isHidden": true,
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream First Pass Reliability Target",
            "description": "Monthly target data supplied by Engineering once every fiscal year",
            "expression": [
              "",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED('Machine'[Department]), [CAST First Pass Reliability (Target)],",
              "    \"COAT\" IN ALLSELECTED(Machine[Department]), [COAT First Pass Reliability (Target)],",
              "    DIVIDE( ([CAST First Pass Reliability (Target)] + [COAT First Pass Reliability (Target)]), 2 )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Changeover Time Target",
            "description": "Average duration of clean up records YTD last fiscal year in 'Upstream Logs - Simplified' table",
            "expression": [
              "",
              "CALCULATE(",
              "    [Upstream Changeover Time],",
              "    SAMEPERIODLASTYEAR(DATESYTD( 'Calendar'[Date], \"09/30\"))",
              ")"
            ],
            "formatString": "#,0 \"min\"",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Performance Impact",
            "description": "Impact on the overall performance of the department. Best used if analyzed by Further Detail, Comments, Non Uptime Reason, or Efficiency Hitter in 'Upstream Logs - Simplified'.",
            "expression": [
              "",
              "DIVIDE(",
              "    [Upstream Ideal Production Footage] - [Upstream Actual Production Footage],",
              "    CALCULATE(",
              "        [Upstream Ideal Production Footage],",
              "        ALL( Machine[Machine] ),",
              "        ALL( 'Production Shift' ),",
              "        ALL(",
              "            'Upstream Log Simplified'[Machine],",
              "            'Upstream Log Simplified'[Production Shift],",
              "            'Upstream Log Simplified'[Non Uptime Reason],",
              "            'Upstream Log Simplified'[Efficiency Hitter],",
              "            'Upstream Log Simplified'[Category],",
              "            'Upstream Log Simplified'[Causes],",
              "            'Upstream Log Simplified'[Further Detail],",
              "            'Upstream Log Simplified'[Comments]",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Availability Impact",
            "description": "Impact on the overall availablility of the department. Best used if analyzed by Further Detail, Comments, Non Uptime Reason, or Efficiency Hitter in 'Upstream Logs - Simplified'.",
            "expression": [
              "",
              "DIVIDE(",
              "    [Upstream Runtime Minutes] - [Upstream Uptime Minutes],",
              "    CALCULATE(",
              "        [Upstream Runtime Minutes],",
              "        ALL( Machine[Machine] ),",
              "        ALL( 'Production Shift' ),",
              "        ALL(",
              "            'Upstream Log Simplified'[Machine],",
              "            'Upstream Log Simplified'[Production Shift],",
              "            'Upstream Log Simplified'[Non Uptime Reason],",
              "            'Upstream Log Simplified'[Efficiency Hitter],",
              "            'Upstream Log Simplified'[Category],",
              "            'Upstream Log Simplified'[Causes],",
              "            'Upstream Log Simplified'[Further Detail],",
              "            'Upstream Log Simplified'[Comments]",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Quality",
            "description": "A fixed number defined by Engineering",
            "expression": [
              "",
              "VAR CAST = 0.95",
              "VAR COAT = 0.98",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(Machine[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(Machine[Department]), COAT,",
              "    0.95",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Overall Equipment Efficiency Impact",
            "description": "Impact on the overall equipment efficiency of the department. Best used if analyzed by Further Detail, Comments, Non Uptime Reason, or Efficiency Hitter in 'Upstream Logs - Simplified'.",
            "expression": [
              "",
              "VAR _avail = ",
              "    CALCULATE(",
              "        [Upstream Availability],",
              "        ALL( Machine[Machine] ),",
              "        ALL( 'Production Shift' ),",
              "        ALL(",
              "            'Upstream Log Simplified'[Machine],",
              "            'Upstream Log Simplified'[Production Shift],",
              "            'Upstream Log Simplified'[Non Uptime Reason],",
              "            'Upstream Log Simplified'[Efficiency Hitter],",
              "            'Upstream Log Simplified'[Category],",
              "            'Upstream Log Simplified'[Causes],",
              "            'Upstream Log Simplified'[Further Detail],",
              "            'Upstream Log Simplified'[Comments]",
              "        )",
              "    )",
              "",
              "VAR _perf = ",
              "    CALCULATE(",
              "        [Upstream Performance],",
              "        ALL( Machine[Machine] ),",
              "        ALL( 'Production Shift' ),",
              "        ALL(",
              "            'Upstream Log Simplified'[Machine],",
              "            'Upstream Log Simplified'[Production Shift],",
              "            'Upstream Log Simplified'[Non Uptime Reason],",
              "            'Upstream Log Simplified'[Efficiency Hitter],",
              "            'Upstream Log Simplified'[Category],",
              "            'Upstream Log Simplified'[Causes],",
              "            'Upstream Log Simplified'[Further Detail],",
              "            'Upstream Log Simplified'[Comments]",
              "        )",
              "    )",
              "",
              "VAR _oee = _avail * _perf * [Upstream Quality]",
              "",
              "RETURN",
              "(( _avail + [Upstream Availability Impact] ) * ( _perf + [Upstream Performance Impact] ) * [Upstream Quality] ) - _oee"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Days Since Last Unplanned Clean Up",
            "description": "Number of days between yesterday and the last unplanned clean up before yesterday",
            "expression": [
              "",
              "VAR Last_Date = ",
              "    SWITCH( TRUE(),",
              "        \"CAST\" IN ALLSELECTED(Machine[Department]),",
              "        CALCULATE(",
              "            MAX( 'Upstream Log Simplified'[Production Date] ),",
              "            FILTER(",
              "                'Upstream Log Simplified',",
              "                'Upstream Log Simplified'[Production Date] <= [Report Date]",
              "                    && 'Upstream Log Simplified'[Category] = \"Clean Up\"",
              "                    && 'Upstream Log Simplified'[Causes] = \"Unplanned - Casting\"",
              "            )    ",
              "        ),",
              "        \"COAT\" IN ALLSELECTED(Machine[Department]),",
              "        CALCULATE(",
              "            MAX( 'Upstream Log'[Production Date] ),",
              "            FILTER(",
              "                'Upstream Log',",
              "                'Upstream Log'[Production Date] <= [Report Date]",
              "                    && 'Upstream Log'[Category] = \"Clean Up\"",
              "                    && 'Upstream Log'[Causes] = \"Unplanned\"",
              "            )    ",
              "        )",
              "    )",
              "",
              "RETURN",
              "DATEDIFF(Last_Date, [Yesterday], DAY)"
            ],
            "formatString": "#,0 \"days\"",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Changeover Scrap",
            "description": "Average footage of clean up records. In Casting, this value is the caliper scrap. In Coating, it is the combined footage of consecutive clean up and setup records.",
            "expression": [
              "",
              "CALCULATE(",
              "    AVERAGE( 'Upstream Log Simplified'[ScrapFootage] ),",
              "    FILTER(",
              "        'Upstream Log Simplified',",
              "        'Upstream Log Simplified'[Category] = \"Clean Up\"",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Report Date",
            "expression": " CALCULATE( Max( 'Calendar'[Date] ), FILTER(  ALLSELECTED('Calendar'), 'Calendar'[Date] <= [Yesterday] ) )",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "Upstream Overall Equipment Efficiency Target Var",
            "description": "Difference between OEE and the corresponding monthly target",
            "expression": "[Upstream Overall Equipment Efficiency] - [Upstream Overall Equipment Efficiency Target]",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Helper_12MoAgo",
            "expression": "YEAR([Date 12 Months Ago]) * 100 + MONTH([Date 12 Months Ago])",
            "isHidden": true,
            "displayFolder": "_Helpers"
          },
          {
            "name": "Helper_CurrMo",
            "expression": "YEAR([Beginning of Month]) * 100 + MONTH([Beginning of Month])",
            "isHidden": true,
            "displayFolder": "_Helpers"
          },
          {
            "name": "Days In Month",
            "expression": " CALCULATE( COUNTROWS( 'Calendar' ), ALL( 'Calendar' ), VALUES( 'Calendar'[Year_Month_Sort_Key] ))",
            "isHidden": true,
            "displayFolder": "_Dates"
          },
          {
            "name": "Days Count",
            "expression": [
              "",
              "CALCULATE(",
              "    COUNTROWS( 'Calendar' ), ",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Report Date]",
              "    )",
              ")"
            ],
            "isHidden": true,
            "displayFolder": "_Dates"
          },
          {
            "name": "Min Date",
            "expression": "MIN( 'Calendar'[Date] )",
            "formatString": "MM/dd/yyyy",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"ShortDate\" FormatString=\"MM/dd/yyyy\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Max Date",
            "expression": "MAX( 'Calendar'[Date] )",
            "formatString": "MM/dd/yyyy",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"ShortDate\" FormatString=\"MM/dd/yyyy\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Report Production Date",
            "expression": "[Report Date] + TIME(6, 0, 0)",
            "formatString": "MM/dd/yyyy HH:mm",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralShortDateTime\" FormatString=\"MM/dd/yyyy HH:mm\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Prior Production Date",
            "expression": " ([Report Date] - 1) + TIME(6, 0, 0)",
            "formatString": "MM/dd/yyyy HH:mm",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralShortDateTime\" FormatString=\"MM/dd/yyyy HH:mm\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Now Date",
            "expression": "NOW() + TIME(0,0,0)",
            "formatString": "MM/dd/yyyy HH:mm",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralShortDateTime\" FormatString=\"MM/dd/yyyy HH:mm\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Inventory Balance",
            "description": [
              "Total of accounting documents posted in the inventory related G/L accounts in SAP.",
              "",
              "By default, this value is in USD but it is toggleable to local currency using the '_Currency to Use' table."
            ],
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Inventory Balance Item'[Amount (Local Currency 2)] ),",
              "        \"Local\", SUM( 'Inventory Balance Item'[Amount (Local Currency)] ),",
              "        SUM( 'Inventory Balance Item'[Amount (Local Currency 2)] )",
              "    ),",
              "    SUM( 'Inventory Balance Item'[Amount (Local Currency 2)] )  ",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "End of Next 3 Months",
            "expression": " EOMONTH( [Report Date], 3 )",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "Sample Request Count",
            "description": "Total count of free-of-charge or sample sales orders and open requests in Odoo",
            "expression": [
              "",
              "CALCULATE(",
              "    DISTINCTCOUNT( 'Sales Order'[Order Number] ),",
              "    FILTER(",
              "        'Sales Order',",
              "        'Sales Order'[Sample Category] <> \"Not Applicable\"",
              "    )",
              ") +",
              "DISTINCTCOUNT( 'Odoo Sample Request'[Ticket Number] )"
            ],
            "formatString": "#,0",
            "displayFolder": "Sample Requests"
          },
          {
            "name": "Beginning of Last Month",
            "expression": "EDATE([Beginning of Month], -1)",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "Inventory Time Supply",
            "description": "[Current Unrestricted Stock Quantity] divided by [Average Monthly Consumption (Last 12 Months)]. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "DIVIDE(",
              "    [Unrestricted Stock Quantity],",
              "    [Avg Monthly Inventory Consumption Last 12 Months]",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory\\_Consumption"
          },
          {
            "name": "Inventory Consumption (Dynamic)",
            "description": "Sum of 'Inventory Consumption'[Quantity].Can be toggled between quantity and amount values (default = quantity)",
            "expression": [
              "",
              "VAR Qty = SUM( 'Inventory Consumption'[Quantity] )",
              "VAR Amt = SUM( 'Inventory Consumption'[Amount (In Current Price)] )",
              "RETURN",
              "    IF(",
              "        HASONEVALUE( '_Value to Use'[Value] ),",
              "        SWITCH(",
              "            VALUES( '_Value to Use'[Value] ),",
              "            \"Quantity\", Qty,",
              "            \"Amount\", Amt,",
              "            Qty",
              "        ),",
              "        Qty",
              "    )"
            ],
            "formatString": "#,0.00",
            "isHidden": true,
            "displayFolder": "Inventory"
          },
          {
            "name": "WH Shipment Amount",
            "description": "Total amount (in USD) of material transactions with 601, 641, 643, and Z43 movement types",
            "expression": [
              "",
              "ABS(",
              "    CALCULATE(",
              "        SUM( 'Inventory Consumption'[Amount] ),",
              "        FILTER(",
              "            'Inventory Consumption',",
              "            'Inventory Consumption'[Movement Type] IN { \"601\", \"641\", \"643\", \"Z43\" }",
              "                && 'Inventory Consumption'[Posting Date] <= [Last Working Day]",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Expenses per Units Picked",
            "description": "Total expenses amount posted in the cost centers for the Logistics department divided by amount of materials shipped",
            "expression": [
              "",
              "DIVIDE(",
              "    CALCULATE(",
              "        [Expenses Amount],",
              "        'Cost Center'[Department] = \"DIST\",",
              "        ALL( Plant[Plant] )",
              "    ),",
              "    [WH Picked Weight]",
              ")"
            ],
            "formatString": "#,0.0000",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Shipment Weight",
            "description": "Total weight of material transactions with 601, 641, 643, and Z43 movement types. Weight is given in the material master data.",
            "expression": [
              "",
              "ABS(",
              "    CALCULATE(",
              "        SUM( 'Inventory Consumption'[Gross Weight] ),",
              "        FILTER(",
              "            'Inventory Consumption',",
              "            'Inventory Consumption'[Movement Type] IN { \"601\", \"641\", \"643\", \"Z43\" }",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Warehouse"
          },
          {
            "name": "Expenses Plan Var%",
            "description": "Difference between expenses amount and plan divided by the plan",
            "expression": "DIVIDE([Expenses Amount] - [Expenses Plan], [Expenses Plan])",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Expenses"
          },
          {
            "name": "WH Storage Bin Amount",
            "description": "Total amount (in USD) of materials in storage bins",
            "expression": "SUM( 'Inventory in Storage Bin'[Amount in USD] )",
            "formatString": "#,0",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Storage Bin Capacity",
            "description": "Capacity per storage bin maintained in the storage bin master data in SAP",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM('Storage Bin'[Capacity] ),",
              "    VALUES( 'Storage Bin'[StorageKey] )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Storage Bin Utilization",
            "description": "The quants in a storage bin divided by its capacity according to the storage bin master data",
            "expression": [
              "",
              "IF(",
              "    ISFILTERED( 'Storage Bin' ),",
              "    DIVIDE(",
              "        [WH Storage Bin Quantity],",
              "        [WH Storage Bin Capacity]",
              "    ),",
              "    BLANK()",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Storage Bin Quantity",
            "description": "Total quantity in a storage bin",
            "expression": "SUM( 'Inventory in Storage Bin'[Quantity] )",
            "formatString": "#,0.00",
            "displayFolder": "Warehouse"
          },
          {
            "name": "Testing Sample Count",
            "description": "Count of samples used in testing. Usually, samples is by batch number. If there are multiple specimens for a batch number, they all count as one sample.",
            "expression": "DISTINCTCOUNT( 'Test Result'[Result Key] )",
            "formatString": "#,0",
            "displayFolder": "Testing"
          },
          {
            "name": "Testing Average Result",
            "description": "Average testing result from online tests in Manufacturing and/or aged testing in QA",
            "expression": "AVERAGE( 'Test Result'[Result Value] )",
            "formatString": "#,0.00",
            "displayFolder": "Testing"
          },
          {
            "name": "Testing Minimum Result",
            "description": "Minimum of all testing results from online testing in Manufacturing and/or aged testing in QA",
            "expression": "MIN( 'Test Result'[Result Value] )",
            "formatString": "#,0.00",
            "displayFolder": "Testing"
          },
          {
            "name": "Testing Maximum Result",
            "description": "Maximum of all testing results in online testing in Manufacturing and/or aged testing in QA",
            "expression": "MAX( 'Test Result'[Result Value] )",
            "formatString": "#,0.00",
            "displayFolder": "Testing"
          },
          {
            "name": "Testing Standard Deviation Result",
            "description": "Population standard deviation of testing results from online tests in Manufacturing and/or aged testing in QA",
            "expression": "STDEV.P( 'Test Result'[Result Value] )",
            "formatString": "#,0.00",
            "displayFolder": "Testing"
          },
          {
            "name": "Testing Passing Rate",
            "description": "Count of specimens that passed the test results divided by the count of speciments",
            "expression": [
              "",
              "DIVIDE(",
              "    [Testing Passed Specimen Count],",
              "    [Testing Passed Specimen Count] + [Testing Failed Specimen Count]",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Testing"
          },
          {
            "name": "Testing Specification Lower Limit",
            "description": "Minimum specification lower limit at the time the tests are conducted",
            "expression": "MIN( 'Test Result'[Specification Lower Limit] )",
            "formatString": "#,0.00",
            "displayFolder": "Testing"
          },
          {
            "name": "Testing Specification Upper Limit",
            "description": "Maximum specification lower limit at the time the tests are conducted",
            "expression": "MAX( 'Test Result'[Specification Upper Limit] )",
            "formatString": "#,0.00",
            "displayFolder": "Testing"
          },
          {
            "name": "Testing Passed Specimen Count",
            "description": "Count of specimens with test results within acceptable range of the specification",
            "expression": [
              "",
              "CALCULATE(",
              "    COUNTA( 'Test Result'[Pass Flag] ),",
              "    'Test Result'[Pass Flag] = \"Yes\"",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Testing"
          },
          {
            "name": "Testing Failed Specimen Count",
            "description": "Count of specimens with test results not within acceptable range of the specification",
            "expression": [
              "",
              "CALCULATE(",
              "    COUNTA( 'Test Result'[Pass Flag] ),",
              "    'Test Result'[Pass Flag] = \"No\"",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Testing"
          },
          {
            "name": "CONV Linear Footage",
            "description": "Total linear footage of goods movement transaction with movement types 101/102 and 531/532 from the Converting machines posted in Converting storage bins.",
            "expression": [
              "",
              "CALCULATE(",
              "    [Goods Receipt Linear Footage],",
              "    KEEPFILTERS('Goods Movement'[Machine] IN {\"014\", \"088\", \"182\", \"733\", \"734\", \"735\", \"736\", \"737\", \"762\", \"MANUAL\"}),",
              "    KEEPFILTERS('Goods Movement'[Storage Bin] IN {\"600\", \"700\", \"733\", \"734\", \"735\", \"736\", \"737\", \"88\", \"182\", \"782\", \"555\"}),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Goods Movement'[Production Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Converting"
          },
          {
            "name": "Absorption Hours",
            "description": "Total quantity (in hours) posted in the earned labor, setup, and overhead cost elements",
            "expression": "SUM( Absorption[Quantity] )",
            "formatString": "#,0.00",
            "displayFolder": "Absorption"
          },
          {
            "name": "WH Shipment Quantity",
            "description": "Total quantity of material transactions with 601, 641, 643, and Z43 movement types",
            "expression": [
              "",
              "ABS(",
              "    CALCULATE(",
              "        SUM( 'Inventory Consumption'[Quantity] ),",
              "        FILTER(",
              "            'Inventory Consumption',",
              "            'Inventory Consumption'[Movement Type] IN { \"601\", \"641\", \"643\", \"Z43\" }",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Future Shipment Amount",
            "description": "Total amount (in USD) of material transactions with 601, 641, 643, and Z43 movement types plus the total amount of open sales orders",
            "expression": [
              "",
              "VAR MinVisibleDate = MIN('Calendar'[Date])",
              "",
              "RETURN",
              "-- Show previous month as actual ship amount",
              "-- Current and future shows actual ship plus customer open",
              "IF(",
              "    MinVisibleDate < [Beginning of Last Month],",
              "    BLANK(),",
              "    IF(",
              "        MinVisibleDate < [Beginning of Month],",
              "        [WH Shipment Amount],",
              "        [WH Shipment Amount] + [Sales Order Open Amount]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Effective On Time Shipment",
            "description": [
              "Daily average ratio of the number of on time lines plus exempted late lines divided by the number of lines.",
              "",
              "A sales order line item is on time if the entire ordered quantity is shipped on or before the first scheduled ship date. Otherwise, the line item is late. If Logistics specified in ZSDSHIP_LATE tcode in SAP that a line is not Arlon's fault, then the line item is exempted from being late."
            ],
            "expression": [
              "",
              "VAR OnTimeLines =",
              "    CALCULATE(",
              "        [Sales Order Line Count by First Ship Date On Time] + [Sales Order Line Count by First Ship Date Late Exempt],",
              "        'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "    )",
              "",
              "VAR AllLines =",
              "    CALCULATE(",
              "        [Sales Order Line Count by First Ship Date],",
              "        'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "    )",
              "",
              "VAR OTS = DIVIDE(OnTimeLines, AllLines)",
              "",
              "RETURN -- Weighted average by line count",
              "    CALCULATE(",
              "        DIVIDE(",
              "            SUMX(",
              "                VALUES( 'Calendar'[Date] ),",
              "                [Sales Order Line Count by First Ship Date] * OTS",
              "            ),",
              "            [Sales Order Line Count by First Ship Date]",
              "        ),",
              "        'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "    )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Complete On Time",
            "description": [
              "Number of customers with 100% on time shipment in sales orders divided by the number of customers.",
              "",
              "The date grouping is by first scheduled ship date in the schedule line of the sales order. For example, if there are 10 customers with shipments scheduled in a certain day and 7 of those customers have all their orders shipped on time, then the complete on time is 70% for that day."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    DIVIDE(",
              "        SUMX(",
              "            VALUES( 'Calendar'[Date] ),",
              "            [WH Complete On Time Hidden] * [Sales Order Line Count by First Ship Date]",
              "        ),",
              "        [Sales Order Line Count by First Ship Date]",
              "    ),",
              "    'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Shipment Calendar List",
            "description": "Combination of customer, sales order number, and sales order type sorted by the ordered amount",
            "expression": [
              "",
              "CONCATENATEX( ",
              "    'Sales Order Shipment Calendar', ",
              "    LEFT([Customer Name], 15) & UNICHAR(10) & \"#\" & VALUE([Order Number]) & \" (\" & [Order Type] & \")\",",
              "    UNICHAR(10),",
              "    [Order Amount], DESC",
              ")"
            ],
            "displayFolder": "Warehouse"
          },
          {
            "name": "Last Working Day",
            "expression": [
              "",
              "CALCULATE(",
              "    MAX( 'Calendar'[Date] ),",
              "    FILTER(",
              "        ALLSELECTED( 'Calendar' ),",
              "        'Calendar'[Date] <= [Report Date]",
              "            && 'Calendar'[Is Weekday]",
              "    )",
              ")"
            ],
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "WH Days of Perfect On Time Shipment",
            "description": "Number of days when on time shipment is 100%",
            "expression": [
              "",
              "VAR CountPerfectDays =",
              "    COUNTROWS(",
              "        FILTER(",
              "            VALUES('Calendar'[Date]),",
              "            [WH Effective On Time Shipment] >= 1",
              "        )",
              "    )",
              "",
              "RETURN",
              "CALCULATE(",
              "    CountPerfectDays + 0,",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Last Working Day]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Warehouse"
          },
          {
            "name": "Cost of Goods Sold",
            "description": "Sum of amount of line items posted under 0057020000 G/L account",
            "expression": "SUM( 'Cost of Goods Sold'[Amount] )",
            "formatString": "#,0",
            "displayFolder": "General"
          },
          {
            "name": "Operations Scrap Amount",
            "description": "Sum of non process order scrap from accounting documents, scrap confirmation from process orders, process order scrap from goods issue material document transactions, and inventory reserve from material document transactions.",
            "expression": [
              "",
              "VAR PO_Scrap =",
              "    CALCULATE(",
              "        SUM( 'Goods Movement'[Amount] ) * -1,",
              "        KEEPFILTERS('Scrap Category'[Category] = \"Process Order Scrap\")",
              "    )",
              "",
              "-- Use G/L account items instead because it has correct debit/credit indicators",
              "VAR NonPO_Scrap =",
              "    CALCULATE(",
              "        SUM( 'Scrap Usage'[Amount] ),",
              "        kEEPFILTERS('Cost Element'[Category] = \"Scrap\"),",
              "        KEEPFILTERS('Cost Element'[Sub Category] = \"Non-PO Scrap\"),",
              "        KEEPFILTERS('Cost Element'[Operations Scrap Flag] = \"Yes\")",
              "    )",
              "",
              "-- ZSCRAP tcode only has confirmations on CAST orders",
              "VAR Scrap_Conf = SUM( 'Scrap Confirmation'[Amount] )",
              "",
              "VAR Inv_Reserve =",
              "    CALCULATE(",
              "        SUM( 'Goods Movement'[Amount] ) * -1,",
              "        KEEPFILTERS('Cost Element'[Category] = \"Inventory Reserve\"),",
              "        KEEPFILTERS('Cost Element'[Operations Scrap Flag] = \"Yes\")",
              "    )",
              "",
              "RETURN",
              "PO_Scrap + NonPO_Scrap + Scrap_Conf + Inv_Reserve"
            ],
            "formatString": "#,0",
            "displayFolder": "Operations Scrap"
          },
          {
            "name": "CONV Scrap Rate",
            "description": "Linear footage of goods issue and scrap transactions divided by the linear footage of goods receipt transactions from Converting machines posted in Converting storage bins.",
            "expression": [
              "CALCULATE(",
              "    [Goods Issue Scrap Rate],",
              "    KEEPFILTERS('Goods Movement'[Machine] IN {\"014\", \"088\", \"182\", \"733\", \"734\", \"735\", \"736\", \"737\", \"762\", \"MANUAL\"}),",
              "    KEEPFILTERS('Goods Movement'[Storage Bin] IN {\"600\", \"700\", \"733\", \"734\", \"735\", \"736\", \"737\", \"88\", \"182\", \"782\", \"555\"}),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Goods Movement'[Production Date] )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Scrap Footage",
            "description": "Total linear footage of goods issue and scrap transactions with movement types 261/262 and 551/552 from the Converting machines posted in Converting storage bins.",
            "expression": [
              "",
              "CALCULATE(",
              "    [Goods Issue Linear Footage],",
              "    KEEPFILTERS('Goods Movement'[Machine] IN {\"014\", \"088\", \"182\", \"733\", \"734\", \"735\", \"736\", \"737\", \"762\", \"MANUAL\"}),",
              "    KEEPFILTERS('Goods Movement'[Storage Bin] IN {\"600\", \"700\", \"733\", \"734\", \"735\", \"736\", \"737\", \"88\", \"182\", \"782\", \"555\"}),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Goods Movement'[Production Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Converting"
          },
          {
            "name": "Operations Scrap as % of COGS",
            "description": "Total scrap amount divided by cost of goods sold",
            "expression": "DIVIDE( [Operations Scrap Amount], [Cost of Goods Sold] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Operations Scrap"
          },
          {
            "name": "Operations Scrap as % of GR",
            "description": "Total scrap amount divided by the goods receipt amount",
            "expression": [
              "",
              "DIVIDE(",
              "    [Operations Scrap Amount],",
              "    CALCULATE(",
              "        [Operations Goods Receipt Amount],",
              "        ALL( 'Scrap Category' ),",
              "        ALL( 'Reason for Movement' ),",
              "        ALL( 'Material Master' ),",
              "        ALL( 'Work Center' ),",
              "        ALL( 'MFG Work Center' )",
              "    )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Operations Scrap"
          },
          {
            "name": "Scrap P&L Amount",
            "description": "Total amount of documents in the 'Scrap Usage' and 'Absorption' table",
            "expression": [
              "",
              "VAR Scrap = SUM( 'Scrap Usage'[Amount] )",
              "VAR Absorb = [Absorption]",
              "",
              "RETURN",
              "Scrap + Absorb"
            ],
            "formatString": "#,0",
            "displayFolder": "Scrap P&L"
          },
          {
            "name": "Operations Goods Receipt Amount",
            "description": "Total of transactions with 261 and 262 movement types with non-0000 reason codes in 'Goods Movement' table",
            "expression": [
              "",
              "CALCULATE(",
              "    ABS(SUM( 'Goods Movement'[Amount] )),",
              "    KEEPFILTERS('Goods Movement'[Movement Type] IN {\"261\", \"262\"}),",
              "    KEEPFILTERS('Goods Movement'[Reason Code for Movement] = \"0000\")",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Operations Scrap"
          },
          {
            "name": "Upstream Avg Scanned Tag Value",
            "description": "Average of values recorded in the PLC tags. Filter or grouped this measure by tag names to get the intended values per PLC tag.",
            "expression": "AVERAGE( 'Upstream Process Log'[Tag Value] )",
            "formatString": "#,0",
            "displayFolder": "Upstream Process Log"
          },
          {
            "name": "CONV Square Footage",
            "description": "Total linear footage of goods movement transaction with movement types 101/102 and 531/532 from the Converting machines posted in Converting storage bins. The dimensions are based on the width and length in the alternative unit of measure in the material master.",
            "expression": [
              "",
              "CALCULATE(",
              "    [Goods Receipt Square Footage],",
              "    KEEPFILTERS('Goods Movement'[Machine] IN {\"014\", \"088\", \"182\", \"733\", \"734\", \"735\", \"736\", \"737\", \"762\", \"MANUAL\"}),",
              "    KEEPFILTERS('Goods Movement'[Storage Bin] IN {\"600\", \"700\", \"733\", \"734\", \"735\", \"736\", \"737\", \"88\", \"182\", \"782\", \"555\"}),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Goods Movement'[Production Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Converting"
          },
          {
            "name": "Upstream Avg Cross Web Value",
            "description": "Average of values in the NDC profile",
            "expression": "AVERAGE( 'Upstream NDC Profile'[Average Cross Web Value] )",
            "formatString": "#,0.00000",
            "displayFolder": "Upstream Process Log"
          },
          {
            "name": "Website Bounce Rate",
            "description": "Sum of 'Website Traffic'[Bounces] divided by the sum of 'Website Traffic'[Sessions]",
            "expression": "DIVIDE( SUM( 'Website Traffic'[Bounces] ), [Website Sessions] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Marketing"
          },
          {
            "name": "Email Open Rate",
            "description": "Open rate is the percentage of people who opened your email out of the people who were delivered your email.",
            "expression": "DIVIDE( [Email Open Count], [Email Delivered Count] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Marketing"
          },
          {
            "name": "Email Click Through Rate",
            "description": "This is the percentage of people who clicked a link in your email out of the people who opened your email.",
            "expression": "DIVIDE( [Email Click Count], [Email Open Count] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Marketing"
          },
          {
            "name": "Prior Report Date",
            "expression": "([Report Date] - 1) + TIME(0,0,0)",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "End of Prior Month",
            "expression": "EOMONTH([Report Date], -1)",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "End of Prior Year",
            "expression": "DATE(YEAR([Report Date])-1, 12, 31)",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "Sunday of Current Week",
            "expression": [
              "",
              "CALCULATE(",
              "    MAX( 'Calendar'[Date] ),",
              "    FILTER(",
              "        ALL('Calendar'),",
              "        'Calendar'[Date] <= [Report Date]",
              "            && 'Calendar'[DayOfWeek] = 7",
              "    )",
              ")"
            ],
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "End of 2 Months Ago",
            "expression": "EOMONTH([Report Date], -2)",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "Sunday of Prior Week",
            "expression": "([Sunday of Current Week] - 7) + TIME(0,0,0)",
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": "_Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "COMP Actual Hours",
            "description": "Total hours spent by operators to perform a task in Compounding",
            "expression": "SUM( 'Compounding Log'[Actual Hours Spent] )",
            "formatString": "#,0.00",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Projected Direct Hours",
            "description": "Expected total hours spent by operators to perform a task directly related to production in Compounding",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Compounding Log'[Projected Hours Spent] ),",
              "    KEEPFILTERS( 'Compounding Log'[Labor Flag] = \"Yes\" ),",
              "    KEEPFILTERS( 'Compounding Log'[Task] <> \"MIXING\" )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Indirect Actual Hours",
            "description": "Total hours spent by operators to perform a task not directly related to production in Compounding",
            "expression": [
              "",
              "CALCULATE(",
              "    [COMP Actual Hours],",
              "    KEEPFILTERS( 'Compounding Log'[Indirect Flag] = \"Yes\" )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Labor Efficiency",
            "description": "Projected direct hours divided by the difference between actual hours and indirect actual hours. The training hours are excluded in the indirect hours so it negatively impacts the efficiency.",
            "expression": [
              "",
              "VAR training = ",
              "    CALCULATE(",
              "        [COMP Actual Hours],",
              "        KEEPFILTERS( 'Compounding Log'[Task] = \"TRAINING\" )",
              "    )",
              "",
              "RETURN",
              "DIVIDE(",
              "    [COMP Projected Direct Hours],",
              "    [COMP Actual Hours] - [COMP Indirect Actual Hours] + training",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Non-Production as % of Direct Labor",
            "description": "Total hours spent by operators to perform non-production task divided by the difference between actual hours and indirect actual hours",
            "expression": [
              "",
              "DIVIDE(",
              "    CALCULATE(",
              "        [COMP Actual Hours],",
              "        KEEPFILTERS( 'Compounding Log'[Task] = \"NON-PRODUCTION (5S/OTHER)\" )",

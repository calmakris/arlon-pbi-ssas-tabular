            "expression": " CALCULATE( [Sales Order Automation Efficiency%], DATESYTD( 'Calendar'[Date], \"09/30\" ) )",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "MTD Sales Order Automation Efficiency%",
            "description": "Date grouping: 'Sales Order Documents'[Enter Date]",
            "expression": " CALCULATE( [Sales Order Automation Efficiency%], DATESMTD( 'Calendar'[Date] ) )",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "Sales Order Lines Manual Entry%",
            "description": "Number of sales order line items entered by Customer Service divided by sales order line count entered by Customer Service and BATCHUSER; Date grouping: 'Sales Order Documents'[Enter Date]",
            "expression": " DIVIDE( [Sales Order Lines Manual], [Sales Order Line Count (Automated + Manual)] )",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "Sales Order Line Count (Automated + Manual)",
            "description": "Date grouping: 'Sales Order Documents'[Enter Date]",
            "expression": " [Sales Order Lines Manual] + [Sales Order Lines Automated]",
            "formatString": "#,0",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "MTD Absorption",
            "description": "Date grouping: 'Absorption'[Posting Date]",
            "expression": [
              " CALCULATE( [Absorption], DATESMTD( 'Calendar'[Date] ) )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "MTD PY Absorption",
            "description": "Date grouping: 'Absorption'[Posting Date]",
            "expression": [
              " CALCULATE( [MTD Absorption], SAMEPERIODLASTYEAR( 'Calendar'[Date] ) )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "PD PO Backorder Amount",
            "description": "Sum of open UB, NB, and ZNB purchase orders with GR requirement scheduled to be delivered up to prior day (date context: Snapshot Date or 'Calendar'[Date])",
            "expression": [
              " ",
              "CALCULATE( ",
              "    SUM( 'Backorder Snapshots'[Incoming Amount] ), ",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] = MAX( 'Calendar'[Date] ) - 1",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "PD Var PO Open Amount",
            "description": "Variance of PO Open Amount to prior day's open amount (PD PO Backorder Amount) which is the sum of open UB, NB, and ZNB purchase orders with GR requirement scheduled to be delivered up to prior day (date context: Snapshot Date or 'Calendar'[Date])",
            "expression": " [PO Open Amount] - [PD PO Backorder Amount]",
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "SO Customer Open Margin",
            "description": "If billing status is not complete, this value is margin amount of non-shipped; Date grouping: 'Sales Order Documents'[Enter Date]",
            "expression": [
              " ",
              "CALCULATE(",
              "    IF(",
              "        HASONEVALUE( '_Currency to Use'[Currency] ),",
              "        SWITCH(",
              "            VALUES( '_Currency to Use'[Currency] ),",
              "            \"USD\", SUM( 'Sales Order'[CustomerOpenMarginAmountUSD] ),",
              "            \"Local\", SUM( 'Sales Order'[CustomerOpenMarginAmount] ),",
              "            SUM( 'Sales Order'[CustomerOpenMarginAmountUSD] )",
              "        ),",
              "        SUM( 'Sales Order'[CustomerOpenMarginAmountUSD] )   ",
              "    ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] )",
              ")"
            ],
            "formatString": "#,##0;(#,##0);\"-\"",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "PD SO Customer Open Amount",
            "description": "Sum of open TA and ZCON sales orders with with active/pre-launch product status, no credit block, has product tier code, and scheduled to be delivered up to prior day (date context: Snapshot Date or 'Calendar'[Date]). Excludes BURLINGTON GRAPHICS customer.",
            "expression": [
              " ",
              "CALCULATE(",
              "    SUM('Backorder Snapshots'[Outgoing Amount]),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] = [Report Date] - 1",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "PD Var SO Customer Open Amount",
            "description": "Variance of SO Customer Open Amount to prior day's open amount (PD SO Customer Open Amount) which is the sum of open TA and ZCON sales orders with with active/pre-launch product status, no credit block, has product tier code, and scheduled to be delivered up to prior day (date context: Snapshot Date or 'Calendar'[Date])",
            "expression": " [SO Customer Open Amount] - [PD SO Customer Open Amount]",
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "GR Linear Footage",
            "description": "Total footage of postings with movement types 101, 102, 531, and 532.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Goods Movement'[Linear Feet] ),",
              "    KEEPFILTERS('Goods Movement'[Movement Type] IN {\"101\", \"531\", \"102\", \"532\"})",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Goods Receipts and Issues"
          },
          {
            "name": "GR Square Footage",
            "description": "Total square footage of postings with movement types 101, 102, 531, and 532.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Goods Movement'[Square Feet] ),",
              "    KEEPFILTERS('Goods Movement'[Movement Type] IN {\"101\", \"531\", \"102\", \"532\"})",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Goods Receipts and Issues"
          },
          {
            "name": "GI Linear Footage",
            "description": "Total footage of postings with movement types 261, 262, 551, and 552.",
            "expression": [
              "",
              "CALCULATE(",
              "    ABS( SUM( 'Goods Movement'[Linear Feet] ) ),",
              "    KEEPFILTERS('Goods Movement'[Movement Type] IN {\"261\", \"551\", \"262\", \"552\"}),",
              "    KEEPFILTERS('Goods Movement'[Storage Bin] IN {\"600\", \"700\", \"733\", \"734\", \"735\", \"736\", \"88\", \"182\", \"782\"}),",
              "    KEEPFILTERS('Goods Movement'[Reason Code for Movement] <> \"0000\")",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Goods Receipts and Issues"
          },
          {
            "name": "GI Scrap Rate",
            "description": "GI Footage over GR Footage",
            "expression": " DIVIDE( [GI Linear Footage], [GR Linear Footage] )",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Goods Receipts and Issues"
          },
          {
            "name": "GIR Linear Footage",
            "expression": " SUM ('Goods Movement'[Linear Feet])",
            "formatString": "#,0",
            "displayFolder": "Goods Receipts and Issues"
          },
          {
            "name": "GIR Square Footage",
            "expression": " SUM ('Goods Movement'[Square Feet])",
            "formatString": "#,0",
            "displayFolder": "Goods Receipts and Issues"
          },
          {
            "name": "GIR Quantity",
            "expression": " SUM( 'Goods Movement'[Quantity])",
            "formatString": "#,0.00",
            "displayFolder": "Goods Receipts and Issues"
          },
          {
            "name": "GIR Amount",
            "expression": " SUM( 'Goods Movement'[Amount] )",
            "formatString": "#,0",
            "displayFolder": "Goods Receipts and Issues"
          },
          {
            "name": "SO Allocated Amount",
            "description": "Total value of items in delivery notes issued without an actual PGI date set; Date grouping: 'Sales Order Documents'[First Delivery Date]",
            "expression": [
              " ",
              "CALCULATE(",
              "    IF(",
              "        HASONEVALUE( '_Currency to Use'[Currency] ),",
              "        SWITCH(",
              "            VALUES( '_Currency to Use'[Currency] ),",
              "            \"USD\", SUM( 'Sales Order'[AllocatedAmountUSD] ),",
              "            \"Local\", SUM( 'Sales Order'[AllocatedAmount] ),",
              "            SUM( 'Sales Order'[AllocatedAmountUSD] )",
              "        ),",
              "        SUM( 'Sales Order'[AllocatedAmountUSD] )   ",
              "    ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] )",
              ")"
            ],
            "formatString": "#,##0;(#,##0);\"-\"",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "SO Shipped Amount",
            "description": "Total value of items in delivery notes issued with actual PGI date set; Date grouping: 'Sales Order Documents'[First Delivery Date]",
            "expression": [
              "CALCULATE(",
              "    IF(",
              "        HASONEVALUE( '_Currency to Use'[Currency] ),",
              "        SWITCH(",
              "            VALUES( '_Currency to Use'[Currency] ),",
              "            \"USD\", SUM( 'Sales Order'[ShippedAmountUSD] ),",
              "            \"Local\", SUM( 'Sales Order'[ShippedAmount] ),",
              "            SUM( 'Sales Order'[ShippedAmountUSD] )",
              "        ),",
              "        SUM( 'Sales Order'[ShippedAmountUSD] )   ",
              "    ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] )",
              ")"
            ],
            "formatString": "#,##0;(#,##0);\"-\"",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "CONV Production Hours",
            "description": "The number of hours across all converting employees when they are producing material",
            "expression": " SUM( 'Converting Man Hours'[Production Hours] )",
            "formatString": "#,0.00",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Available Hours",
            "description": "The total number of hours converting is scheduled to work within a shift (excluding breaks and lunch)",
            "expression": " SUM( 'Converting Man Hours'[Available Hours] )",
            "formatString": "#,0.00",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Square Feet per Man Hour",
            "description": "Total number of square footage over total production hours in Converting",
            "expression": "DIVIDE( [CONV Square Footage], [CONV Production Hours] )",
            "formatString": "#,0",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Square Feet per Man Hour (Target)",
            "description": "These values are provided by Converting at the beginning of the fiscal year. If none provided, the default value used is 5,000.",
            "expression": [
              " ",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"Sqftmh\",",
              "        TREATAS(",
              "            VALUES( 'Company Code'[Company Code] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )",
              "RETURN",
              "    IF(",
              "        ISBLANK( TargetVal ),",
              "        5000,",
              "        TargetVal",
              "    )",
              " "
            ],
            "formatString": "#,0",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Square Feet per Man Hour (Target Variance)",
            "expression": " [CONV Square Feet per Man Hour] - [CONV Square Feet per Man Hour (Target)]",
            "formatString": "#,0",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Uptime Hours",
            "expression": " DIVIDE( DIVIDE( SUM( 'Converting Uptime'[Uptime Seconds] ), 60 ), 60 )",
            "formatString": "#,0.00",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Availability",
            "description": "Converting uptime hours divided by available hours",
            "expression": [
              " ",
              "AVERAGEX(",
              "    VALUES( 'Converting Uptime'[Machine Name] ),",
              "    CALCULATE(",
              "        AVERAGEX(",
              "            VALUES( 'Calendar'[Date] ),",
              "            DIVIDE( [CONV Uptime Hours], [CONV Available Hours] )",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Converting"
          },
          {
            "name": "PD CONV Linear Footage",
            "description": "If this is used without the date field, this measure requires a 'Time Intelligence[Time Calculation]' filter to be active.",
            "expression": [
              "",
              "CALCULATE( ",
              "    [CONV Linear Footage],",
              "    PREVIOUSDAY('Calendar'[Date])",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "PD CONV Scrap Footage",
            "description": "If this is used without the date field, this measure requires a 'Time Intelligence[Time Calculation]' filter to be active.",
            "expression": [
              "",
              "CALCULATE(",
              "    [CONV Scrap Footage],",
              "    PREVIOUSDAY( 'Calendar'[Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "PD CONV Scrap Rate",
            "description": "If this is used without the date field, this measure requires a 'Time Intelligence[Time Calculation]' filter to be active.",
            "expression": [
              "",
              "CALCULATE(",
              "    [CONV Scrap Rate],",
              "    PREVIOUSDAY( 'Calendar'[Date] )",
              ")"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "Current S-Block Quantity",
            "expression": " SUM( 'Blocked Inventory'[SBlock Quantity] )",
            "formatString": "#,##0.00;-#,##0.00;\"-\"",
            "displayFolder": "Inventory"
          },
          {
            "name": "Current Q-Block Quantity",
            "expression": " SUM( 'Blocked Inventory'[QBlock Quantity] )",
            "formatString": "#,##0.00;-#,##0.00;\"-\"",
            "displayFolder": "Inventory"
          },
          {
            "name": "Current Expired Quantity",
            "description": "Sum of 'Expired Inventory'[Stock Amount] where the [Expiration Date] is less than or equal to [Report Date]. Can be toggled between quantity and amount values (default = quantity)",
            "expression": [
              "",
              "VAR Qty =",
              "    CALCULATE(",
              "        SUM( 'Expired Inventory'[Stock Quantity] ),",
              "        FILTER(",
              "            ALL( 'Expired Inventory'[Expiration Date] ),",
              "            'Expired Inventory'[Expiration Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "VAR Amt =",
              "    CALCULATE(",
              "        SUM( 'Expired Inventory'[Stock Amount] ),",
              "        FILTER(",
              "            ALL( 'Expired Inventory'[Expiration Date] ),",
              "            'Expired Inventory'[Expiration Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "    IF(",
              "        HASONEVALUE( '_Value to Use'[Value] ),",
              "        SWITCH(",
              "            VALUES( '_Value to Use'[Value] ),",
              "            \"Quantity\", Qty,",
              "            \"Amount\", Amt,",
              "            Qty",
              "        ),",
              "        Qty",
              "    )"
            ],
            "formatString": "#,##0.00;-#,##0.00;\"-\"",
            "displayFolder": "Inventory"
          },
          {
            "name": "Upstream Actual Production Footage",
            "description": "Sum of 'Upstream Logs - Simplified'[Footage] of production records excluding FEs",
            "expression": [
              " ",
              "CALCULATE(",
              "    [Upstream Actual Footage],",
              "    FILTER(",
              "        'Upstream Log Simplified',",
              "        'Upstream Log Simplified'[Category] = \"Production\"",
              "            && LEFT('Upstream Log Simplified'[Causes], 4) = \"Roll\"",
              "            && NOT(CONTAINSSTRING('Upstream Log Simplified'[Causes], \"FE\"))",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Ideal Production Footage",
            "description": "Sum of 'Upstream Logs - Simplified'[Footage] of production records excluding FEs",
            "expression": [
              " ",
              "CALCULATE(",
              "    [Upstream Ideal Footage],",
              "    FILTER(",
              "        'Upstream Log Simplified',",
              "        'Upstream Log Simplified'[Category] = \"Production\"",
              "            && LEFT('Upstream Log Simplified'[Causes], 4) = \"Roll\"",
              "            && NOT(CONTAINSSTRING('Upstream Log Simplified'[Causes], \"FE\"))",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Uptime Minutes",
            "description": "Sum of duration between 'Start Time' and 'Stop Time' in minutes of production records excluding FEs",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Upstream Log Simplified'[DurationMinute] ),",
              "    FILTER(",
              "        'Upstream Log Simplified',",
              "        'Upstream Log Simplified'[Category] = \"Production\"",
              "            && NOT(CONTAINSSTRING('Upstream Log Simplified'[Causes], \"FE\"))",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Runtime Minutes",
            "description": "Sum of duration between 'Start Time' and 'Stop Time' in minutes of all records except end of production, EOS, and FEs",
            "expression": [
              "",
              "VAR CAST = ",
              "    CALCULATE(",
              "        SUM( 'Upstream Log Simplified'[DurationMinute] ),",
              "        FILTER(",
              "            'Upstream Log Simplified',",
              "            'Upstream Log Simplified'[Category] <> \"End of Production\"",
              "                && 'Upstream Log Simplified'[Further Detail] <> \"EOS\"",
              "                && NOT(CONTAINSSTRING('Upstream Log Simplified'[Causes], \"FE\"))",
              "        )",
              "    )",
              "",
              "VAR COAT = ",
              "    CALCULATE(",
              "        SUM( 'Upstream Log Simplified'[DurationMinute] ),",
              "        FILTER(",
              "            'Upstream Log Simplified',",
              "            'Upstream Log Simplified'[Category] <> \"End of Production\"",
              "                && 'Upstream Log Simplified'[Further Detail] <> \"EOS\"",
              "                && 'Upstream Log Simplified'[Non Uptime Reason] <> \"PLANNED DT\"",
              "                && NOT(CONTAINSSTRING('Upstream Log Simplified'[Causes], \"FE\"))",
              "        )",
              "    )",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(Machine[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(Machine[Department]), COAT,",
              "    CAST",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Actual Footage",
            "description": "Sum of 'Upstreal Logs - Simplified'[Footage]",
            "expression": " SUM('Upstream Log Simplified'[Footage])",
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Ideal Footage",
            "description": "Sum of 'Upstreal Logs - Simplified'[Ideal Footage]. Ideal footage is derived from the specification linespeed and duration between 'Start Time' and 'Stop Time' in minutes",
            "expression": "SUM('Upstream Log Simplified'[IdealFootage])",
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Downtime Hours",
            "description": "Sum of duration between 'Start Time' and 'Stop Time' in minutes of unplanned clean up and unplanned downtime records",
            "expression": [
              "",
              "CALCULATE(",
              "    DIVIDE( SUM( 'Upstream Log Simplified'[DurationMinute] ), 60 ) + 0,",
              "    FILTER(",
              "        'Upstream Log Simplified',",
              "        'Upstream Log Simplified'[Category] IN { \"Clean Up\", \"Downtime\" }",
              "            && LEFT('Upstream Log Simplified'[Causes], 9) = \"Unplanned\"",
              "    )",
              ")"
            ],
            "formatString": "#,###0.00 \"hr\";-#,###0.00 #,###0.00 \"hr\";0 \"hr\"",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "CAST Clean Up Count",
            "expression": "COUNTROWS(FILTER('Upstream Log Simplified', 'Upstream Log Simplified'[Category] = \"Clean Up\" && 'Upstream Log Simplified'[Causes] IN { \"Planned\", \"Unplanned\", \"Unplanned - Casting\" })) + 0",
            "formatString": "#,0",
            "isHidden": true
          },
          {
            "name": "COAT Clean Up Count",
            "expression": "COUNTROWS(FILTER('Upstream Log', 'Upstream Log'[Category] = \"Clean Up\" && 'Upstream Log'[Causes] IN { \"Planned\", \"Unplanned\" })) + 0",
            "formatString": "#,0",
            "isHidden": true
          },
          {
            "name": "Upstream Planned CU Count",
            "description": "Number of planned clean up records. For casting, the clean ups from 'Upstream Logs - Simplified' table is counted where consecutively similar clean ups are combined into one clean up. For coating, it is the raw count of clean ups in 'Upstream Logs - Raw'.",
            "expression": [
              "",
              "VAR CAST = CALCULATE( [CAST Clean Up Count], 'Upstream Log Simplified'[Causes] = \"Planned\" )",
              "VAR COAT = CALCULATE( [COAT Clean Up Count], 'Upstream Log'[Causes] = \"Planned\" )",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(Machine[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(Machine[Department]), COAT,",
              "    CAST + COAT",
              ")",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Unplanned CU Count",
            "description": "Number of unplanned clean up records. For casting, the clean ups from 'Upstream Logs - Simplified' table is counted where consecutively similar clean ups are combined into one clean up. For coating, it is the raw count of clean ups in 'Upstream Logs - Raw'",
            "expression": [
              "",
              "VAR CAST = CALCULATE( [CAST Clean Up Count], 'Upstream Log Simplified'[Causes] = \"Unplanned - Casting\" )",
              "VAR COAT = CALCULATE( [COAT Clean Up Count], 'Upstream Log'[Causes] = \"Unplanned\" )",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(Machine[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(Machine[Department]), COAT,",
              "    CAST + COAT",
              ")",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Performance",
            "description": "Upstream actual production footage divided by ideal production footage",
            "expression": [
              " ",
              "CALCULATE(",
              "    DIVIDE( [Upstream Actual Production Footage], [Upstream Ideal Production Footage] ),",
              "    ALLSELECTED(",
              "        'Upstream Log Simplified'[Category],",
              "        'Upstream Log Simplified'[Causes],",
              "        'Upstream Log Simplified'[Further Detail],",
              "        'Upstream Log Simplified'[Comments],",
              "        'Upstream Log Simplified'[Non Uptime Reason],",
              "        'Upstream Log Simplified'[Specification Number],",
              "        'Upstream Log Simplified'[Efficiency Hitter]",
              "    )",
              ")"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Availability",
            "description": "Upstream uptime duration minutes divided by runtime duration minutes",
            "expression": [
              " ",
              "CALCULATE(",
              "    DIVIDE( [Upstream Uptime Minutes], [Upstream Runtime Minutes] ),",
              "    ALLSELECTED(",
              "        'Upstream Log Simplified'[Category],",
              "        'Upstream Log Simplified'[Causes],",
              "        'Upstream Log Simplified'[Further Detail],",
              "        'Upstream Log Simplified'[Comments],",
              "        'Upstream Log Simplified'[Non Uptime Reason],",
              "        'Upstream Log Simplified'[Specification Number],",
              "        'Upstream Log Simplified'[Efficiency Hitter]",
              "    )",
              ")",
              ""
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Log"
          },
          {
            "name": "Upstream Overall Equipment Efficiency",

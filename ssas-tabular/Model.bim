        "measures": [
          {
            "name": "Gross Sales",
            "description": "Total revenue amount of billing documents posted in SAP. This value is toggleable to local currency using the \"_Currency To Use\" table.",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice'[GrossSalesAmount] ),",
              "        \"Local\", SUMX( 'Sales Invoice', 'Sales Invoice'[GrossSalesAmount] * 'Sales Invoice'[Exchange Rate] ),",
              "        SUM('Sales Invoice'[GrossSalesAmount] )",
              "    ),",
              "    SUM('Sales Invoice'[GrossSalesAmount] )    ",
              ")",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Net Sales",
            "description": "Difference between gross sales and discounts applied to gross sales",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice'[NetSales] ),",
              "        \"Local\", SUMX( 'Sales Invoice',  'Sales Invoice'[NetSales] * 'Sales Invoice'[Exchange Rate] ),",
              "        SUM('Sales Invoice'[NetSales] )",
              "    ),",
              "    SUM('Sales Invoice'[NetSales] )    ",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Gross Margin",
            "description": "Difference between gross sales and standard cost in the billing document. This value is toggleable to local currency using the \"_Currency To Use\" table.",
            "expression": "SUM( 'Sales Invoice'[GrossMargin] )",
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Net Margin",
            "description": "Difference between net sales and standard cost in the billing document. This value is toggleable to local currency using the \"_Currency To Use\" table.",
            "expression": " [Net Sales] - [Frozen Standard Cost]",
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Gross Margin %",
            "description": "Gross margin divided by gross sales. Gross margin is the difference between gross sales and standard cost in the billing document.",
            "expression": " DIVIDE([Gross Margin], [Gross Sales])",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Net Margin %",
            "description": "Net margin divided by net sales. Net margin is the difference between net sales and standard cost in the billing document.",
            "expression": " DIVIDE( [Net Margin], [Net Sales] )",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Sales Invoice"

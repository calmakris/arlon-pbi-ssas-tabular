          },
          {
            "name": "CONV Square Feet per Man Hour (Target Variance)",
            "expression": " [CONV Square Feet per Man Hour] - [CONV Square Feet per Man Hour (Target)]",
            "formatString": "#,0",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Uptime Hours",
            "expression": " SUM( 'Converting Uptime'[Uptime Seconds] ) / 60 / 60",
            "formatString": "#,0.00",
            "displayFolder": "Converting"
          },
          {
            "name": "CONV Availability",
            "description": "Converting uptime hours divided by available hours",
            "expression": [
              " ",
              "AVERAGEX(",
              "    VALUES( 'Converting Uptime'[Machine Name] ),",
              "    CALCULATE(",
              "        AVERAGEX(",
              "            VALUES( 'Calendar'[Date] ),",
              "            DIVIDE( [CONV Uptime Hours], [CONV Available Hours] )",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Converting"
          },
          {
            "name": "PD GR Linear Footage",
            "description": "If this is used without the date field, this measure requires a 'Time Intelligence[Time Calculation]' filter to be active.",
            "expression": " CALCULATE( [GR Linear Footage],PREVIOUSDAY('Calendar'[Date]) )",
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "PD GI Linear Footage",
            "description": "If this is used without the date field, this measure requires a 'Time Intelligence[Time Calculation]' filter to be active.",
            "expression": " CALCULATE( [GI Linear Footage], PREVIOUSDAY( 'Calendar'[Date] ) )",
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "PD GI Scrap Rate",
            "description": "If this is used without the date field, this measure requires a 'Time Intelligence[Time Calculation]' filter to be active.",
            "expression": " CALCULATE( [GI Scrap Rate], PREVIOUSDAY( 'Calendar'[Date] ) )",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "Current S-Block Quantity",
            "expression": " SUM( 'Blocked Inventory'[SBlock Quantity] )",
            "formatString": "#,##0.00;-#,##0.00;\"-\"",
            "displayFolder": "Inventory"
          },
          {
            "name": "Current Q-Block Quantity",
            "expression": " SUM( 'Blocked Inventory'[QBlock Quantity] )",
            "formatString": "#,##0.00;-#,##0.00;\"-\"",
            "displayFolder": "Inventory"
          },
          {
            "name": "Current Expired Quantity",
            "description": "Sum of 'Expired Inventory'[Stock Amount] where the [Expiration Date] is less than or equal to [Report Date]. Can be toggled between quantity and amount values (default = quantity)",
            "expression": [
              "",
              "VAR Qty =",
              "    CALCULATE(",
              "        SUM( 'Expired Inventory'[Stock Quantity] ),",
              "        FILTER(",
              "            ALL( 'Expired Inventory'[Expiration Date] ),",
              "            'Expired Inventory'[Expiration Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "VAR Amt =",
              "    CALCULATE(",
              "        SUM( 'Expired Inventory'[Stock Amount] ),",
              "        FILTER(",
              "            ALL( 'Expired Inventory'[Expiration Date] ),",
              "            'Expired Inventory'[Expiration Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "    IF(",
              "        HASONEVALUE( '_Value to Use'[Value] ),",
              "        SWITCH(",
              "            VALUES( '_Value to Use'[Value] ),",
              "            \"Quantity\", Qty,",
              "            \"Amount\", Amt,",
              "            Qty",
              "        ),",
              "        Qty",
              "    )"
            ],
            "formatString": "#,##0.00;-#,##0.00;\"-\"",
            "displayFolder": "Inventory"
          },
          {
            "name": "Upstream Actual Production Footage",
            "description": "Sum of 'Upstream Logs - Simplified'[Footage] of production records excluding FEs",
            "expression": [
              " ",
              "CALCULATE(",
              "    [Upstream Actual Footage],",
              "    FILTER(",
              "        'Upstream Logs - Simplified',",
              "        'Upstream Logs - Simplified'[Category] = \"Production\"",
              "            && LEFT('Upstream Logs - Simplified'[Causes], 4) = \"Roll\"",
              "            && NOT(CONTAINSSTRING('Upstream Logs - Simplified'[Causes], \"FE\"))",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Ideal Production Footage",
            "description": "Sum of 'Upstream Logs - Simplified'[Footage] of production records excluding FEs",
            "expression": [
              " ",
              "CALCULATE(",
              "    [Upstream Ideal Footage],",
              "    FILTER(",
              "        'Upstream Logs - Simplified',",
              "        'Upstream Logs - Simplified'[Category] = \"Production\"",
              "            && LEFT('Upstream Logs - Simplified'[Causes], 4) = \"Roll\"",
              "            && NOT(CONTAINSSTRING('Upstream Logs - Simplified'[Causes], \"FE\"))",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Uptime Minutes",
            "description": "Sum of duration between 'Start Time' and 'Stop Time' in minutes of production records excluding FEs",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Upstream Logs - Simplified'[Duration Minutes] ),",
              "    FILTER(",
              "        'Upstream Logs - Simplified',",
              "        'Upstream Logs - Simplified'[Category] = \"Production\"",
              "            && NOT(CONTAINSSTRING('Upstream Logs - Simplified'[Causes], \"FE\"))",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Runtime Minutes",
            "description": "Sum of duration between 'Start Time' and 'Stop Time' in minutes of all records except end of production, EOS, and FEs",
            "expression": [
              "",
              "VAR CAST = ",
              "    CALCULATE(",
              "        SUM( 'Upstream Logs - Simplified'[Duration Minutes] ),",
              "        FILTER(",
              "            'Upstream Logs - Simplified',",
              "            'Upstream Logs - Simplified'[Category] <> \"End of Production\"",
              "                && 'Upstream Logs - Simplified'[Further Detail] <> \"EOS\"",
              "                && NOT(CONTAINSSTRING('Upstream Logs - Simplified'[Causes], \"FE\"))",
              "        )",
              "    )",
              "",
              "VAR COAT = ",
              "    CALCULATE(",
              "        SUM( 'Upstream Logs - Simplified'[Duration Minutes] ),",
              "        FILTER(",
              "            'Upstream Logs - Simplified',",
              "            'Upstream Logs - Simplified'[Category] <> \"End of Production\"",
              "                && 'Upstream Logs - Simplified'[Further Detail] <> \"EOS\"",
              "                && 'Upstream Logs - Simplified'[Non Uptime Reason] <> \"PLANNED DT\"",
              "                && NOT(CONTAINSSTRING('Upstream Logs - Simplified'[Causes], \"FE\"))",
              "        )",
              "    )",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(_Machines[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(_Machines[Department]), COAT,",
              "    CAST",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Actual Footage",
            "description": "Sum of 'Upstreal Logs - Simplified'[Footage]",
            "expression": " SUM('Upstream Logs - Simplified'[Footage])",
            "formatString": "#,0",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Ideal Footage",
            "description": "Sum of 'Upstreal Logs - Simplified'[Ideal Footage]. Ideal footage is derived from the specification linespeed and duration between 'Start Time' and 'Stop Time' in minutes",
            "expression": "SUM('Upstream Logs - Simplified'[Ideal Footage])",
            "formatString": "#,0",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Downtime Hours",
            "description": "Sum of duration between 'Start Time' and 'Stop Time' in minutes of unplanned clean up and unplanned downtime records",
            "expression": [
              "",
              "CALCULATE(",
              "    DIVIDE( SUM( 'Upstream Logs - Simplified'[Duration Minutes] ), 60 ) + 0,",
              "    FILTER(",
              "        'Upstream Logs - Simplified',",
              "        'Upstream Logs - Simplified'[Category] IN { \"Clean Up\", \"Downtime\" }",
              "            && LEFT('Upstream Logs - Simplified'[Causes], 9) = \"Unplanned\"",
              "    )",
              ")"
            ],
            "formatString": "#,###0.00 \"hr\";-#,###0.00 #,###0.00 \"hr\";0 \"hr\"",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "CAST Clean Up Count",
            "expression": "COUNTROWS(FILTER('Upstream Logs - Simplified', 'Upstream Logs - Simplified'[Category] = \"Clean Up\" && 'Upstream Logs - Simplified'[Causes] IN { \"Planned\", \"Unplanned\", \"Unplanned - Casting\" })) + 0",
            "formatString": "#,0",
            "isHidden": true
          },
          {
            "name": "COAT Clean Up Count",
            "expression": "COUNTROWS(FILTER('Upstream Logs - Raw', 'Upstream Logs - Raw'[Category] = \"Clean Up\" && 'Upstream Logs - Raw'[Causes] IN { \"Planned\", \"Unplanned\" })) + 0",
            "formatString": "#,0",
            "isHidden": true
          },
          {
            "name": "Upstream Planned CU Count",
            "description": "Number of planned clean up records. For casting, the clean ups from 'Upstream Logs - Simplified' table is counted where consecutively similar clean ups are combined into one clean up. For coating, it is the raw count of clean ups in 'Upstream Logs - Raw'.",
            "expression": [
              "",
              "VAR CAST = CALCULATE( [CAST Clean Up Count], 'Upstream Logs - Simplified'[Causes] = \"Planned\" )",
              "VAR COAT = CALCULATE( [COAT Clean Up Count], 'Upstream Logs - Raw'[Causes] = \"Planned\" )",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(_Machines[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(_Machines[Department]), COAT,",
              "    CAST + COAT",
              ")",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Unplanned CU Count",
            "description": "Number of unplanned clean up records. For casting, the clean ups from 'Upstream Logs - Simplified' table is counted where consecutively similar clean ups are combined into one clean up. For coating, it is the raw count of clean ups in 'Upstream Logs - Raw'",
            "expression": [
              "",
              "VAR CAST = CALCULATE( [CAST Clean Up Count], 'Upstream Logs - Simplified'[Causes] = \"Unplanned - Casting\" )",
              "VAR COAT = CALCULATE( [COAT Clean Up Count], 'Upstream Logs - Raw'[Causes] = \"Unplanned\" )",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(_Machines[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(_Machines[Department]), COAT,",
              "    CAST + COAT",
              ")",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Performance",
            "description": "Upstream actual production footage divided by ideal production footage",
            "expression": [
              " ",
              "CALCULATE(",
              "    DIVIDE( [Upstream Actual Production Footage], [Upstream Ideal Production Footage] ),",
              "    ALL(",
              "        'Upstream Logs - Simplified'[Category],",
              "        'Upstream Logs - Simplified'[Causes],",
              "        'Upstream Logs - Simplified'[Further Detail],",
              "        'Upstream Logs - Simplified'[Comments],",
              "        'Upstream Logs - Simplified'[Non Uptime Reason],",
              "        'Upstream Logs - Simplified'[Specification Number],",
              "        'Upstream Logs - Simplified'[Efficiency Hitter]",
              "    )",
              ")"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Availability",
            "description": "Upstream uptime duration minutes divided by runtime duration minutes",
            "expression": [
              " ",
              "CALCULATE(",
              "    DIVIDE( [Upstream Uptime Minutes], [Upstream Runtime Minutes] ),",
              "    ALL(",
              "        'Upstream Logs - Simplified'[Category],",
              "        'Upstream Logs - Simplified'[Causes],",
              "        'Upstream Logs - Simplified'[Further Detail],",
              "        'Upstream Logs - Simplified'[Comments],",
              "        'Upstream Logs - Simplified'[Non Uptime Reason],",
              "        'Upstream Logs - Simplified'[Specification Number],",
              "        'Upstream Logs - Simplified'[Efficiency Hitter]",
              "    )",
              ")",
              ""
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Overall Equipment Efficiency",
            "description": "Product of upstream availability, performance, and quality",
            "expression": "[Upstream Availability] * [Upstream Performance] * [Upstream Quality]",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream First Pass Reliability",
            "description": "Number of planned clean ups divided by the total planned and unplanned clean ups",
            "expression": " DIVIDE( [Upstream Planned CU Count], [Upstream Planned CU Count] + [Upstream Unplanned CU Count] )",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Changeover Time",
            "description": "Average duration of clean up records in 'Upstream Logs - Simplified'",
            "expression": [
              "",
              "CALCULATE(",
              "    AVERAGE( 'Upstream Logs - Simplified'[Duration Minutes] ),",
              "    FILTER(",
              "        'Upstream Logs - Simplified',",
              "        'Upstream Logs - Simplified'[Category] = \"Clean Up\"",
              "    )",
              ")"
            ],
            "formatString": "#,### \"min\"",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "CAST Overall Equipment Efficiency (Target)",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( Target_KPI[TargetValue] ),",
              "        Target_KPI[KpiKey] = \"OEE_CAST\",",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            Target_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.85,",
              "        TargetVal",
              "    )"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "isHidden": true,
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "COAT Overall Equipment Efficiency (Target)",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( Target_KPI[TargetValue] ),",
              "        Target_KPI[KpiKey] = \"OEE_COAT\",",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            Target_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.85,",
              "        TargetVal",
              "    )"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "isHidden": true,
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Overall Equipment Efficiency (Target)",
            "description": "Monthly target data supplied by Engineering once every fiscal year",
            "expression": [
              "",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED('_Machines'[Department]), [CAST Overall Equipment Efficiency (Target)],",
              "    \"COAT\" IN ALLSELECTED(_Machines[Department]), [COAT Overall Equipment Efficiency (Target)],",
              "    ([CAST Overall Equipment Efficiency (Target)] + [COAT Overall Equipment Efficiency (Target)]) / 2",
              ")"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "CAST First Pass Reliability (Target)",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( Target_KPI[TargetValue] ),",
              "        Target_KPI[KpiKey] = \"FPass_CAST\",",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            Target_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.95,",
              "        TargetVal",
              "    )"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "isHidden": true,
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "COAT First Pass Reliability (Target)",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( Target_KPI[TargetValue] ),",
              "        Target_KPI[KpiKey] = \"FPass_COAT\",",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            Target_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.95,",
              "        TargetVal",
              "    )"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "isHidden": true,
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream First Pass Reliability (Target)",
            "description": "Monthly target data supplied by Engineering once every fiscal year",
            "expression": [
              "",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED('_Machines'[Department]), [CAST First Pass Reliability (Target)],",
              "    \"COAT\" IN ALLSELECTED(_Machines[Department]), [COAT First Pass Reliability (Target)],",
              "    ([CAST First Pass Reliability (Target)] + [COAT First Pass Reliability (Target)]) / 2",
              ")"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Changeover Time (Target)",
            "description": "Average duration of clean up records YTD last fiscal year in 'Upstream Logs - Simplified'",
            "expression": [
              "",
              "CALCULATE(",
              "    [Upstream Changeover Time],",
              "    SAMEPERIODLASTYEAR(DATESYTD( 'Calendar'[Date], \"09/30\"))",
              ")"
            ],
            "formatString": "#,### \"min\"",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Performance Impact",
            "description": "Impact on the overall performance of the department. Best used if analyzed by Further Detail, Comments, Non Uptime Reason, or Efficiency Hitter in 'Upstream Logs - Simplified'.",
            "expression": [
              "",
              "DIVIDE(",
              "    [Upstream Ideal Production Footage] - [Upstream Actual Production Footage],",
              "    CALCULATE(",
              "        [Upstream Ideal Production Footage],",
              "        ALL(",
              "            'Upstream Logs - Simplified'[Category],",
              "            'Upstream Logs - Simplified'[Causes],",
              "            'Upstream Logs - Simplified'[Further Detail],",
              "            'Upstream Logs - Simplified'[Comments],",
              "            'Upstream Logs - Simplified'[Non Uptime Reason],",
              "            'Upstream Logs - Simplified'[Specification Number],",
              "            'Upstream Logs - Simplified'[Efficiency Hitter]",
              "        ),",
              "        ALL(_Machines[Machine Name])",
              "    )",
              ")"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Availability Impact",
            "description": "Impact on the overall availablility of the department. Best used if analyzed by Further Detail, Comments, Non Uptime Reason, or Efficiency Hitter in 'Upstream Logs - Simplified'.",
            "expression": [
              "",
              "DIVIDE(",
              "    CALCULATE(",
              "        [Upstream Runtime Minutes],",
              "        FILTER(",
              "            'Upstream Logs - Simplified',",
              "            'Upstream Logs - Simplified'[Category] <> \"Production\"",
              "        )",
              "    ),",
              "    CALCULATE(",
              "        [Upstream Runtime Minutes],",
              "        ALL(",
              "            'Upstream Logs - Simplified'[Category],",
              "            'Upstream Logs - Simplified'[Causes],",
              "            'Upstream Logs - Simplified'[Further Detail],",
              "            'Upstream Logs - Simplified'[Comments],",
              "            'Upstream Logs - Simplified'[Non Uptime Reason],",
              "            'Upstream Logs - Simplified'[Specification Number],",
              "            'Upstream Logs - Simplified'[Efficiency Hitter]",
              "        ),",
              "        ALL(_Machines[Machine Name])",
              "    )",
              ")"
            ],
            "formatString": "#,0.00%;-#,0.00%;#,0.00%",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Quality",
            "description": "A fixed number defined by Engineering",
            "expression": [
              "",
              "VAR CAST = 0.95",
              "VAR COAT = 0.98",
              "",
              "RETURN",
              "SWITCH(TRUE(),",
              "    \"CAST\" IN ALLSELECTED(_Machines[Department]), CAST,",
              "    \"COAT\" IN ALLSELECTED(_Machines[Department]), COAT,",
              "    0.95",
              ")"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Overall Equipment Efficiency Impact",
            "description": "Impact on the overall equipment efficiency of the department. Best used if analyzed by Further Detail, Comments, Non Uptime Reason, or Efficiency Hitter in 'Upstream Logs - Simplified'.",
            "expression": "(( [Upstream Availability] + [Upstream Availability Impact] ) * ( [Upstream Performance] + [Upstream Performance Impact] ) * [Upstream Quality] ) - [Upstream Overall Equipment Efficiency]",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Days Since Last Unplanned Clean Up",
            "description": "Number of days between yesterday and the last unplanned clean up before yesterday",
            "expression": [
              "",
              "VAR Last_Date = ",
              "    SWITCH( TRUE(),",
              "        \"CAST\" IN ALLSELECTED(_Machines[Department]),",
              "        CALCULATE(",
              "            MAX( 'Upstream Logs - Simplified'[Production Date] ),",
              "            FILTER(",
              "                'Upstream Logs - Simplified',",
              "                'Upstream Logs - Simplified'[Production Date] <= [Report Date]",
              "                    && 'Upstream Logs - Simplified'[Category] = \"Clean Up\"",
              "                    && 'Upstream Logs - Simplified'[Causes] = \"Unplanned - Casting\"",
              "            )    ",
              "        ),",
              "        \"COAT\" IN ALLSELECTED(_Machines[Department]),",
              "        CALCULATE(",
              "            MAX( 'Upstream Logs - Raw'[Production Date] ),",
              "            FILTER(",
              "                'Upstream Logs - Raw',",
              "                'Upstream Logs - Raw'[Production Date] <= [Report Date]",
              "                    && 'Upstream Logs - Raw'[Category] = \"Clean Up\"",
              "                    && 'Upstream Logs - Raw'[Causes] = \"Unplanned\"",
              "            )    ",
              "        )",
              "    )",
              "",
              "RETURN",
              "DATEDIFF(Last_Date, [Yesterday], DAY)"
            ],
            "formatString": "#,###0 \"days\"",
            "displayFolder": "Upstream Logs"
          },
          {
            "name": "Upstream Changeover Scrap",
            "description": "Average footage of clean up records. In Casting, this value is the caliper scrap. In Coating, it is the combined footage of consecutive clean up and setup records.",
            "expression": [
              "",
              "CALCULATE(",
              "    AVERAGE( 'Upstream Logs - Simplified'[Scrap Footage] ),",
              "    FILTER(",
              "        'Upstream Logs - Simplified',",
              "        'Upstream Logs - Simplified'[Category] = \"Clean Up\"",

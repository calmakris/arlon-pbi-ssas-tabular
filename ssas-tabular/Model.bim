              "        'Sales Order',",
              "        'Sales Order'[Sample Category] <> \"Not Applicable\"",
              "    )",
              ") +",
              "SUM( 'Odoo Sample Request'[Cost Amount] )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Sample Requests"
          },
          {
            "name": "Avg Monthly Inventory Consumption Last 3 Months",
            "description": "Monthly average of sum of 'Inventory Consumption'[Quantity] posted in the previous 3 calendar months. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "VAR NumOfMonths = 3",
              "VAR Age = DATEDIFF( MAX( 'Material Master'[Created Date] ), [Report Date], MONTH )",
              "VAR Result = ",
              "    DIVIDE(",
              "        CALCULATE(",
              "            [Inventory Consumption (Dynamic)],",
              "            DATESINPERIOD(",
              "                'Calendar'[Start of Month],",
              "                [Beginning of Last Month],",
              "                -NumOfMonths,",
              "                MONTH)",
              "        ) + 0,",
              "        NumOfMonths",
              "    )",
              "RETURN",
              "    IF(",
              "        (Result || [Unrestricted Stock Quantity]) && Age >= NumOfMonths,",
              "        ABS( Result ),",
              "        BLANK() ",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory\\_Consumption"
          },

        ],
        "measures": [
          {
            "name": "Gross Sales",
            "description": "(SAP Source: Sum of CE110US.ERLOS)",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice Documents'[Gross Sales Amount] ),",
              "        \"Local\", SUMX( 'Sales Invoice Documents', 'Sales Invoice Documents'[Gross Sales Amount] * 'Sales Invoice Documents'[Exchange Rate] ),",
              "        SUM('Sales Invoice Documents'[Gross Sales Amount] )",
              "    ),",
              "    SUM('Sales Invoice Documents'[Gross Sales Amount] )    ",
              ")"
            ],
            "formatString": "#,##0;(#,##0);\"-\"",
            "displayFolder": "Sales"
          },
          {
            "name": "Net Sales",
            "description": "Net of gross sales and discounts",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice Documents'[Net Sales Amount] ),",
              "        \"Local\", SUMX( 'Sales Invoice Documents',  'Sales Invoice Documents'[Net Sales Amount] * 'Sales Invoice Documents'[Exchange Rate] ),",
              "        SUM('Sales Invoice Documents'[Net Sales Amount] )",
              "    ),",
              "    SUM('Sales Invoice Documents'[Net Sales Amount] )    ",
              ")"
            ],
            "formatString": "#,##0;(#,##0);\"-\"",
            "displayFolder": "Sales"
          },
          {
            "name": "Cost of Goods",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice Documents'[Overall Cost Amount] ),",
              "        \"Local\", SUMX( 'Sales Invoice Documents', 'Sales Invoice Documents'[Overall Cost Amount] * 'Sales Invoice Documents'[Exchange Rate] ),",
              "        SUM('Sales Invoice Documents'[Overall Cost Amount] )",
              "    ),",
              "    SUM('Sales Invoice Documents'[Overall Cost Amount] )    ",
              ")"
            ],
            "formatString": "#,##0;(#,##0);\"-\"",
            "displayFolder": "Sales"
          },
          {
            "name": "Gross Margin",
            "description": "Net of gross sales and overall cost",
            "expression": " [Gross Sales] - [Cost of Goods]",
            "formatString": "#,##0;(#,##0);\"-\"",
            "displayFolder": "Sales"
          },
          {
            "name": "Net Margin",
            "expression": " [Net Sales] - [Cost of Goods]",
            "formatString": "#,##0;(#,##0);\"-\"",
            "displayFolder": "Sales"
          },
          {
            "name": "Gross Margin %",
            "expression": " DIVIDE([Gross Margin], [Gross Sales])",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Sales"
          },
          {
            "name": "Net Margin %",
            "expression": " DIVIDE( [Net Margin], [Net Sales] )",
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Sales"
          },
          {
            "name": "Complaint Value",
            "expression": " SUM('Complaint Documents'[Complaint Amount]) + 0",
            "formatString": "\\$#,0;(\\$#,0);\\$#,0",
            "isHidden": true,

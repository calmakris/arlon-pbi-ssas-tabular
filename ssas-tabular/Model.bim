              "            MIN( 'Material Costing History'[LatestRank] ),",
              "            IF(",
              "                _isMaterialFiltered,",
              "                CALCULATE(",
              "                    MIN( 'Material Costing History'[LatestRank] ),",
              "                    KEEPFILTERS( 'Plant'[Plant] = \"1001\" )",
              "                ),",
              "                BLANK()",
              "            )",
              "        ),",
              "        FILTER(",
              "            'Material Costing History',",
              "            'Material Costing History'[Costing Start Date] <= _maxVisibleDate",
              "        )",
              "    )",
              "",
              "RETURN",
              "    IF(",
              "        ISBLANK(_latestRank),",
              "        BLANK(),",
              "        SWITCH(",
              "            TRUE(),",
              "            HASONEVALUE( Plant[Plant] ),",
              "                CALCULATE(",
              "                    AVERAGE( 'Material Costing History'[Standard Cost] ),",
              "                    KEEPFILTERS( 'Material Costing History'[LatestRank] = _latestRank + 1 )",
              "                ),",
              "            CALCULATE(",
              "                AVERAGE( 'Material Costing History'[Standard Cost] ),",
              "                KEEPFILTERS( 'Material Costing History'[LatestRank] = _latestRank + 1 ),",
              "                KEEPFILTERS( Plant[Plant] = \"1001\" )",
              "            )",
              "        )",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Material Cost and Price"
          },
          {
            "name": "Frozen Sales Forecast Quantity",
            "description": [
              "Total planned quantity of planned independent requirements with the change date (by material, plant, and delivery date) closest and before the material's [Commercialization Date] in the material master.",
              "",
              "If not available, the first one available after the [Commercialization Date] is used.",
              "",
              "This forecast is static."
            ],
            "expression": [
              "",
              "VAR _BeforeLaunch =",
              "    SUMX(",
              "        VALUES( 'Planned Independent Requirement'[MaterialPlantDateKey] ),",
              "        CALCULATE(",
              "            SUM( 'Planned Independent Requirement'[Planned Quantity] ),",
              "            KEEPFILTERS( 'Planned Independent Requirement'[RankFrozen] = -1 )",
              "        )",
              "    )",
              "",
              "VAR _AfterLaunch =",
              "    SUMX(",
              "        VALUES( 'Planned Independent Requirement'[MaterialPlantDateKey] ),",
              "        CALCULATE(",
              "            SUM( 'Planned Independent Requirement'[Planned Quantity] ),",
              "            KEEPFILTERS( 'Planned Independent Requirement'[RankFrozen] = 1 )",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF(",
              "    ISBLANK( _BeforeLaunch ),",
              "    _AfterLaunch,",
              "    _BeforeLaunch",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Sales Forecast"
          },
          {
            "name": "Flash Sales Forecast Quantity",
            "description": [
              "Total planned quantity of planned independent requirements with the latest change date by material, plant, and delivery date.",
              "",
              "This forecast value changes every month."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Planned Independent Requirement'[Planned Quantity] ),",
              "    KEEPFILTERS( 'Planned Independent Requirement'[RankNewest] = 1 )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Sales Forecast"
          },
          {
            "name": "Pending Specification Count",
            "description": "Count of specification documents that are submitted but not yet active, published, approved, nor declined by approvers",
            "expression": "SUM( 'Pending Specification'[PendingCount] )",
            "formatString": "#,0",
            "displayFolder": "EDOC"
          },
          {
            "name": "TRIR Target",
            "description": "This monthly target data is supplied by Robert Nicholson and uploaded by IT",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"TRIR\",",
              "        TREATAS(",
              "            VALUES( 'Company Code'[Company Code] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.00,",
              "        CALCULATE(",
              "            CALCULATE( TargetVal, DATESYTD( 'Calendar'[Date], \"09/30\" ) ),",
              "            FILTER( 'Calendar', 'Calendar'[Date] <= [Report Date] )",
              "        )",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Safety"
          },
          {
            "name": "TRIR Projected",
            "description": "Projected TRIR based on the fiscal year-to-date work hours and last fiscal year's work hours",
            "expression": [
              "",
              "DIVIDE(",
              "    [Safety Incident Count FYTD] * 200000,",
              "    CALCULATE(",
              "        SUM( 'Safety Work Hours'[Work Hours Projected] ),",
              "        DATESYTD( 'Calendar'[Date], \"09/30\" )",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Safety"
          },
          {
            "name": "Perfect Safety Days",
            "description": "Number of days where there is no incident marked as 'NO' under 'Safety Incidents'[Is Perfect Day]",
            "expression": [
              "",
              "VAR AllDays =",
              "COUNTROWS(",
              "    FILTER('Calendar', 'Calendar'[Date] <= [Report Date])",
              ")",
              "",
              "VAR NonPerfectDays =",
              "    COUNTROWS(",
              "        FILTER(",
              "            SUMMARIZE(",
              "                FILTER('Calendar', 'Calendar'[Date] <= [Report Date]),",
              "                'Calendar'[Date],",
              "                \"SCORE\", COUNTROWS( FILTER( 'Safety Incident Log', NOT('Safety Incident Log'[Perfect Day Flag] = \"Yes\") ) ) + 0",
              "            ),",
              "            [SCORE] <> 0",
              "        )",
              "    )",
              "",
              "RETURN",
              "AllDays - NonPerfectDays"
            ],
            "formatString": "#,0 \"days\"",
            "displayFolder": "Safety"
          },
          {
            "name": "Last Safety Incident Date",
            "description": "Date of the last incident recorded in the Safety Incidents Log",
            "expression": [
              "VAR _lastDate =",
              "    CALCULATE(",
              "        MAX( 'Safety Incident Log'[Incident Date] ),",
              "        FILTER(",
              "            ALL( 'Calendar'[Date] ),",
              "            'Calendar'[Date] <= [Report Date]",
              "        )",
              "    ) + TIME(0, 0, 0)",
              "",
              "RETURN",
              "IF(",
              "    _lastDate = DATE(1899, 12, 30),",
              "    BLANK(),",
              "    FORMAT(_lastDate, \"mm/dd/yyyy\")",
              ")"
            ],
            "displayFolder": "Safety"
          },
          {
            "name": "Days Since Last Safety Incident",
            "description": "Number of days between between last safety incident and yesterday (or whatever is the latest date in the 'Calendar'[Date] filter)",
            "expression": "DATEDIFF(DATEVALUE([Last Safety Incident Date]), [Report Date], DAY)",
            "formatString": "#,0 \"days\"",
            "displayFolder": "Safety"
          },
          {
            "name": "TRIR FYTD",
            "description": [
              "Product of number of incidents and 200000 divided by total work hours. The number of incidents and work hours are the total since October.",
              "",
              "Filter the 'Safety Incidents Log' table to measure the TRIR of certain types of incidents."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    DIVIDE(",
              "        [Safety Incident Count FYTD] * 200000,",
              "        [Safety Work Hours FYTD]",
              "    ),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Report Date]",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Safety"
          },
          {
            "name": "TRIR FYTD PY",
            "description": "Last fiscal year's TRIR (see TRIR measure for the description)",
            "expression": [
              "",
              "VAR incidents =",
              "    CALCULATE(",
              "        CALCULATE(",
              "            [Safety Incident Count],",
              "            FILTER(",
              "                'Calendar',",
              "                'Calendar'[Date] <= [End of Fiscal Year]",
              "            )",
              "        ),",
              "        SAMEPERIODLASTYEAR( DATESYTD( 'Calendar'[Date], \"09/30\" ) )",
              "    )",
              "",
              "VAR hours =",
              "    CALCULATE(",
              "        CALCULATE(",
              "            SUM( 'Safety Work Hours'[Work Hours] ),",
              "            FILTER(",
              "                'Calendar',",
              "                'Calendar'[Date] <= [End of Fiscal Year]",
              "            )",
              "        ),",
              "        SAMEPERIODLASTYEAR( DATESYTD( 'Calendar'[Date], \"09/30\" ) )",
              "    )",
              "",
              "RETURN",
              "DIVIDE(",
              "    incidents * 200000,",
              "    hours",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Safety"
          },
          {
            "name": "Safety Work Hours PY",
            "expression": [
              "",
              "VAR MinVisibleDate = MIN ( 'Calendar'[Date] )",
              "VAR LastDateWithHours = CALCULATE( MAX( 'Safety Work Hours'[Date] ), REMOVEFILTERS() )",
              "RETURN",
              "    IF(",
              "        MinVisibleDate > LastDateWithHours,",
              "        CALCULATE(",
              "            SUM( 'Safety Work Hours'[Work Hours] ),",
              "            SAMEPERIODLASTYEAR( 'Calendar'[Date] )",
              "        ),",
              "        0",
              "    )"
            ],
            "formatString": "#,0",
            "isHidden": true,
            "displayFolder": "Safety"
          },
          {
            "name": "Safety Work Hours FYTD PY",
            "expression": "CALCULATE( [Safety Work Hours PY], DATESYTD('Calendar'[Date], \"09/30\"))",
            "formatString": "#,0",
            "isHidden": true,
            "displayFolder": "Safety"
          },
          {
            "name": "Safety Work Hours FYTD",
            "description": "Total work hours since October. This data is supplied by HR and entered by Robert Nicholson in the Safety Incident Log spreadsheet.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Safety Work Hours'[Work Hours] ),",
              "    DATESYTD( 'Calendar'[Date], \"09/30\" )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Safety"
          },
          {
            "name": "Safety Incident Count FYTD",
            "description": "Fiscal year-to-date count of incidents recorded in the Safety Incidents Log",
            "expression": [
              "",
              "CALCULATE(",
              "    CALCULATE(",
              "        [Safety Incident Count],",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] <= [End of Fiscal Year]",
              "        )",
              "    ),",
              "    DATESYTD( 'Calendar'[Date], \"09/30\" )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Safety"
          },
          {
            "name": "Was New Product Gross Sales",
            "description": [
              "Total revenue amount of new products at the time of the invoice posting date. A material is a new product if the invoice is posted within the next 36 months after its Commercialization Date according to the material master.",
              "",
              "This value is toggleable to local currency using the \"_Currency To Use\" table."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    [Gross Sales],",
              "    KEEPFILTERS( 'Sales Invoice'[Was New Product Flag] = \"Yes\" )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Was New Product Gross Sales %Grand",
            "description": [
              "Total revenue amount of new products at the time of the invoice posting date. A material is a new product if the invoice is posted within the next 36 months after its Commercialization Date according to the material master.",
              "",
              "This value is toggleable to local currency using the \"_Currency To Use\" table."
            ],
            "expression": [
              "",
              "DIVIDE(",
              "    [Was New Product Gross Sales],",
              "    [Gross Sales]",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "COMP Efficiency",
            "description": "Projected direct hours divided by the difference between actual hours and indirect actual hours",
            "expression": [
              "",
              "DIVIDE(",
              "    [COMP Projected Direct Hours],",
              "    [COMP Actual Hours] - [COMP Indirect Actual Hours]",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Efficiency Target",
            "description": "Monthly target values uploaded to the Power BI data warehouse. User sends the target data to IT personnel and uploads it in ARLPBIPRD.ARLPBI_STAGE SQL database in 'zTARGETS_KPI' table; KpiKey = \"Eff_COMP\"",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"Eff_COMP\",",
              "        TREATAS(",
              "            VALUES( 'Company Code'[Company Code] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.90,",
              "        TargetVal",
              "    )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Efficiency Target Var",
            "description": "Difference between the compounding efficiency and the corresponding monthly target",
            "expression": "[COMP Efficiency] - [COMP Efficiency Target]",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP First Pass Reliability Target",
            "description": "Monthly target values uploaded to the Power BI data warehouse. User sends the target data to IT personnel and uploads it in ARLPBIPRD.ARLPBI_STAGE SQL database in 'zTARGETS_KPI' table; KpiKey = \"FPass_COMP\"",
            "expression": [
              "",
              "VAR TargetVal = ",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"FPass_COMP\",",
              "        TREATAS(",
              "            VALUES( 'Company Code'[Company Code] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )    ",
              "RETURN",
              "    IF(",
              "        ISBLANK(TargetVal),",
              "        0.95,",
              "        TargetVal",
              "    )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Labor Efficiency Target Var",
            "description": "Difference between the labor efficiency and the corresponding monthly target. The target is the same as the Compounding Efficiency target.",
            "expression": "[COMP Labor Efficiency] - [COMP Efficiency Target]",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP First Pass Reliability Target Var",
            "description": "Difference between the first pass reliability and the corresponding monthly target",
            "expression": "[COMP First Pass Reliability] - [COMP First Pass Reliability Target]",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Compounding"
          },
          {
            "name": "CONV Performance",
            "description": [
              "Total footage divided by the ideal footage of production runs.",
              "",
              "Footage is based on the footage recorded in the runtime log. The ideal footage based on the spec linespeed of the material assigned in the linespeed table in the Converting's Access database.",
              ""
            ],
            "expression": [
              "",
              "DIVIDE(",
              "    [CONV Actual Footage],",
              "    [CONV Ideal Footage]",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Converting\\_OEE"
          },
          {
            "name": "CONV Quality",
            "description": [
              "Net of goods receipt and scrap footage form goods movement transactions divided by the goods receipt footage.",
              "",
              "Footage is the quantity converted to linear footage using the length and width data from the material master."
            ],
            "expression": [
              "",
              "DIVIDE(",
              "    [CONV OEE Goods Receipt Footage] - [CONV OEE Scrap Footage],",
              "    [CONV OEE Goods Receipt Footage]",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Converting\\_OEE"
          },
          {
            "name": "CONV Efficiency",
            "description": "Product of availability, performance, and quality",
            "expression": "[CONV Availability] * [CONV Performance] * [CONV Quality]",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Converting\\_OEE"
          },
          {
            "name": "Frozen Labor Cost per Unit",
            "description": "Frozen labor cost divided by quantity sold",
            "expression": [
              "DIVIDE(",
              "    [Frozen Labor Cost],",
              "    [Quantity Sold]",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Cost in Sales Invoice\\_Per Unit"
          },
          {
            "name": "Frozen Material Cost per Unit",
            "description": "Frozen material cost divided by quantity sold",
            "expression": [
              "DIVIDE(",
              "    [Frozen Material Cost],",
              "    [Quantity Sold]",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Cost in Sales Invoice\\_Per Unit"
          },
          {
            "name": "Frozen Overhead Cost per Unit",
            "description": "Frozen overhead cost divided by quantity sold",
            "expression": [
              "DIVIDE(",
              "    [Frozen Overhead Cost],",
              "    [Quantity Sold]",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Cost in Sales Invoice\\_Per Unit"
          },
          {
            "name": "Frozen Setup Cost per Unit",
            "description": "Frozen setup cost divided by quantity sold",
            "expression": [
              "DIVIDE(",
              "    [Frozen Setup Cost],",
              "    [Quantity Sold]",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Cost in Sales Invoice\\_Per Unit"
          },
          {
            "name": "Frozen Standard Cost per Unit",
            "description": "Frozen standard cost divided by quantity sold",
            "expression": [
              "DIVIDE(",
              "    [Frozen Standard Cost],",
              "    [Quantity Sold]",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Cost in Sales Invoice\\_Per Unit"
          },
          {
            "name": "Invoice Selling Price",
            "description": "Gross sales divided by quantity sold of billing documents posted in SAP. This measure is essentially the average selling price of billing documents.",
            "expression": [
              "",
              "ABS(",
              "    DIVIDE(",
              "        [Gross Sales],",
              "        [Quantity Sold]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Invoice Selling Price MTD",
            "description": "MTD gross sales divided by quantity sold as of yesterday or whatever is the last date filtered in 'Calendar'[Date]",
            "expression": [
              "",
              "CALCULATE(",
              "    CALCULATE(",
              "        [Invoice Selling Price],",
              "        DATESMTD( 'Calendar'[Date] )",
              "    ),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Report Date]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Invoice\\_Time-Based"
          },
          {
            "name": "Selling Price",
            "description": [
              "Latest selling price by material and customer based on sales quote or price list. It is the selling price per one unit of measure.",
              "",
              "This value is in local currency by default but is toggleable to USD by filtering the '_Currency to Use' table. The sales quote and price list are maintained by regional Customer Service and Sales departments."
            ],
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", AVERAGE( 'Price List'[UnitPriceUSD] ),",
              "        \"Local\", AVERAGE( 'Price List'[UnitPrice] ),",
              "        AVERAGE( 'Price List'[UnitPrice] )",
              "    ),",
              "    AVERAGE( 'Price List'[UnitPrice] )    ",
              ")",
              ""
            ],
            "formatString": "#,0.00",
            "displayFolder": "Material Cost and Price"
          },
          {
            "name": "Skipped Planned Orders Duration",
            "description": [
              "Total duration of process orders scheduled between current production date and now.",
              "",
              "Skipped process orders have data in schedule files but none in machine data. In addition, an order is skipped if there is machine data on a schedule order after it."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Planning Schedule Comparison'[Plan Duration] ),",
              "    KEEPFILTERS( 'Planning Schedule Comparison'[StatusFlag] = 0 ), -- 0 is skipped",
              "    FILTER(",
              "        'Planning Schedule Comparison',",
              "        'Planning Schedule Comparison'[ProductionDate] >= [Report Production Date]",
              "    )",
              ")"
            ],
            "formatString": "#,0.0 \"hr\"",
            "displayFolder": "Planning Schedule"
          },
          {
            "name": "Planning Schedule Footage Variance",
            "description": "Variance between the actual and scheduled footage between start of current production date and now.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Planning Schedule Comparison'[VarQuantity] ),",
              "    KEEPFILTERS( 'Planning Schedule Comparison'[Plan Start Date] <> BLANK() ),",
              "    KEEPFILTERS( 'Planning Schedule Comparison'[StatusFlag] = 1 ) -- 1 is orders with schedule and machine data",
              ")",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Planning Schedule"
          },
          {
            "name": "Planning Schedule Status",
            "description": "Log error / typo (-1); Skipped S/O (0); Other (1)",
            "expression": "MAX( 'Planning Schedule Comparison'[StatusFlag] )",
            "formatString": "0",
            "displayFolder": "Planning Schedule"
          },
          {
            "name": "Planning Schedule Duration Variance",
            "description": "Variance between the actual and scheduled duration of process orders",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Planning Schedule Comparison'[VarDuration] ),",
              "    KEEPFILTERS( 'Planning Schedule Comparison'[Plan Start Date] <> BLANK() ),",
              "    KEEPFILTERS( 'Planning Schedule Comparison'[Actual Start Date] <> BLANK() )",
              ")"
            ],
            "formatString": "#,0.0",
            "displayFolder": "Planning Schedule"
          },
          {
            "name": "Earned Labor Budget",
            "description": "Total of the monthly earned labor entered in the ZBUDGET table in SAP.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( TARGET_Absorption[Labor] ),",
              "    TARGET_Absorption[ForecastType] = \"B\",",
              "    TREATAS(",
              "        VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "        TARGET_Absorption[CalendarTimeKey]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption"
          },
          {
            "name": "Earned Overhead Budget",
            "description": "Total of the monthly earned overhead budget entered in the ZBUDGET table in SAP.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( TARGET_Absorption[Overhead] ),",
              "    TARGET_Absorption[ForecastType] = \"B\",",
              "    TREATAS(",
              "        VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "        TARGET_Absorption[CalendarTimeKey]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption"
          },
          {
            "name": "Absorption Budget",
            "description": "Total of the monthly earned labor and overhead budget entered in the ZBUDGET table in SAP.",
            "expression": " [Earned Labor Budget] + [Earned Overhead Budget]",
            "formatString": "#,0",
            "displayFolder": "Absorption"
          },
          {
            "name": "Absorption per Output Last 3 Months",
            "description": "Total absorption posted in the last 3 calendar months divided by the total output of departments. Casting and coating output is from machine data, Converting output is GR transaction, and others are delivered quantity of shop orders that ended in the last 3 months.",
            "expression": [
              "",
              "VAR BegDate = EDATE([Beginning of Month], -3)",
              "VAR Upstream_Out = ",
              "    CALCULATE(",
              "        [Upstream Actual Production Footage],",
              "        ALL( 'Calendar' ),",
              "        FILTER(",
              "            ALL( 'Upstream Log Simplified'[Start Time] ),",
              "            'Upstream Log Simplified'[Start Time] >= BegDate",
              "                && 'Upstream Log Simplified'[Start Time] < [Beginning of Month]",
              "        )",
              "    )",
              "",
              "VAR Conv_Out = ",
              "    CALCULATE(",
              "        [CONV Linear Footage],",
              "        ALL( 'Calendar' ),",
              "        FILTER(",
              "            ALL( 'Goods Movement'[Posting Date] ),",
              "            'Goods Movement'[Posting Date] >= BegDate",
              "                && 'Goods Movement'[Posting Date] < [Beginning of Month]",
              "        )",
              "    )",
              "",
              "VAR Other_Out = ",
              "    CALCULATE(",
              "        SUM('Process Order'[DeliveredQuantity] ),",
              "        ALL( 'Calendar' ),",
              "        FILTER(",
              "            ALL( 'Process Order'[EndDateKey] ),",
              "            'Process Order'[EndDateKey] >= BegDate",
              "                && 'Process Order'[EndDateKey] < [Beginning of Month]",
              "        ),",
              "        'Process Order'[Close Flag] = \"Yes\"",
              "    )",
              "",
              "RETURN",
              "DIVIDE(",
              "    CALCULATE(",
              "        [Absorption],",
              "        FILTER(",
              "            ALL( 'Calendar'[Date] ),",
              "            'Calendar'[Date] >= BegDate",
              "                && 'Calendar'[Date] < [Beginning of Month]",
              "        )",
              "    ),",
              "    SWITCH(",
              "        SELECTEDVALUE( 'Department'[Department] ),",
              "        \"CONV\", Conv_Out,",
              "        \"CAST\", Upstream_Out,",
              "        \"COAT\", Upstream_Out,",
              "        Other_Out",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Absorption\\_Projection"
          },
          {
            "name": "Sales Order Amount Projected Month End",
            "description": "Average daily ordered amount multiplied by the total number of weekdays in the month",
            "expression": [
              "",
              "VAR _MinVisibleDate = MIN('Calendar'[Date])",
              "",
              "RETURN",
              "IF(",
              "    _MinVisibleDate >= [Beginning of Month]",
              "        && _MinVisibleDate <= [End of Month],",
              "    [Sales Order Amount Daily Entry Rate MTD] * [Weekdays Count],",
              "    IF(",
              "        _MinVisibleDate > [End of Month],",
              "        [Sales Order Amount Daily Entry Rate] * [Weekdays Count]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Weekdays Count MTD",
            "description": "Number of MTD weekdays in a month",
            "expression": [
              "",
              "CALCULATE(",
              "    CALCULATE(",
              "        COUNTROWS( 'Calendar' ),",
              "        KEEPFILTERS( 'Calendar'[Is Weekday] ),",
              "        DATESMTD( 'Calendar'[Date] )",
              "    ),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Report Date]",
              "    )    ",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "_Dates"
          },
          {
            "name": "Sales Order Amount Daily Entry Rate",
            "description": [
              "MTD ordered value of sales orders divided by MTD number of weekdays.",
              "",
              "For future months, the daily order entry average amount between the beginning and end of last month is taken and multiplied by the assigned monthly index. The monthly index is manually assigned per sales region."
            ],
            "expression": [
              "",
              "VAR _YtdAvg =",
              "    CALCULATE(",
              "        AVERAGEX(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            [Sales Order Amount Daily Entry Rate Hidden]",
              "        ),",
              "        FILTER(",
              "            ALL( 'Calendar' ),",
              "            'Calendar'[Date] >= [Beginning of Fiscal Year]",
              "                && 'Calendar'[Date] < [Beginning of Month]",
              "        )",
              "    )",
              "",
              "VAR _Index =",
              "    CALCULATE(",
              "        AVERAGE( TARGET_SalesOrder[MonthlyIndex] ),",
              "        TREATAS(",
              "            VALUES( 'Customer Master'[Sales Region] ),",
              "            TARGET_SalesOrder[SalesRegionKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_SalesOrder[CalendarTimeKey]",
              "        )",
              "    )",
              "",
              "RETURN",
              "    IF(",
              "        [Sales Order Amount Daily Entry Rate Hidden] <> BLANK(),",
              "        [Sales Order Amount Daily Entry Rate Hidden],",
              "        _YtdAvg * _Index",
              "    )"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Sales Order Amount Daily Entry Rate MTD",
            "description": [
              "MTD ordered value of sales orders divided by MTD number of weekdays.",
              "",
              "Use this measure if none of the fields in 'Calendar' table are used in the visual."
            ],
            "expression": [
              "",
              "DIVIDE(",
              "    [Sales Order Amount MTD],",
              "    [Weekdays Count MTD]",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Weekdays Count Current Month End",
            "description": "Number of weekdays in the entire month of the current month",
            "expression": [
              "",
              "CALCULATE(",
              "    COUNTROWS( 'Calendar' ),",
              "    FILTER(",
              "        ALL( 'Calendar' ),",
              "        'Calendar'[Date] >= [Beginning of Month]",
              "            && 'Calendar'[Date] <= [End of Month]",
              "            && 'Calendar'[Is Weekday]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "_Dates"
          },
          {
            "name": "Sales Order Amount Projected Current Month End",
            "description": [
              "Average daily ordered amount multiplied by the total number of weekdays in the month.",
              "",
              "Use this measure if none of the fields in 'Calendar' table are used in the visual."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    [Sales Order Amount Daily Entry Rate MTD],",
              "    FILTER(",
              "        ALL( 'Calendar'[Date] ),",
              "        'Calendar'[Date] >= [Beginning of Month]",
              "            && 'Calendar'[Date] <= [Report Date]",
              "    )",
              ") * [Weekdays Count Current Month End]"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Weekdays Count",
            "description": "Number of weekdays",
            "expression": [
              "",
              "CALCULATE(",
              "    COUNTROWS( 'Calendar' ),",
              "    FILTER(",
              "        ALL( 'Calendar'[Is Weekday] ),",
              "        'Calendar'[Is Weekday]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "_Dates"
          },
          {
            "name": "Weekdays in Month",
            "description": "Number of weekdays in the whole month",
            "expression": [
              "",
              "CALCULATE(",
              "    COUNTROWS( 'Calendar' ),",
              "    FILTER(",
              "        ALL( 'Calendar' ),",
              "        'Calendar'[Is Weekday]",
              "    ),",
              "    VALUES( 'Calendar'[Year_Month_Sort_Key] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "_Dates"
          },
          {
            "name": "Sales Order Amount Daily Entry Rate Hidden",
            "description": [
              "MTD ordered value of sales orders divided by MTD number of weekdays.",
              "",
              "For future months, the daily order entry average amount between the beginning and end of last month is taken and multiplied by the assigned monthly index. The monthly index is manually assigned per sales region."
            ],
            "expression": [
              "",
              "DIVIDE(",
              "    [Sales Order Ordered Amount],",
              "    [Weekdays Count]",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Sales Order Amount Regional Budget",
            "description": "Planned order entry amount per month and sales region",
            "expression": [
              " ",
              "CALCULATE(",
              "    SUM( TARGET_SalesOrder[OrderEntryGoal] ),",
              "    TREATAS(",
              "        VALUES( 'Customer Master'[Sales Region] ),",
              "        TARGET_SalesOrder[SalesRegionKey]",
              "    ),",
              "    TREATAS(",
              "        VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "        TARGET_SalesOrder[CalendarTimeKey]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Sales Order Amount Regional Budget MTD",
            "description": [
              "Planned order entry amount per month and sales region.",
              "",
              "Use this measure if none of the fields in 'Calendar' table are used in the visual."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    CALCULATE(",
              "        [Sales Order Amount Regional Budget],",
              "        DATESMTD( 'Calendar'[Date] )",
              "    ),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Report Date]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Sales Order Amount Projected Regional Budget Var",
            "description": "Variance between current month end's sales order entry projection and budget",
            "expression": [
              "",
              "IF(",
              "    ISBLANK( [Sales Order Amount Regional Budget MTD] ),",
              "    BLANK(),",
              "    [Sales Order Amount Projected Current Month End] - [Sales Order Amount Regional Budget MTD]",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Sales Order Amount Daily Entry Rate Regional Budget",
            "description": "Monthly budgeted order entry amount divided by the total number of weekdays of the corresponding month",
            "expression": [
              "",
              "AVERAGEX(",
              "    VALUES('Calendar'[Date]),",
              "    DIVIDE(",
              "        [Sales Order Amount Regional Budget],",
              "        [Weekdays in Month]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Sales Order Amount Regional Budget Adjusted",
            "description": "Adjusted budget to meet the fiscal year-end goal if the previous months of the year didn't meet the goal",
            "expression": [
              " ",
              "VAR _MinVisibleDate = MIN( 'Calendar'[Date] )",
              "",
              "VAR _sales =",
              "    CALCULATE(",
              "        [Gross Sales],",
              "        FILTER(",
              "            ALL( 'Calendar' ),",
              "            'Calendar'[Date] >= [Beginning of Fiscal Year]",
              "                && 'Calendar'[Date] <= [End of Prior Month]",
              "        )",
              "    )",
              "",
              "VAR _goal =",
              "    CALCULATE(",
              "        [Sales Order Amount Regional Budget],",
              "        FILTER(",
              "            ALL( 'Calendar' ),",
              "            'Calendar'[Date] >= [Beginning of Fiscal Year]",
              "                && 'Calendar'[Date] <= [End of Month]",
              "        )",
              "    )",
              "",
              "VAR _currentMonthProj = [Sales Order Amount Projected Current Month End]",
              "",
              "VAR _currentPeriod = MONTH( [Report Date] ) - IF( MONTH( [Report Date] ) >= 10, 9, -3 )",
              "",
              "VAR _remainMonthsInFY =",
              "--- Remaining months of FY excluding next month",
              "--- Per LBABILO, some orders taken this month gets shipped next month",
              "    IF(",
              "        _currentPeriod >= 8,",
              "        0,",
              "        12 - _currentPeriod + 1",
              "    )",
              "",
              "VAR _extraNeeded = DIVIDE( ( _goal - _sales - _currentMonthProj ), _remainMonthsInFY )",
              "",
              "RETURN",
              "IF(",
              "    _MinVisibleDate > [End of Month],",
              "    [Sales Order Amount Regional Budget] + _extraNeeded",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Sales Order Amount Daily Entry Rate Regional Budget Adjusted",
            "description": "Monthly budgeted and adjusted order entry amount divided by the total number of weekdays of the corresponding month",
            "expression": [
              "",
              "AVERAGEX(",
              "    VALUES('Calendar'[Date]),",
              "    DIVIDE(",
              "        [Sales Order Amount Regional Budget Adjusted],",
              "        [Weekdays in Month]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Sales Order Amount Projected Regional Budget Var%",
            "description": "Variance in percentage between current month end's sales order entry projection and budget",
            "expression": [
              "",
              "DIVIDE(",
              "    [Sales Order Amount Projected Regional Budget Var],",
              "    [Sales Order Amount Regional Budget MTD]",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Sales Order Amount FYTD Regional Budget Var",
            "description": "FYTD gross sales up to last month plus current month end projection of sales order entry minus FYTD sales order entry budget",
            "expression": [
              "--- FYTD gross sales up to last month plus current month end projection of sales order entry minus FYTD sales order entry budget",
              "(",
              "    [Gross Sales FYTD]",
              "    - CALCULATE(",
              "        [Gross Sales MTD],",
              "        FILTER( 'Calendar', 'Calendar'[Date] <= [Report Date] )",
              "    ) + [Sales Order Amount Projected Current Month End]",
              ") - CALCULATE(",
              "    CALCULATE(",
              "        [Sales Order Amount Regional Budget],",
              "        DATESYTD( 'Calendar'[Date], \"09/30\" )",
              "    ),",
              "    FILTER( 'Calendar', 'Calendar'[Date] <= [Report Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Sales Order Amount FY End Regional Budget Var",
            "description": "FYTD gross sales up to last month plus current and future month end projections of sales order entry minus total FY sales order entry budget",
            "expression": [
              "--- FYTD gross sales up to last month plus current and future month end projections of sales order entry minus total FY sales order entry budget",
              "VAR _RestOfYearProj =",
              "    CALCULATE(",
              "        SUMX(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            [Sales Order Amount Projected Month End]",
              "        ),",
              "        FILTER(",
              "            ALL( 'Calendar' ),",
              "            'Calendar'[Date] >= [Beginning of Month]",
              "                && 'Calendar'[Date] <= [End of Fiscal Year]",
              "        )",
              "    )",
              "",
              "VAR _FYTDSales =",
              "--- FYTD gross sales up to last month",
              "    CALCULATE(",
              "        [Gross Sales FYTD],",
              "        FILTER(",
              "            ALL( 'Calendar' ),",
              "            'Calendar'[Date] >= [Beginning of Fiscal Year]",
              "                && 'Calendar'[Date] < [Beginning of Month]",
              "        )",
              "    )",
              "",
              "VAR _YearBudget =",
              "--- Total year sales order entry budget",
              "    CALCULATE(",
              "        [Sales Order Amount Regional Budget],",
              "        FILTER(",
              "            ALL( 'Calendar' ),",
              "            'Calendar'[Date] >= [Beginning of Fiscal Year]",
              "                && 'Calendar'[Date] <= [End of Fiscal Year]",
              "        )",
              "    )",
              "",
              "RETURN",
              "_FYTDSales + _RestOfYearProj - _YearBudget"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Order Entry Analysis"
          },
          {
            "name": "Gross Margin%",
            "description": "Gross margin divided by gross sales. Gross margin is the difference between gross sales and standard cost in the billing document.",
            "expression": " DIVIDE([Gross Margin], [Gross Sales])",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Complaint Amount",
            "description": "Total amount in the complaint (in USD)",
            "expression": " SUM('Complaint'[ComplaintValueUSD])",
            "formatString": "#,0",
            "displayFolder": "Complaints"
          },
          {
            "name": "Complaint Credit Issued Amount",
            "description": "Total amount credited to the customer in the complaint",
            "expression": " SUM('Complaint'[CreditIssueValueUSD])",
            "formatString": "#,0",
            "displayFolder": "Complaints"
          },
          {
            "name": "Complaint Open Amount",
            "description": "Difference between the complaint amount and credit issued in the complaint",
            "expression": " SUM('Complaint'[OpenValueUSD])",
            "formatString": "#,0",
            "displayFolder": "Complaints"
          },
          {
            "name": "Product Quality Complaint Amount",
            "description": "Total amount (in USD) of complaints with reason code marked as 'Q' (quality).",
            "expression": [
              "",
              "CALCULATE(",
              "    [Complaint Amount],",
              "    KEEPFILTERS( 'Complaint'[Department] = \"Q\" ),",
              "    KEEPFILTERS( LEFT('Complaint'[Reason Code], 8) <> \"WARRANTY\" )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Complaints\\_Product Quality"
          },
          {
            "name": "Product Quality Credit Issued",
            "description": [
              "Total credit issue amount of service quality complaints divided by the gross sales. For example , in Jan 2023, this value is the total credit amount issued in Jan 2023 divided by the gross sales in Jan 2023.",
              "",
              "Service quality are complaints entered with reason codes marked as 'S' (service).",
              ""
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    [Complaint Credit Issued Amount],",
              "    KEEPFILTERS( 'Complaint'[Department] = \"Q\" ),",
              "    KEEPFILTERS( LEFT('Complaint'[Reason Code], 8) <> \"WARRANTY\" )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Complaints\\_Product Quality"
          },
          {
            "name": "Product Quality Open Amount",
            "description": [
              "Difference between the complaint and credit issue amount of product quality complaints.",
              "",
              "Product quality complaints are complaints entered with reason codes marked as 'Q' (quality).",
              ""
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    [Complaint Open Amount],",
              "    KEEPFILTERS( 'Complaint'[Department] = \"Q\" ),",
              "    KEEPFILTERS( LEFT('Complaint'[Reason Code], 8) <> \"WARRANTY\")",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Complaints\\_Product Quality"
          },
          {
            "name": "Product Quality Index Credited",
            "description": [
              "Total credit issue amount of product quality complaints entered divided by the gross sales. For example, in Jan 2023, this value is the total credit issue amount of complaints entered in Jan 2023 divided by the gross sales in Jan 2023.",
              "",
              "Product quality complaints are complaints entered with reason codes marked as 'Q' (quality)."
            ],
            "expression": " DIVIDE( [Product Quality Credit Issued], [Gross Sales] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Complaints\\_Product Quality"
          },
          {
            "name": "Product Quality Index Open",
            "description": [
              "Total open amount of product quality complaints entered divided by the gross sales. For example, in Jan 2023, this value is the total open amount of complaints entered in Jan 2023 divided by the gross sales in Jan 2023.",
              "",
              "Product quality complaints are complaints entered with reason codes marked as 'Q' (quality). Open amount is the difference between the complaint and credit issued amount."
            ],
            "expression": " DIVIDE( [Product Quality Open Amount], [Gross Sales] )",
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Complaints\\_Product Quality"
          },
          {
            "name": "Hold Tag Amount",
            "description": "Total standard cost of materials in the hold tag",
            "expression": " SUM('Hold Tag'[Amount])",
            "formatString": "#,0",
            "displayFolder": "Hold Tag"
          },
          {
            "name": "Hold Tag Quantity",
            "description": "Total quantity of materials in the hold tag",
            "expression": " SUM('Hold Tag'[Quantity])",
            "formatString": "#,0.00",
            "displayFolder": "Hold Tag"
          },
          {
            "name": "S-Block Amount",
            "description": "Total standard cost of inventory on S block",
            "expression": " SUM('Blocked Inventory'[SBlock Amount])",
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Blocked"
          },
          {
            "name": "Q-Block Amount",
            "description": "Total standard cost of inventory on Q block",
            "expression": " SUM('Blocked Inventory'[QBlock Amount])",
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Blocked"
          },
          {
            "name": "Blocked Inventory Amount",
            "description": "Total standard cost of materials on S block, Q block, and expired as of yesterday",
            "expression": " [S-Block Amount] + [Q-Block Amount] + [Expired Amount]",
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Blocked"
          },
          {
            "name": "Expired Amount",
            "description": "Total standard cost of inventory with expiration/SLED date on or before yesterday.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Expired Inventory'[Stock Amount] ),",
              "    FILTER(",
              "        ALL( 'Expired Inventory'[Expiration Date] ),",
              "        'Expired Inventory'[Expiration Date] <= [Report Date]",
              "    )",
              ")",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Blocked\\_Expired"
          },
          {
            "name": "Product Quality Index Target",
            "description": "Monthly target values supplied by Quality Assurance department to the IT department annually",
            "expression": [
              "",
              "CALCULATE(",
              "    AVERAGE( TARGET_KPI[TargetValue] ),",
              "    TARGET_KPI[KpiKey] = \"PQI\",",
              "    TREATAS(",
              "        VALUES( 'Customer Master'[Sales Organization] ),",
              "        TARGET_KPI[LocationKey]",
              "    ),",
              "    TREATAS(",
              "        VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "        TARGET_KPI[CalendarTimeKey]",
              "    )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Complaints\\_Product Quality"
          },
          {
            "name": "Product Quality Complaint Cycle Time",
            "description": [
              "Average number of days between the complaint creation date and latest date when a credit is issued.",
              "",
              "This measure only applies to product quality complaints which are complaints with reason code marked as 'Q' (quality)."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    AVERAGE( 'Complaint'[CycleTime] ),",
              "    KEEPFILTERS( 'Complaint'[Department] = \"Q\" ),",
              "    KEEPFILTERS( LEFT('Complaint'[Reason Code], 8) <> \"WARRANTY\" ),",
              "    USERELATIONSHIP('Calendar'[Date], 'Complaint'[Last Credit Date])",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Complaints\\_Product Quality"
          },
          {
            "name": "Service Quality Complaint Amount",
            "description": [
              "Total credit issue amount for service quality complaints.",
              "",
              "Service quality are complaints entered with reason codes marked as 'S' (service)."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    [Complaint Credit Issued Amount],",
              "    KEEPFILTERS( 'Complaint'[Billing Block Indicator] = \"\" ),",
              "    KEEPFILTERS( 'Complaint'[Department] = \"S\" ),",
              "    USERELATIONSHIP('Calendar'[Date], 'Complaint'[Last Credit Date])",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Complaints\\_Service Quality"
          },
          {
            "name": "Customer Accommodation Complaint Amount",
            "description": [
              "Total credit issue amount for customer accommodation complaints.",
              "",
              "Customer accommodation are complaints entered with reason codes marked as 'S1' (accommodation)."
            ],
            "expression": [
              "",

        "measures": [
          {
            "name": "Gross Sales",
            "description": "(SAP Source: Sum of CE110US.ERLOS)",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice Documents'[Gross Sales Amount] ),",
              "        \"Local\", SUMX( 'Sales Invoice Documents', 'Sales Invoice Documents'[Gross Sales Amount] * 'Sales Invoice Documents'[Exchange Rate] ),",
              "        SUM('Sales Invoice Documents'[Gross Sales Amount] )",
              "    ),",
              "    SUM('Sales Invoice Documents'[Gross Sales Amount] )    ",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales"
          },
          {
            "name": "Net Sales",
            "description": "Net of gross sales and discounts; Date grouping: 'Sales Invoice Documents'[Posting Date]",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice Documents'[Net Sales Amount] ),",
              "        \"Local\", SUMX( 'Sales Invoice Documents',  'Sales Invoice Documents'[Net Sales Amount] * 'Sales Invoice Documents'[Exchange Rate] ),",

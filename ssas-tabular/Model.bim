          {
            "name": "CONV OEE Scrap Footage",
            "description": "Total linear footage of goods issue or scrap transactions per process order from SAP that has a match in the Converting runtime log and Converting OEE AAS table.",
            "expression": "SUM( 'Converting OEE'[ScrapFootage] )",
            "formatString": "#,0",
            "displayFolder": "Converting\\_OEE"
          },
          {
            "name": "Frozen Additive Cost",
            "description": "Total additive cost of billing documents. Cost is based on the cost roll at the pricing date of the billing document.",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice'[AdditiveCost] ),",
              "        \"Local\", SUMX( 'Sales Invoice', 'Sales Invoice'[AdditiveCost] * 'Sales Invoice'[Exchange Rate] ),",
              "        SUM( 'Sales Invoice'[AdditiveCost] )",
              "    ),",
              "    SUM( 'Sales Invoice'[AdditiveCost] ) ",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Cost in Sales Invoice"
          },
          {
            "name": "Official Additive Cost",
            "description": "This cost is the setup cost in the latest cost roll. It is multipled by the quantity sold in the billing document.",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice'[CurrentAdditiveCost] ),",
              "        \"Local\", SUMX( 'Sales Invoice', 'Sales Invoice'[CurrentAdditiveCost] * 'Sales Invoice'[Exchange Rate] ),",
              "        SUM( 'Sales Invoice'[CurrentAdditiveCost] )",
              "    ),",
              "    SUM( 'Sales Invoice'[CurrentAdditiveCost] ) ",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Cost in Sales Invoice"
          },
          {
            "name": "Additive Cost",
            "description": "Latest official additive cost of the material. This measure is best used if analyzed by 'Material Master' and/or 'Plant'. Also If this is analyzed by 'Calendar', this measure shows the last cost within a date range.",
            "expression": [
              "VAR _isMaterialFiltered = HASONEFILTER( 'Material Costing History'[Material Number] ) || ISFILTERED( 'Material Master' )",
              "VAR _isPlantFiltered = HASONEFILTER( 'Material Costing History'[Plant] ) || ISFILTERED( 'Plant' )",
              "VAR _maxVisibleDate = ",
              "    CALCULATE(",
              "        MAX( 'Calendar'[Date] ),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "VAR _latestRank =",
              "    CALCULATE(",
              "        IF(",
              "            _isMaterialFiltered && _isPlantFiltered,",
              "            MIN( 'Material Costing History'[LatestRank] ),",
              "            IF(",
              "                _isMaterialFiltered,",
              "                CALCULATE(",
              "                    MIN( 'Material Costing History'[LatestRank] ),",
              "                    KEEPFILTERS( 'Plant'[Plant] = \"1001\" )",
              "                ),",
              "                BLANK()",
              "            )",
              "        ),",
              "        FILTER(",
              "            'Material Costing History',",
              "            'Material Costing History'[Costing Start Date] <= _maxVisibleDate",
              "        )",
              "    )",
              "",
              "RETURN",
              "    IF(",
              "        ISBLANK(_latestRank),",
              "        BLANK(),",
              "        SWITCH(",
              "            TRUE(),",
              "            HASONEVALUE( Plant[Plant] ),",
              "                CALCULATE(",
              "                    AVERAGE( 'Material Costing History'[Additive Cost] ),",
              "                    KEEPFILTERS( 'Material Costing History'[LatestRank] = _latestRank )",
              "                ),",
              "            CALCULATE(",
              "                AVERAGE( 'Material Costing History'[Additive Cost] ),",
              "                KEEPFILTERS( 'Material Costing History'[LatestRank] = _latestRank ),",
              "                KEEPFILTERS( Plant[Plant] = \"1001\" )",
              "            )",
              "        )",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Material Cost and Price"
          },
          {
            "name": "Official Additive Cost per Unit",
            "description": "Official additive cost divided by quantity sold",
            "expression": [
              "DIVIDE(",
              "    [Official Additive Cost],",
              "    [Quantity Sold]",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Cost in Sales Invoice\\_Per Unit"
          },
          {
            "name": "Frozen Additive Cost per Unit",
            "description": "Frozen setup cost divided by quantity sold",
            "expression": [
              "DIVIDE(",
              "    [Frozen Additive Cost],",
              "    [Quantity Sold]",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Cost in Sales Invoice\\_Per Unit"
          },
          {
            "name": "Inventory Additive Cost",
            "description": "Product of unrestricted stock quantity and current additive cost per unit",
            "expression": "SUM( 'Inventory'[Additive Cost (USD)] )",
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Cost of Inventory"
          },
          {
            "name": "Inventory Snapshot Additive Cost",
            "description": "Latest snapshot of the additive cost of inventory depending on the date filter/context. If there is no date filter/context, then this value is yesterday's snapshot.",
            "expression": [
              "",
              "VAR RefDate =",
              "    CALCULATE(",
              "        MAX('Calendar'[Date]),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "    CALCULATE(",
              "        SUM( 'Inventory Snapshots'[Additive Cost] ),",
              "        KEEPFILTERS( 'Calendar'[Date] = RefDate )",
              "    )"
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Time-Based"
          },
          {
            "name": "Planning Schedule Scheduled Hours",
            "description": "Total scheduled duration (in hours) of scheduled process orders",
            "expression": "SUM( 'Planning Schedule Comparison'[Plan Duration] )",
            "formatString": "#,0.0",
            "displayFolder": "Planning Schedule"
          },
          {
            "name": "Planning Schedule Actual Hours",
            "description": "Total actual duration (in hours) of scheduled process orders",
            "expression": "SUM( 'Planning Schedule Comparison'[Actual Duration] )",
            "formatString": "#,0.0",
            "displayFolder": "Planning Schedule"
          },
          {
            "name": "Planning Schedule Gained Hours",
            "description": "Difference of scheduled and actual start timestamps of the latest order at the time of data refresh. Then, subtract the total hours of skipped process orders in the schedule.",
            "expression": [
              "",
              "CALCULATE(",
              "    DIVIDE( SUM( 'Planning Schedule Comparison'[VarStartInMins] ), 60 ),",
              "    KEEPFILTERS( 'Planning Schedule Comparison'[LatestFlag] = \"Yes\" ),",
              "    FILTER(",
              "        ALL( 'Calendar' ),",
              "        'Calendar'[Date] >= [Report Date]",
              "    )",
              ") - [Skipped Planned Orders Duration]"
            ],
            "formatString": "#,0.0 \"hr\"",
            "displayFolder": "Planning Schedule"
          },
          {
            "name": "Planning Schedule Comparison Data Integrity%",
            "description": "Percentage of which the production log has data (has start and stop datetimes) within a given timeframe. The closer to 100% the better but it is expected the number to be not exactly 100%. Anything above 100% indicates the log has overlapping records which means there is an error in the data logging. When the value is low, see the last datetime stamp recorded then ask operators for more details.",
            "expression": [
              "",
              "VAR _Required = DIVIDE( DATEDIFF([Report Production Date], [Now Date], SECOND), 60 )",
              "",
              "VAR _Actual =",
              "    CALCULATE(",
              "        SUM( 'Upstream Log Simplified'[DurationMinute] ),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] >= [Report Date]",
              "        ),",
              "        FILTER(",
              "            'Upstream Log Simplified',",
              "            'Upstream Log Simplified'[Start Time] <= [Now Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "    DIVIDE( _Actual, _Required )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Planning Schedule"
          }
        ]
      },

            },
            {
              "name": "PY Var",
              "expression": [
                "VAR PreviousPeriod = CALCULATE( [Expenses Amount], SAMEPERIODLASTYEAR('Calendar'[Date]) )",
                "RETURN",
                "    IF (",
                "        NOT ISBLANK( [Expenses Amount] )",
                "            && NOT ISBLANK( PreviousPeriod ),",
                "        [Expenses Amount] - PreviousPeriod",
                "    )"

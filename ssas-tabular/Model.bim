              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Sales Invoice'[SalesDiscount] ),",
              "        \"Local\", SUMX( 'Sales Invoice',  'Sales Invoice'[SalesDiscount] * 'Sales Invoice'[Exchange Rate] ),",
              "        SUM('Sales Invoice'[SalesDiscount] )",
              "    ),",
              "    SUM('Sales Invoice'[SalesDiscount] )    ",
              ")"
            ],

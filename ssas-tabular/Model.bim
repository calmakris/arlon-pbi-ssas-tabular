          {
            "name": "COMP Actual Hours",
            "description": "Sum of 'Compounding Log'[Actual Hours Spent]",
            "expression": "SUM( 'Compounding Log'[Actual Hours Spent] )",
            "formatString": "#,0.00",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Projected Direct Hours",
            "description": "Sum of 'Compounding Log'[Projected Hours Spent] where [Is Labor] is 1 and [Task] is not 'MIXING'",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Compounding Log'[Projected Hours Spent] ),",
              "    KEEPFILTERS( 'Compounding Log'[Labor Flag] = \"Yes\" ),",
              "    KEEPFILTERS( 'Compounding Log'[Task] <> \"MIXING\" )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Indirect Actual Hours",
            "description": "Sum of 'Compounding Log'[Actual Hours Spent] where [Is Indirect] equals to 1",
            "expression": [
              "",
              "CALCULATE(",
              "    [COMP Actual Hours],",
              "    KEEPFILTERS( 'Compounding Log'[Indirect Flag] = \"Yes\" )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Labor Efficiency",
            "description": "[COMP Projected Direct Hours] divided by the difference between [COMP Actual Hours], [COMP Indirect Actual Hours], and [COMP Actual Hours] where [Task] is 'TRAINING'",
            "expression": [
              "",
              "VAR training = ",
              "    CALCULATE(",
              "        [COMP Actual Hours],",
              "        KEEPFILTERS( 'Compounding Log'[Task] = \"TRAINING\" )",
              "    )",
              "",
              "RETURN",
              "DIVIDE(",
              "    [COMP Projected Direct Hours],",
              "    [COMP Actual Hours] - [COMP Indirect Actual Hours] + training",
              ")"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Non-Production as % of Direct Labor",
            "description": "[COMP Actual Hours] where [Task] is 'NON-PRODUCTION (5S/OTHER)' divided by [COMP Actual Hours]",
            "expression": [
              "",
              "DIVIDE(",
              "    CALCULATE(",
              "        [COMP Actual Hours],",
              "        KEEPFILTERS( 'Compounding Log'[Task] = \"NON-PRODUCTION (5S/OTHER)\" )",
              "    ),",
              "    [COMP Actual Hours] - [COMP Indirect Actual Hours]",
              ")",
              ""
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP Shop Order Count",

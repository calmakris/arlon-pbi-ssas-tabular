          {
            "name": "Intercompany Sales",
            "description": "Total revenue amount of intercompany billing documents as determined by their invoice document type.",
            "expression": [
              "CALCULATE(",
              "    [Gross Sales],",
              "    FILTER(",
              "        ALL('Sales Invoice'[Document Type]),",
              "        'Sales Invoice'[Document Type] IN {\"IV\", \"IVA\", \"IVS\"}",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Invoice"
          },
          {
            "name": "Net Inventory Quantity After Order Reservations",
            "description": "Difference between unrestricted stock quantity and cumulative total of open order reservations. Requirement date of the order reservation is the date to determine when the reservation consumes the quantity.",
            "expression": [
              "",
              "VAR _Today = [Report Date] + 1",
              "",
              "VAR _MinVisibleDate = MIN( 'Calendar'[Date] )",
              "",
              "VAR _Inventory =",
              "    IF(",
              "        [Unrestricted Stock Quantity] = 0,",
              "        BLANK(),",
              "        [Unrestricted Stock Quantity]",
              "    )",
              "",
              "VAR _MaxReqDateWithData = ",
              "    CALCULATE(",
              "        MAX( 'Process Order Reservation'[Requirement Date] ),",
              "        ALL( 'Calendar' )",
              "    )",
              "",
              "VAR _AllRequirement = ",
              "    CALCULATE( ",
              "        SUM( 'Process Order Reservation'[Requirement Quantity] ), ",
              "        ALL( 'Calendar' ) ",
              "    )",
              "",
              "VAR _HasRequirement =",
              "    _AllRequirement <> BLANK() && _AllRequirement <> 0",
              "    ",
              "VAR _CumulativeTotal =",
              "    CALCULATE(",
              "        SUM( 'Process Order Reservation'[Requirement Quantity] ),",
              "        FILTER(",
              "            ALL( 'Calendar'[Date] ),",
              "            'Calendar'[Date] <= MAX( 'Calendar'[Date] )",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF(",
              "    _MinVisibleDate >= _Today,",
              "    _Inventory + _CumulativeTotal + IF( _HasRequirement, 0 )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "MRP"
          },
          {
            "name": "Operations Scrap Quantity",
            "description": "Sum of quantities of non process order scrap from accounting documents, scrap confirmation from process orders, process order scrap from goods issue material document transactions, and inventory reserve from material document transactions.",
            "expression": [
              "",
              "VAR PO_Scrap =",
              "    CALCULATE(",
              "        SUM( 'Goods Movement'[Quantity] ) * -1,",
              "        KEEPFILTERS('Scrap Category'[Category] = \"Process Order Scrap\")",
              "    )",
              "",
              "-- Use G/L account items instead because it has correct debit/credit indicators",
              "VAR NonPO_Scrap =",
              "    CALCULATE(",
              "        SUM( 'Scrap Usage'[Quantity] ),",
              "        kEEPFILTERS('Cost Element'[Category] = \"Scrap\"),",
              "        KEEPFILTERS('Cost Element'[Sub Category] = \"Non-PO Scrap\"),",
              "        KEEPFILTERS('Cost Element'[Operations Scrap Flag] = \"Yes\")",
              "    )",
              "",
              "-- ZSCRAP tcode only has confirmations on CAST orders",
              "VAR Scrap_Conf = SUM( 'Scrap Confirmation'[Quantity] )",
              "",
              "VAR Inv_Reserve =",
              "    CALCULATE(",
              "        SUM( 'Goods Movement'[Quantity] ) * -1,",
              "        KEEPFILTERS('Cost Element'[Category] = \"Inventory Reserve\"),",
              "        KEEPFILTERS('Cost Element'[Operations Scrap Flag] = \"Yes\")",
              "    )",
              "",
              "RETURN",
              "PO_Scrap + NonPO_Scrap + Scrap_Conf + Inv_Reserve"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Operations Scrap"
          },
          {
            "name": "Gross Sales Discount",
            "description": "Total discount applied to the gross sales revenue. This number is different from cash discount or the discount applied to the selling price based on price book or sales quote.",
            "expression": [

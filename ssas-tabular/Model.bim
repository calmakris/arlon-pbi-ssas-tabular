            "expression": [
              "",
              "VAR RefDate =",
              "    CALCULATE(",
              "        MAX('Calendar'[Date]),",
              "        FILTER( 'Calendar', 'Calendar'[Date] <= [Report Date])",
              "    )",
              "",

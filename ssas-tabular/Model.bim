          {
            "name": "Annual Inventory Consumption Next 12 Months",
            "description": "If time supply < 12, then [Current Unrestricted Stock Quantity], else, [Average Monthly Consumption (Last 12 Months)] * 12. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "IF(",
              "    [Inventory Time Supply] < 12,",
              "    [Unrestricted Stock Quantity],",
              "    [Avg Monthly Inventory Consumption Last 12 Months] * 12",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory\\_Consumption"
          },
          {
            "name": "Annual Inventory Consumption After 12th Month",
            "description": "If unrestricted stock is equal to consumption in the next 12 months, then 0. Else if consumption in the last 12 months is the same as the difference of current stock and consumption in the next 12 months, then value is the consumption in the next 12 months. Otherwise, value is the difference between current stock and consumption in the next 12 months. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "VAR Usage = [Avg Monthly Inventory Consumption Last 12 Months] * 12",
              "VAR Net = [Unrestricted Stock Quantity] - [Annual Inventory Consumption Next 12 Months]",
              "RETURN",
              "    SWITCH(",
              "        TRUE(),",
              "        [Unrestricted Stock Quantity] = [Annual Inventory Consumption Next 12 Months], BLANK(),",
              "        Usage < Net, Usage,",
              "        Net",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory\\_Consumption"
          },
          {
            "name": "Annual Inventory Consumption After 24th Month",
            "description": "If unrestricted stock is equal to consumption in the next 24 months, then 0. Else if consumption in the last 12 months is the same as the difference of current stock and consumption in the next 24 months, then value is the consumption in the next 24 months. Otherwise, value is the difference between current stock and consumption in the next 24 months. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "VAR Usage = [Avg Monthly Inventory Consumption Last 12 Months] * 12",
              "VAR Net = [Unrestricted Stock Quantity] - [Annual Inventory Consumption Next 12 Months] - [Annual Inventory Consumption After 12th Month]",
              "RETURN",
              "    SWITCH(",
              "        TRUE(),",
              "        [Unrestricted Stock Quantity] = ([Annual Inventory Consumption Next 12 Months] + [Annual Inventory Consumption After 12th Month]), BLANK(),",
              "        Usage < Net, Usage,",
              "        Net",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory\\_Consumption"
          },
          {
            "name": "Annual Inventory Consumption After 36th Month",
            "description": "If unrestricted stock is equal to consumption in the next 36 months, then 0. Else if consumption in the last 12 months is the same as the difference of current stock and consumption in the next 36 months, then value is the consumption in the next 36 months. Otherwise, value is the difference between current stock and consumption in the next 36 months. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "VAR Usage = [Avg Monthly Inventory Consumption Last 12 Months] * 12",
              "VAR Net = [Unrestricted Stock Quantity] - [Annual Inventory Consumption Next 12 Months] - [Annual Inventory Consumption After 12th Month] - [Annual Inventory Consumption After 24th Month]",
              "RETURN",
              "    SWITCH(",
              "        TRUE(),",
              "        [Unrestricted Stock Quantity] = ([Annual Inventory Consumption Next 12 Months] + [Annual Inventory Consumption After 12th Month] + [Annual Inventory Consumption After 24th Month]), BLANK(),",
              "        Usage < Net, Usage,",
              "        Net",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory\\_Consumption"
          },
          {
            "name": "WH Picked Weight",
            "description": [
              "Total weight of materials picked from finished good racks (starting with X) and 901 555.",
              "",
              "Data is based on the weight in transfer orders done by users in ORDER ASSEMBLY and WAREHOUSE OPERATIONS departments. Departments are assigned in the user data in SAP."
            ],
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Transfer Order'[Weight] ),",
              "    FILTER(",
              "        'DIM_UsersSAP',",
              "        'DIM_UsersSAP'[Department] IN { \"ORDER ASSEMBLY\", \"WAREHOUSE OPERATIONS\" }",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Warehouse"
          },
          {
            "name": "Purchase Order Open Amount CW",
            "description": "Total amount of open purchase orders scheduled to be delivered this week (based on the static first delivery date in the purchase order)",
            "expression": [
              " ",
              "VAR BegDate = ([Report Date] + 1) - WEEKDAY( [Report Date] + 1 )",
              "VAR EndDate = BegDate + 7",
              "RETURN",
              "CALCULATE(",
              "    [Purchase Order Open Amount],",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] >= BegDate",
              "            && 'Calendar'[Date] <= EndDate",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Purchase Order\\_Time-Based"
          },
          {
            "name": "Purchase Order Goods Receipt Amount CW",
            "description": "Total amount of materials in purchase orders for incoming inventory received this week",
            "expression": [
              " ",
              "VAR BegDate = ([Report Date] + 1) - WEEKDAY( [Report Date] + 1 )",
              "VAR EndDate = BegDate + 7",
              "RETURN",
              "CALCULATE(",
              "    ABS([GRIR Amount in Local Currency2]),",
              "    FILTER(",
              "        'Goods and Invoice Receipt',",
              "        'Goods and Invoice Receipt'[Document Type] IN {\"WE\", \"WL\"}",
              "    ),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] >= BegDate",
              "            && 'Calendar'[Date] <= EndDate",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Purchase Order\\_Time-Based"
          },
          {
            "name": "Purchase Order Goods Receipt Amount",
            "description": "Total amount of materials received for the purchase order line item",
            "expression": " SUM('Purchase Order'[Goods Receipt Amount])",
            "formatString": "#,0",
            "displayFolder": "Purchase Order"
          },
          {
            "name": "Sales Order Open Internal Amount CW",
            "description": [
              "Total amount in the sales order scheduled to ship this week that is not yet shipped nor allocated to the customer. Unlike the sales order open amount, this measure considers allocations and doesn't count them as open.",
              "",
              "This value is toggleable to local currency using the '_Currency To Use' table."
            ],
            "expression": [
              " ",
              "VAR BegDate = ([Report Date] + 1) - WEEKDAY( [Report Date] + 1 )",
              "VAR EndDate = BegDate + 7",
              "RETURN",
              "CALCULATE(",
              "    [Sales Order Open Internal Amount],",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] >= BegDate",
              "            && 'Calendar'[Date] <= EndDate",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Time-Based"
          },
          {
            "name": "Sales Order Shipped Amount CW",
            "description": [
              "Total amount of sales order line items that shipped (or, delivery notes with actual PGI date set) this week.",
              "",
              "This value is toggleable to local currency using the '_Currency to Use' table."
            ],
            "expression": [
              " ",
              "VAR BegDate = ([Report Date] + 1) - WEEKDAY( [Report Date] + 1 )",
              "VAR EndDate = BegDate + 7",
              "RETURN",
              "CALCULATE(",
              "    SUM( 'Sales Order'[ShippedAmountUSD] ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[Last PGI Date] ),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] >= BegDate",
              "            && 'Calendar'[Date] <= EndDate",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order\\_Time-Based"
          },
          {
            "name": "Sales Order Open Weight Month End",
            "description": "Weight of materials not yet shipped and scheduled to ship at the end of the month as determined by the First Requested Ship Date of the sales order",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Sales Order'[CustomerOpenWeight] ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] ),",
              "    FILTER(",
              "        ALL('Calendar'[Date]),",
              "        'Calendar'[Date] >= [Beginning of Month]",
              "            && 'Calendar'[Date] <= [End of Month]",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Sales Order"
          },
          {
            "name": "WH Storage Bin Material Age in Days",
            "description": "Average days between the material's goods receipt date and today",
            "expression": "AVERAGE( 'Inventory in Storage Bin'[Age in Days] )",
            "formatString": "#,0 \"days\"",
            "displayFolder": "Warehouse"
          },
          {
            "name": "Storage Bin Utilization Snapshot%",
            "description": [
              "Latest snapshot of the storage bin utilization depending on the date filter/context. If there is no date filter/context, then this value is yesterday's snapshot.",
              "",
              "Storage bin utilization is the average of the quants in the storage bin divided by the capacity of the storage bin."
            ],
            "expression": [
              "",
              "VAR RefDate =",
              "    CALCULATE(",
              "        MAX('Calendar'[Date]),",
              "        FILTER( 'Calendar', 'Calendar'[Date] <= [Report Date])",
              "    )",
              "",
              "RETURN",
              "    CALCULATE(",
              "        DIVIDE(",
              "            SUM( 'Storage Bin Utilization Snapshots'[Utilized] ),",
              "            SUM( 'Storage Bin Utilization Snapshots'[Capacity] )",
              "        ),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] = RefDate",
              "        )",
              "    )"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Inventory\\_Time-Based"
          },
          {
            "name": "Absorption CW",
            "description": "Current week's total cost posted in earned labor, setup, and overhead cost elements",
            "expression": [
              "",
              "VAR BegDate = ([Report Date]) - WEEKDAY( [Report Date] )",
              "VAR EndDate = BegDate + 7",
              "RETURN",
              "CALCULATE(",
              "    [Absorption],",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] >= BegDate",
              "            && 'Calendar'[Date] <= EndDate",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Absorption FYTD",
            "description": "Fiscal year-to-date total cost posted in earned labor, setup, and overhead cost elements",
            "expression": [
              "",
              "VAR _DayofFiscal =",
              "    DATEDIFF(",
              "        [Beginning of Fiscal Year],",
              "        [Report Date],",
              "        DAY",
              "    ) + 1",
              "RETURN",
              "    CALCULATE(",
              "        CALCULATE(",
              "            CALCULATE(",
              "                [Absorption],",
              "                FILTER(",
              "                    'Calendar',",
              "                    'Calendar'[Day of Fiscal Year] <= _DayofFiscal",
              "                )",
              "            ),",
              "            DATESYTD( 'Calendar'[Date], \"09/30\" )",
              "        ),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] <= [Report Date]",
              "        )",
              "    )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Absorption FYTD PY",
            "description": "Previous fiscal year-to-date total cost posted in earned labor, setup, and overhead cost elements",
            "expression": "CALCULATE( [Absorption FYTD], SAMEPERIODLASTYEAR('Calendar'[Date]) )",
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Absorption Hours MTD",
            "description": "Month-to-date quantity (in hours) posted in earned labor, setup, and overhead cost elements",
            "expression": [
              " CALCULATE( [Absorption Hours], DATESMTD( 'Calendar'[Date] ) )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Absorption Hours MTD PY",
            "description": "Previous year's month-to-date quantity (in hours) posted in earned labor, setup, and overhead cost elements",
            "expression": [
              " CALCULATE( [Absorption Hours MTD], SAMEPERIODLASTYEAR( 'Calendar'[Date] ) )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Time-Based"
          },
          {
            "name": "Actual Production Start Timestamp",
            "description": "Actual datetime the production per department started since the beginning of the month. This value is from machine data. If not available, the beginning of month datetime is used.",
            "expression": [
              "",
              "VAR StartTime = ",
              "    CALCULATE(",
              "        MIN( 'Upstream Log Simplified'[Start Time] ),",
              "        ALL( 'Calendar' ),",
              "        FILTER(",
              "            ALL( 'Upstream Log Simplified'[Start Time] ),",
              "            'Upstream Log Simplified'[Start Time] >= [Beginning of Month]",
              "        ),",
              "        'Upstream Log Simplified'[Category] = \"Production\"",
              "    )",
              "",
              "RETURN",
              "IF( ISBLANK(StartTime) && [Absorption MTD] <> BLANK(), [Beginning of Month] + TIME(0, 0, 0), StartTime )"
            ],
            "formatString": "MM/dd/yyyy HH:mm:ss",
            "displayFolder": "Absorption\\_Projection",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralLongDateTime\" FormatString=\"MM/dd/yyyy HH:mm:ss\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Scheduled Production End Timestamp",
            "description": "The last scheduled finish date available in the schedule data. If not available, the value used is today's end datetime before midnight.",
            "expression": [
              "",
              "VAR SchedEnd = ",
              "    CALCULATE(",
              "        MAX( 'Planning Schedule'[Plan Finish Date] ),",
              "        USERELATIONSHIP( 'Calendar'[Date], 'Planning Schedule'[PlanFinishDateAltKey] ),",
              "        ALL( 'Calendar'[Date] ),",
              "        FILTER(",
              "            'Planning Schedule',",
              "            'Planning Schedule'[PlanFinishDateAltKey] <= [End of Month]",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF( ISBLANK(SchedEnd) && [Absorption MTD] <> BLANK(), [Report Date] + TIME(23, 59, 59), SchedEnd )"
            ],
            "formatString": "MM/dd/yyyy HH:mm:ss",
            "displayFolder": "Absorption\\_Projection",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralLongDateTime\" FormatString=\"MM/dd/yyyy HH:mm:ss\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Absorption Estimated based on Scheduled Output by Month End",
            "description": "[MTD Absorption] plus the product of [S/O Future Scheduled Output by Month End (Upstream)] and the average absorption per output in the last 3 calendar months ([Absorption per Output (Last 3 Months)])",
            "expression": [
              "",
              "SUMX( VALUES('Department'[Department]), [Absorption MTD] + ([Process Order Scheduled Output by Month End] * [Absorption per Output Last 3 Months]) )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Projection"
          },
          {
            "name": "Absorption Estimated Daily Avg",
            "description": "[Estimated Absorption based on Scheduled Output by Month End] divided by the number of days between [Actual Production Start Datetime] and [Scheduled Production Stop Datetime]",
            "expression": [
              "",
              "DIVIDE(",
              "    [Absorption Estimated based on Scheduled Output by Month End],",
              "    DIVIDE( DATEDIFF( [Actual Production Start Timestamp], [Scheduled Production End Timestamp], MINUTE ), 1440 )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Projection"
          },
          {
            "name": "Absorption Projected",
            "description": "[Average Daily Absorption based on Estimate] multiplied by the number of days between [Actual Product Start Datetime] and an end date. If current date is before 15th day of the month, the end date is the calendar month end datetime before midnight. Otherwise, it is the [Scheduled Production Stop Datetime].",
            "expression": [
              "",
              "VAR EndDate = [End of Month] + TIME(23, 59, 59)",
              "",
              "VAR MinVisibleDate = MIN('Calendar'[Date])",
              "",
              "VAR Proj =",
              "    SUMX( ",
              "        VALUES('Department'[Department]), ",
              "        [Absorption Estimated based on Scheduled Output by Month End]",
              "            + ([Absorption Estimated Daily Avg] * DIVIDE((DATEDIFF([Scheduled Production End Timestamp], EndDate, MINUTE) - ([Production Off Hours After Schedule End] * 60)), 1440)) ",
              "    )",
              "",
              "RETURN",
              "-- Only project current month",
              "-- Display MTD of previous month only. Otherwise, blank",
              "IF(",
              "    ISFILTERED( 'Calendar' ),",
              "    IF(",
              "        MinVisibleDate < [Beginning of Last Month],",
              "        BLANK(),",
              "        IF(",
              "            MinVisibleDate < [Beginning of Month],",
              "            [Absorption MTD],",
              "            Proj",
              "        )",
              "    ),",
              "    Proj",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption\\_Projection"
          },
          {
            "name": "Production Off Hours After Schedule End",
            "expression": [
              "",
              "VAR BegDate = ",
              "    DATE(",
              "        YEAR( [Scheduled Production End Timestamp] ),",
              "        MONTH( [Scheduled Production End Timestamp] ),",
              "        DAY( [Scheduled Production End Timestamp])",
              "    )",
              "",
              "RETURN",
              "CALCULATE(",
              "    SUM( 'FACT_DepartmentOffDay'[Off Hours] ),",
              "    ALL( 'Calendar'[Date] ),",
              "    FILTER(",
              "        ALL( 'FACT_DepartmentOffDay'[Date] ),",
              "        'FACT_DepartmentOffDay'[Date] > BegDate",
              "            && 'FACT_DepartmentOffDay'[Date] <= [End of Month]",
              "    )",
              ")"
            ],
            "formatString": "0.00",
            "displayFolder": "Absorption\\_Projection"
          },
          {
            "name": "Sales Order Line Count by First Ship Date",
            "description": "Count of sales order line items by first scheduled ship date",
            "expression": [
              "",
              "CALCULATE(",
              "    CALCULATE(",
              "        [Sales Order Line Count],",
              "        USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] )",
              "    ),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Report Date]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "WH Effective On Time Shipment Target",
            "description": "Monthly target values supplied by Logistics to the IT department annually. If there is no value available, the default values are 95% for OTS and 100% for T1 OTS.",
            "expression": [
              "",
              "VAR KeyVal =",
              "    IF(",
              "        HASONEVALUE( 'Plant Data for Material'[Tier] )",
              "            && SELECTEDVALUE( 'Plant Data for Material'[Tier] ) = \"A\",",
              "        \"OTS_T1\",",
              "        \"OTS\"",
              "    )",
              "",
              "VAR Target =",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = KeyVal,",
              "        TREATAS(",
              "            VALUES( Plant[Plant] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF(",
              "    ISBLANK( Target ),",
              "    SWITCH(",
              "        KeyVal,",
              "        \"OTS\", 0.95,",
              "        \"OTS_T1\", 1.00,",
              "        0.95",
              "    ),",
              "    Target",
              ")",
              ""
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Warehouse"
          },
          {
            "name": "Sales Order Line Count by First Ship Date On Time",
            "description": "Count of sales order line items where the ordered quantity is fully shipped on or before the first scheduled ship date",
            "expression": [
              "",
              "VAR OnTime =",
              "    CALCULATE(",
              "        [Sales Order Line Count by First Ship Date],",
              "        KEEPFILTERS('Sales Order'[Late Flag] = \"No\"),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "-- Days with lines but no on time should show 0",
              "IF(",
              "    [Sales Order Line Count by First Ship Date] <> BLANK(),",
              "    OnTime + 0,",
              "    BLANK()",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Sales Order Line Count by First Ship Date Late Exempt",
            "description": "Count of sales order line items where the ordered quantity is not fully delivered on or before the first scheduled ship date but marked as 'No' under 'IsArlonFault' column in the ZSDSHIP_LATE tcode in SAP",
            "expression": [
              "",
              "VAR Exempt =",
              "    CALCULATE(",
              "        [Sales Order Line Count by First Ship Date],",
              "        KEEPFILTERS('Sales Order'[Late Flag] = \"Yes\"),",
              "        KEEPFILTERS('Sales Order'[Reason Type for Late Shipment] = \"3rd Party\"),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF(",
              "    [Sales Order Line Count by First Ship Date] <> BLANK(),",
              "    Exempt + 0,",
              "    BLANK()",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Order"
          },
          {
            "name": "WH Complete On Time Target",
            "description": "Monthly target values supplied by Logistics to the IT department annually. If there is no value available, the default value is 90%.",
            "expression": [
              "",
              "VAR Target =",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"COT\",",
              "        TREATAS(",
              "            VALUES( Plant[Plant] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF(",
              "    ISBLANK( Target ),",
              "    0.90,",
              "    Target",
              ")",
              ""
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Complete On Time Hidden",
            "expression": [
              "",
              "AVERAGEX(",
              "    VALUES( 'Customer Master'[Customer Name] ),",
              "    IF(",
              "        CALCULATE(",
              "            [Sales Order Line Count by First Ship Date],",
              "            'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "        ) = BLANK(),",
              "        BLANK(),",
              "        IF(",
              "            CALCULATE(",
              "                [Sales Order Line Count by First Ship Date],",
              "                'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "            ) = CALCULATE(",
              "                [Sales Order Line Count by First Ship Date On Time] + [Sales Order Line Count by First Ship Date Late Exempt],",
              "                'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "            ),",
              "            1,",
              "            0",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "isHidden": true,
            "displayFolder": "Warehouse"
          },
          {
            "name": "Sales Order Late Quantity",
            "description": "Count of sales order line items where the ordered quantity is not fully delivered on or before its first scheduled ship date in the schedule line",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Sales Order'[LateQuantity] ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] ),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Report Date]",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Sales Order"
          },
          {
            "name": "Operations Scrap as % of GR Target",
            "description": "Monthly KPI target manually assigned in the beginning of the fiscal year. Operations provides this data to IT for upload.",
            "expression": [
              "",
              "CALCULATE(",
              "    AVERAGE( TARGET_KPI[TargetValue] ),",
              "    TARGET_KPI[KpiKey] = \"ScrapGR\",",
              "    TREATAS(",
              "        VALUES( 'Company Code'[Company Code] ),",
              "        TARGET_KPI[LocationKey]",
              "    ),",
              "    TREATAS(",
              "        VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "        TARGET_KPI[CalendarTimeKey]",
              "    )",
              ")"
            ],
            "formatString": "#,0.0%;-#,0.0%;#,0.0%",
            "displayFolder": "Operations Scrap"
          },
          {
            "name": "Inventory Amount Closing Prior Month",
            "description": "Inventory snapshot amount at the end of month last month",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Inventory Snapshots'[Amount] ),",
              "    FILTER( ",
              "        'Calendar',",
              "        'Calendar'[Date] = [End of Prior Month]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Time-Based"
          },
          {
            "name": "Inventory Amount Opening Prior Week Var",
            "description": "Difference between the inventory snapshot amount on Sundays of current and prior week",
            "expression": [
              "",
              "IF(",
              "    OR( ISBLANK( [Inventory Amount Opening Current Week] ), ISBLANK( [Inventory Amount Opening Prior Week] ) ),",
              "    BLANK(),",
              "    [Inventory Amount Opening Current Week] - [Inventory Amount Opening Prior Week]",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Time-Based"
          },
          {
            "name": "Inventory Amount Closing Prior 2 Months Var",
            "description": "Difference between the inventory amount snapshots of last month and the month before last month",
            "expression": [
              "",
              "IF(",
              "    OR( ISBLANK( [Inventory Amount Closing Prior Month] ), ISBLANK( [Inventory Amount Closing 2 Months Ago] ) ),",
              "    BLANK(),",
              "    [Inventory Amount Closing Prior Month] - [Inventory Amount Closing 2 Months Ago]",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Time-Based"
          },
          {
            "name": "Inventory Material Cost",
            "description": "Product of unrestricted stock quantity and current material cost per unit",
            "expression": "SUM( 'Inventory'[Material Cost (USD)] )",
            "formatString": "#,0",
            "displayFolder": "Inventory\\_Cost of Inventory"
          },
          {
            "name": "Inventory Labor Cost",
            "description": "Product of unrestricted stock quantity and current labor cost per unit",
            "expression": "SUM( 'Inventory'[Labor Cost (USD)] )",
            "formatString": "#,0",

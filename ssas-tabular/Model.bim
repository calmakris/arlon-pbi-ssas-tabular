            "formatString": "#,0.00",
            "summarizeBy": "sum"
          }
        ],
        "partitions": [
          {
            "name": "CalculatedTable 1",
            "source": {
              "type": "calculated",
              "expression": [
                "",
                "VAR Actuals =",
                "    SUMMARIZE(",
                "        FILTER(",
                "            'Upstream Logs - Raw',",
                "            'Upstream Logs - Raw'[Stop Time] >= [Report Production Date] - 14",
                "                && 'Upstream Logs - Raw'[Start Time] <= [Now Date]",
                "                && 'Upstream Logs - Raw'[Category] = \"Production\"",
                "                && 'Upstream Logs - Raw'[Process Order Number] <> \"000000000000\"",
                "        ),",
                "        'Upstream Logs - Raw'[Machine Name],",
                "        'Upstream Logs - Raw'[Process Order Number],",
                "        \"Plan Start\", BLANK(),",
                "        \"Plan Stop\", BLANK(),",
                "        \"Actual Start\", MIN( 'Upstream Logs - Raw'[Start Time] ),",
                "        \"Actual Stop\", MAX( 'Upstream Logs - Raw'[Stop Time] ),",
                "        \"Plan Ft\", BLANK(),",
                "        \"Actual Ft\", SUM( 'Upstream Logs - Raw'[Footage] ),",
                "        \"Plan Dur\", BLANK(),",
                "        \"Actual Dur\", SUM( 'Upstream Logs - Raw'[Duration Minutes] ) / 60.0",
                "    )",
                "",
                "VAR Schedule =",
                "    SUMMARIZE(",
                "        FILTER(",
                "            'Upstream Schedule',",
                "            'Upstream Schedule'[Plan Finish Date] >= [Report Production Date] - 14",
                "                && 'Upstream Schedule'[Plan Start Date] < [Report Production Date] + 14",
                "                && 'Upstream Schedule'[Publish Date] <= [Now Date]",
                "                && 'Upstream Schedule'[Ranking Desc] = 1",
                "                && 'Upstream Schedule'[Target Quantity] > 0",
                "        ),",
                "        'Upstream Schedule'[Machine Name],",
                "        'Upstream Schedule'[Process Order Number],",
                "        \"Plan Start\", MIN( 'Upstream Schedule'[Plan Start Date] ),",
                "        \"Plan Stop\", MAX( 'Upstream Schedule'[Plan Finish Date] ),",
                "        \"Actual Start\", BLANK(),",
                "        \"Actual Stop\", BLANK(),",
                "        \"Plan Ft\", SUM( 'Upstream Schedule'[Target Quantity] ),",
                "        \"Actual Ft\", BLANK(),",
                "        \"Plan Dur\", SUM( 'Upstream Schedule'[Processing Hour] ),",
                "        \"Actual Dur\", BLANK()",
                "    )",
                "",
                "RETURN ",
                "    UNION(Actuals, Schedule)"
              ]
            }
          }
        ]

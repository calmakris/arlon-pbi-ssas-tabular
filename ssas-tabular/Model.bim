          {
            "name": "COMP Shop Order Success Count",
            "expression": "[COMP Shop Order Count] - [COMP Shop Order Fail Count]",
            "formatString": "0",
            "displayFolder": "Compounding"
          },
          {
            "name": "COMP First Pass Reliability",
            "expression": [
              "",

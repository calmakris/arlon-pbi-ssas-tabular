        "partitions": [
          {
            "name": "CalculatedTable 1",
            "source": {
              "type": "calculated",
              "expression": [
                "",
                "SUMMARIZE(",
                "    'PREP_MachineStatus',",
                "    'PREP_MachineStatus'[Machine],",
                "    'PREP_MachineStatus'[Process Order Number],",
                "    \"Plan Start Time\", MIN( 'PREP_MachineStatus'[Plan Start] ),",
                "    \"Plan Stop Time\", MAX( 'PREP_MachineStatus'[Plan Stop] ),",
                "    \"Actual Start Time\", MIN( 'PREP_MachineStatus'[Actual Start] ),",
                "    \"Actual Stop Time\", MAX( 'PREP_MachineStatus'[Actual Stop] ),",
                "    \"Plan Footage\", SUM( 'PREP_MachineStatus'[Plan Ft] ),",
                "    \"Actual Footage\", SUM( 'PREP_MachineStatus'[Actual Ft] ),",
                "    \"Plan Duration Hour\", SUM( 'PREP_MachineStatus'[Plan Dur] ),",
                "    \"Actual Duration Hour\", SUM( 'PREP_MachineStatus'[Actual Dur] ),",
                "    \"Status\",",
                "        SWITCH( TRUE(),",
                "            NOT(ISBLANK( MIN( 'PREP_MachineStatus'[Actual Start] ) ) )",
                "                && ISBLANK( MIN( 'PREP_MachineStatus'[Plan Start] ) )",
                "                && MAX( 'PREP_MachineStatus'[Actual Stop] ) >= [Report Production Date]",
                "                && MIN( 'PREP_MachineStatus'[Actual Start] ) <= [Now Date], -1,",
                "            ISBLANK( MIN( 'PREP_MachineStatus'[Actual Start] ) )",
                "                && NOT(ISBLANK( MIN( 'PREP_MachineStatus'[Plan Start] ) ) )",
                "                && MAX( 'PREP_MachineStatus'[Plan Stop] ) >= [Report Production Date]",
                "                && MAX( 'PREP_MachineStatus'[Plan Stop] ) <= [Now Date], 0,",
                "            NOT(ISBLANK( MIN( 'PREP_MachineStatus'[Actual Start] ) ) )",
                "                && NOT(ISBLANK( MIN( 'PREP_MachineStatus'[Plan Start] ) ) )",
                "                && MAX( 'PREP_MachineStatus'[Actual Stop] ) >= [Report Production Date]",
                "                && MIN( 'PREP_MachineStatus'[Actual Start] ) <= [Now Date], 1,",
                "            NOT( ISBLANK( MIN( 'PREP_MachineStatus'[Actual Start] ) ) )",
                "                && MIN( 'PREP_MachineStatus'[Actual Start] ) < [Report Production Date]",
                "                && MIN( 'PREP_MachineStatus'[Actual Start] ) >= [Report Production Date] - 2, -2,",
                "            NOT( ISBLANK( MIN( 'PREP_MachineStatus'[Plan Start] ) ) )",
                "                && MIN( 'PREP_MachineStatus'[Plan Start] ) < [Report Production Date]",
                "                && MIN( 'PREP_MachineStatus'[Plan Start] ) >= [Report Production Date] - 2, -2,",
                "            BLANK()",
                "        )",
                ")"
              ]
            }
          }
        ]

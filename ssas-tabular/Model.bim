          {
            "name": "Inventory Time Supply",
            "description": "[Current Unrestricted Stock Quantity] divided by [Average Monthly Consumption (Last 12 Months)]. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "DIVIDE(",
              "    [Current Unrestricted Stock Quantity],",
              "    [Average Monthly Consumption (Last 12 Months)]",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory"
          },
          {
            "name": "Inventory Annual Consumption (Next 12 Months)",
            "description": "If time supply < 12, then [Current Unrestricted Stock Quantity], else, [Average Monthly Consumption (Last 12 Months)] * 12. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "IF(",
              "    [Inventory Time Supply] < 12,",
              "    [Current Unrestricted Stock Quantity],",
              "    [Average Monthly Consumption (Last 12 Months)] * 12",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory"
          },
          {
            "name": "Inventory Annual Consumption (After 12th Month)",
            "description": "If unrestricted stock is equal to consumption in the next 12 months, then 0. Else if consumption in the last 12 months is the same as the difference of current stock and consumption in the next 12 months, then value is the consumption in the next 12 months. Otherwise, value is the difference between current stock and consumption in the next 12 months. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "VAR Usage = [Average Monthly Consumption (Last 12 Months)] * 12",
              "VAR Net = [Current Unrestricted Stock Quantity] - [Inventory Annual Consumption (Next 12 Months)]",
              "RETURN",
              "    SWITCH(",
              "        TRUE(),",
              "        [Current Unrestricted Stock Quantity] = [Inventory Annual Consumption (Next 12 Months)], BLANK(),",
              "        Usage < Net, Usage,",
              "        Net",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory"
          },
          {
            "name": "Inventory Annual Consumption (After 24th Month)",
            "description": "If unrestricted stock is equal to consumption in the next 24 months, then 0. Else if consumption in the last 12 months is the same as the difference of current stock and consumption in the next 24 months, then value is the consumption in the next 24 months. Otherwise, value is the difference between current stock and consumption in the next 24 months. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "VAR Usage = [Average Monthly Consumption (Last 12 Months)] * 12",
              "VAR Net = [Current Unrestricted Stock Quantity] - [Inventory Annual Consumption (Next 12 Months)] - [Inventory Annual Consumption (After 12th Month)]",
              "RETURN",
              "    SWITCH(",
              "        TRUE(),",
              "        [Current Unrestricted Stock Quantity] = ([Inventory Annual Consumption (Next 12 Months)] + [Inventory Annual Consumption (After 12th Month)]), BLANK(),",
              "        Usage < Net, Usage,",
              "        Net",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory"
          },
          {
            "name": "Inventory Annual Consumption (After 36th Month)",
            "description": "If unrestricted stock is equal to consumption in the next 36 months, then 0. Else if consumption in the last 12 months is the same as the difference of current stock and consumption in the next 36 months, then value is the consumption in the next 36 months. Otherwise, value is the difference between current stock and consumption in the next 36 months. Can be toggled between quantity and amount values (default = quantity).",
            "expression": [
              "",
              "VAR Usage = [Average Monthly Consumption (Last 12 Months)] * 12",
              "VAR Net = [Current Unrestricted Stock Quantity] - [Inventory Annual Consumption (Next 12 Months)] - [Inventory Annual Consumption (After 12th Month)] - [Inventory Annual Consumption (After 24th Month)]",
              "RETURN",
              "    SWITCH(",
              "        TRUE(),",
              "        [Current Unrestricted Stock Quantity] = ([Inventory Annual Consumption (Next 12 Months)] + [Inventory Annual Consumption (After 12th Month)] + [Inventory Annual Consumption (After 24th Month)]), BLANK(),",
              "        Usage < Net, Usage,",
              "        Net",
              "    )"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory"
          },
          {
            "name": "Inventory Consumption (Dynamic)",
            "description": "Sum of 'Inventory Consumption'[Quantity].Can be toggled between quantity and amount values (default = quantity)",
            "expression": [
              "",
              "VAR Qty = SUM( 'Inventory Consumption'[Quantity] )",

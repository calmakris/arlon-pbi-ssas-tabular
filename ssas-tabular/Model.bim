            ],
            "formatString": "#,0",
            "displayFolder": "Testing"
          },
          {
            "name": "CW Absorption",
            "description": "Current week's total absorption (relative to [Report Date])",
            "expression": [
              "",
              "VAR BegDate = ([Report Date]) - WEEKDAY( [Report Date] )",
              "VAR EndDate = BegDate + 7",
              "RETURN",
              "CALCULATE(",
              "    [Absorption],",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] >= BegDate",
              "            && 'Calendar'[Date] <= EndDate",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "FYTD Absorption",
            "description": "Date grouping: 'Absorption'[Posting Date]",
            "expression": [
              "",
              "VAR _DayofFiscal =",
              "    DATEDIFF(",
              "        [Beginning of Fiscal Year],",
              "        [Report Date],",
              "        DAY",
              "    ) + 1",
              "RETURN",
              "    CALCULATE(",
              "        CALCULATE(",
              "            CALCULATE(",
              "                [Absorption],",
              "                FILTER(",
              "                    'Calendar',",
              "                    'Calendar'[Day of Fiscal Year] <= _DayofFiscal",
              "                )",
              "            ),",
              "            DATESYTD( 'Calendar'[Date], \"09/30\" )",
              "        ),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] <= [Report Date]",
              "        )",
              "    )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "FYTD PY Absorption",
            "description": "Date grouping: 'Absorption'[Posting Date]",
            "expression": "CALCULATE( [FYTD Absorption], SAMEPERIODLASTYEAR('Calendar'[Date]) )",
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "MTD Absorption Hours",
            "description": "Date grouping: 'Absorption'[Posting Date]",
            "expression": [
              " CALCULATE( [Absorption Hours], DATESMTD( 'Calendar'[Date] ) )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "MTD PY Absorption Hours",
            "description": "Date grouping: 'Absorption'[Posting Date]",
            "expression": [
              " CALCULATE( [MTD Absorption Hours], SAMEPERIODLASTYEAR( 'Calendar'[Date] ) )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "CONV Linear Footage",
            "expression": [
              "",
              "CALCULATE(",
              "    [GR Linear Footage],",
              "    KEEPFILTERS('Goods Movement'[Machine] IN {\"014\", \"088\", \"182\", \"733\", \"734\", \"735\", \"736\", \"737\", \"762\", \"MANUAL\"}),",
              "    KEEPFILTERS('Goods Movement'[Storage Bin] IN {\"600\", \"700\", \"733\", \"734\", \"735\", \"736\", \"737\", \"88\", \"182\", \"782\", \"555\"}),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Goods Movement'[Production Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Converting"
          },
          {
            "name": "S/O Future Scheduled Output by Month End (Upstream)",
            "description": "Total target quantity of scheduled shop orders after today until end of month (relative to [Report Date]). Scheduled data is the from the latest schedule published by Planning.",
            "expression": [
              "",
              "CALCULATE(",
              "    [S/O Scheduled Quantity (Upstream)],",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Upstream Schedule'[PlanFinishDateAltKey] ),",
              "    ALL( 'Calendar'[Date] ),",
              "    FILTER(",
              "        ALL( 'Upstream Schedule'[PlanFinishDateAltKey] ),",
              "        'Upstream Schedule'[PlanFinishDateAltKey] > [Report Date]",
              "            && 'Upstream Schedule'[PlanFinishDateAltKey] <= [End of Month]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption (Projected)"
          },
          {
            "name": "Absorption per Output (Last 3 Months)",
            "description": "Total absorption posted in the last 3 calendar months divided by the total output of departments. Casting and coating output is from machine data, Converting output is GR transaction, and others are delivered quantity of shop orders that ended in the last 3 months.",
            "expression": [
              "",
              "VAR BegDate = EDATE([Beginning of Month], -3)",
              "VAR Upstream_Out = ",
              "    CALCULATE(",
              "        [Upstream Actual Production Footage],",
              "        ALL( 'Calendar' ),",
              "        FILTER(",
              "            ALL( 'Upstream Log Simplified'[Start Time] ),",
              "            'Upstream Log Simplified'[Start Time] >= BegDate",
              "                && 'Upstream Log Simplified'[Start Time] < [Beginning of Month]",
              "        )",
              "    )",
              "",
              "VAR Conv_Out = ",
              "    CALCULATE(",
              "        [CONV Linear Footage],",
              "        ALL( 'Calendar' ),",
              "        FILTER(",
              "            ALL( 'Goods Movement'[Posting Date] ),",
              "            'Goods Movement'[Posting Date] >= BegDate",
              "                && 'Goods Movement'[Posting Date] < [Beginning of Month]",
              "        )",
              "    )",
              "",
              "VAR Other_Out = ",
              "    CALCULATE(",
              "        SUM('Process Order'[DeliveredQuantity] ),",
              "        ALL( 'Calendar' ),",
              "        FILTER(",
              "            ALL( 'Process Order'[EndDateKey] ),",
              "            'Process Order'[EndDateKey] >= BegDate",
              "                && 'Process Order'[EndDateKey] < [Beginning of Month]",
              "        ),",
              "        'Process Order'[Close Flag] = \"Yes\"",
              "    )",
              "",
              "RETURN",
              "DIVIDE(",
              "    CALCULATE(",
              "        [Absorption],",
              "        FILTER(",
              "            ALL( 'Calendar'[Date] ),",
              "            'Calendar'[Date] >= BegDate",
              "                && 'Calendar'[Date] < [Beginning of Month]",
              "        )",
              "    ),",
              "    SWITCH(",
              "        SELECTEDVALUE( 'Department'[Department] ),",
              "        \"CONV\", Conv_Out,",
              "        \"CAST\", Upstream_Out,",
              "        \"COAT\", Upstream_Out,",
              "        Other_Out",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Absorption (Projected)"
          },
          {
            "name": "Actual Production Start Datetime",
            "description": "Actual datetime the production per department started since the beginning of the month. This value is from machine data. If not available, the beginning of month datetime is used.",
            "expression": [
              "",
              "VAR StartTime = ",
              "    CALCULATE(",
              "        MIN( 'Upstream Log Simplified'[Start Time] ),",
              "        ALL( 'Calendar' ),",
              "        FILTER(",
              "            ALL( 'Upstream Log Simplified'[Start Time] ),",
              "            'Upstream Log Simplified'[Start Time] >= [Beginning of Month]",
              "        ),",
              "        'Upstream Log Simplified'[Category] = \"Production\"",
              "    )",
              "",
              "RETURN",
              "IF( ISBLANK(StartTime) && [MTD Absorption] <> BLANK(), [Beginning of Month] + TIME(0, 0, 0), StartTime )"
            ],
            "formatString": "MM/dd/yyyy HH:mm:ss",
            "displayFolder": "Absorption (Projected)",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralLongDateTime\" FormatString=\"MM/dd/yyyy HH:mm:ss\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Scheduled Production Stop Datetime",
            "description": "The last scheduled finish date available in the schedule data. If not available, the value used is today's end datetime before midnight.",
            "expression": [
              "",
              "VAR SchedEnd = ",
              "    CALCULATE(",
              "        MAX( 'Upstream Schedule'[Plan Finish Date] ),",
              "        USERELATIONSHIP( 'Calendar'[Date], 'Upstream Schedule'[PlanFinishDateAltKey] ),",
              "        ALL( 'Calendar'[Date] ),",
              "        FILTER(",
              "            'Upstream Schedule',",
              "            'Upstream Schedule'[PlanFinishDateAltKey] <= [End of Month]",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF( ISBLANK(SchedEnd) && [MTD Absorption] <> BLANK(), [Report Date] + TIME(23, 59, 59), SchedEnd )"
            ],
            "formatString": "MM/dd/yyyy HH:mm:ss",
            "displayFolder": "Absorption (Projected)",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralLongDateTime\" FormatString=\"MM/dd/yyyy HH:mm:ss\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Estimated Absorption based on Scheduled Output by Month End",
            "description": "[MTD Absorption] plus the product of [S/O Future Scheduled Output by Month End (Upstream)] and the average absorption per output in the last 3 calendar months ([Absorption per Output (Last 3 Months)])",
            "expression": [
              "",
              "SUMX( VALUES('Department'[Department]), [MTD Absorption] + ([S/O Future Scheduled Output by Month End (Upstream)] * [Absorption per Output (Last 3 Months)]) )",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption (Projected)"
          },
          {
            "name": "Average Daily Absorption based on Estimate",
            "description": "[Estimated Absorption based on Scheduled Output by Month End] divided by the number of days between [Actual Production Start Datetime] and [Scheduled Production Stop Datetime]",
            "expression": [
              "",
              "DIVIDE(",
              "    [Estimated Absorption based on Scheduled Output by Month End],",
              "    DIVIDE( DATEDIFF( [Actual Production Start Datetime], [Scheduled Production Stop Datetime], MINUTE ), 1440 )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption (Projected)"
          },
          {
            "name": "Projected Absorption",
            "description": "[Average Daily Absorption based on Estimate] multiplied by the number of days between [Actual Product Start Datetime] and an end date. If current date is before 15th day of the month, the end date is the calendar month end datetime before midnight. Otherwise, it is the [Scheduled Production Stop Datetime].",
            "expression": [
              "",
              "VAR EndDate = [End of Month] + TIME(23, 59, 59)",
              "",
              "VAR MinVisibleDate = MIN('Calendar'[Date])",
              "",
              "VAR Proj =",
              "    SUMX( ",
              "        VALUES('Department'[Department]), ",
              "        [Estimated Absorption based on Scheduled Output by Month End]",
              "            + ([Average Daily Absorption based on Estimate] * DIVIDE((DATEDIFF([Scheduled Production Stop Datetime], EndDate, MINUTE) - ([Future Off Hours After Schedule End] * 60)), 1440)) ",
              "    )",
              "",
              "RETURN",
              "-- Only project current month",
              "-- Display MTD of previous month only. Otherwise, blank",
              "IF(",
              "    ISFILTERED( 'Calendar' ),",
              "    IF(",
              "        MinVisibleDate < [Beginning of Last Month],",
              "        BLANK(),",
              "        IF(",
              "            MinVisibleDate < [Beginning of Month],",
              "            [MTD Absorption],",
              "            Proj",
              "        )",
              "    ),",
              "    Proj",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Absorption (Projected)"
          },
          {
            "name": "Future Off Hours After Schedule End",
            "expression": [
              "",
              "VAR BegDate = ",
              "    DATE(",
              "        YEAR( [Scheduled Production Stop Datetime] ),",
              "        MONTH( [Scheduled Production Stop Datetime] ),",
              "        DAY( [Scheduled Production Stop Datetime])",
              "    )",
              "",
              "RETURN",
              "CALCULATE(",
              "    SUM( 'FACT_DepartmentOffDay'[Off Hours] ),",
              "    ALL( 'Calendar'[Date] ),",
              "    FILTER(",
              "        ALL( 'FACT_DepartmentOffDay'[Date] ),",
              "        'FACT_DepartmentOffDay'[Date] > BegDate",
              "            && 'FACT_DepartmentOffDay'[Date] <= [End of Month]",
              "    )",
              ")"
            ],
            "formatString": "0.00",
            "displayFolder": "Absorption (Projected)"
          },
          {
            "name": "Absorption Hours",
            "expression": "SUM( Absorption[Quantity] )",
            "formatString": "#,0.00",
            "displayFolder": "Absorption"
          },
          {
            "name": "WH Shipment Quantity",
            "description": "Total weight of material transactions with 601, 641, 643, and Z43 movement types. Weight is given in the material master data.",
            "expression": [
              "",
              "ABS(",
              "    CALCULATE(",
              "        SUM( 'Inventory Consumption'[Quantity] ),",
              "        FILTER(",
              "            'Inventory Consumption',",
              "            'Inventory Consumption'[Movement Type] IN { \"601\", \"641\", \"643\", \"Z43\" }",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Future Shipment Amount",
            "expression": [
              "",
              "VAR MinVisibleDate = MIN('Calendar'[Date])",
              "",
              "RETURN",
              "-- Show previous month as actual ship amount",
              "-- Current and future shows actual ship plus customer open",
              "IF(",
              "    MinVisibleDate < [Beginning of Last Month],",
              "    BLANK(),",
              "    IF(",
              "        MinVisibleDate < [Beginning of Month],",
              "        [WH Shipment Amount],",
              "        [WH Shipment Amount] + [SO Customer Open Amount]",
              "    )",
              ")"
            ],
            "formatString": "\\$#,0;(\\$#,0);\\$#,0",
            "displayFolder": "Warehouse",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"Currency\"><Currency LCID=\"1033\" DisplayName=\"$ English (United States)\" Symbol=\"$\" PositivePattern=\"0\" NegativePattern=\"0\" /></Format>"
              }
            ]
          },
          {
            "name": "Sales Order Line Count (By First Ship Date)",
            "expression": [
              "",
              "CALCULATE(",
              "    [Sales Order Line Count],",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "WH Effective On Time Shipment",
            "description": "Daily average ratio of the number of on time lines plus exempted late lines divided by the number of lines. Date grouping: 'Sales Order Documents'[First Delivery Date].",
            "expression": [
              "",
              "VAR OnTimeLines =",
              "    CALCULATE(",
              "        [WH SO Line Count (On Time)] + [WH SO Line Count (Exempt)],",
              "        'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "    )",
              "",
              "VAR AllLines =",
              "    CALCULATE(",
              "        [WH SO Line Count (All)],",
              "        'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "    )",
              "",
              "VAR OTS = DIVIDE(OnTimeLines, AllLines)",
              "",
              "RETURN -- Weighted average by line count",
              "    CALCULATE(",
              "        DIVIDE(",
              "            SUMX(",
              "                VALUES( 'Calendar'[Date] ),",
              "                [WH SO Line Count (All)] * OTS",
              "            ),",
              "            [WH SO Line Count (All)]",
              "        ),",
              "        'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "    )"
            ],
            "formatString": "#,##0.00 %",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Effective On Time Shipment (Target)",
            "description": "Data Source: KPI Targets SharePoint List. If there is no value available, the default values are 95% for OTS and 100% for T1 OTS.",
            "expression": [
              "",
              "VAR KeyVal =",
              "    IF(",
              "        HASONEVALUE( 'Plant Data for Material'[Tier] )",
              "            && SELECTEDVALUE( 'Plant Data for Material'[Tier] ) = \"A\",",
              "        \"OTS_T1\",",
              "        \"OTS\"",
              "    )",
              "",
              "VAR Target =",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = KeyVal,",
              "        TREATAS(",
              "            VALUES( Plant[Plant] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF(",
              "    ISBLANK( Target ),",
              "    SWITCH(",
              "        KeyVal,",
              "        \"OTS\", 0.95,",
              "        \"OTS_T1\", 1.00,",
              "        0.95",
              "    ),",
              "    Target",
              ")",
              ""
            ],
            "formatString": "#,##0.00 %",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Complete On Time",
            "expression": [
              "",
              "CALCULATE(",
              "    DIVIDE(",
              "        SUMX(",
              "            VALUES( 'Calendar'[Date] ),",
              "            [WH Complete On Time (Hidden)] * [WH SO Line Count (All)]",
              "        ),",
              "        [WH SO Line Count (All)]",
              "    ),",
              "    'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              ")"
            ],
            "formatString": "#,##0.00 %",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH SO Line Count (All)",
            "expression": [
              "",
              "CALCULATE(",
              "    [Sales Order Line Count (By First Ship Date)],",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Report Date]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH SO Line Count (On Time)",
            "expression": [
              "",
              "VAR OnTime =",
              "    CALCULATE(",
              "        [Sales Order Line Count (By First Ship Date)],",
              "        KEEPFILTERS('Sales Order'[Late Flag] = \"No\"),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "-- Days with lines but no on time should show 0",
              "IF(",
              "    [WH SO Line Count (All)] <> BLANK(),",
              "    OnTime + 0,",
              "    BLANK()",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH SO Line Count (Exempt)",
            "expression": [
              "",
              "VAR Exempt =",
              "    CALCULATE(",
              "        [Sales Order Line Count (By First Ship Date)],",
              "        KEEPFILTERS('Sales Order'[Late Flag] = \"Yes\"),",
              "        KEEPFILTERS('Sales Order'[Reason Type for Late Shipment] = \"3rd Party\"),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] <= [Report Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF(",
              "    [WH SO Line Count (All)] <> BLANK(),",
              "    Exempt + 0,",
              "    BLANK()",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Complete On Time (Target)",
            "description": "Data Source: KPI Targets SharePoint List. If there is no value available, the default value is 90%.",
            "expression": [
              "",
              "VAR Target =",
              "    CALCULATE(",
              "        AVERAGE( TARGET_KPI[TargetValue] ),",
              "        TARGET_KPI[KpiKey] = \"COT\",",
              "        TREATAS(",
              "            VALUES( Plant[Plant] ),",
              "            TARGET_KPI[LocationKey]",
              "        ),",
              "        TREATAS(",
              "            VALUES( 'Calendar'[Year_Month_Sort_Key] ),",
              "            TARGET_KPI[CalendarTimeKey]",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF(",
              "    ISBLANK( Target ),",
              "    0.90,",
              "    Target",
              ")",
              ""
            ],
            "formatString": "#,##0.00 %",
            "displayFolder": "Warehouse"
          },
          {
            "name": "WH Shipment Calendar List",
            "expression": [
              "",
              "CONCATENATEX( ",
              "    'Sales Order Shipment Calendar', ",
              "    LEFT([Customer Name], 15) & UNICHAR(10) & \"#\" & VALUE([Order Number]) & \" (\" & [Order Type] & \")\",",
              "    UNICHAR(10),",
              "    [Order Amount], DESC",
              ")"
            ],
            "displayFolder": "Warehouse"
          },
          {
            "name": "Last Working Day",
            "expression": [
              "",
              "CALCULATE(",
              "    MAX( 'Calendar'[Date] ),",
              "    FILTER(",
              "        ALLSELECTED( 'Calendar' ),",
              "        'Calendar'[Date] <= [Report Date]",
              "            && 'Calendar'[Is Weekday]",
              "    )",
              ")"
            ],
            "formatString": "Short Date",
            "isHidden": true,
            "displayFolder": ".Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeShortDatePattern\" />"
              }
            ]
          },
          {
            "name": "WH Complete On Time (Hidden)",
            "expression": [
              "",
              "AVERAGEX(",
              "    VALUES( 'Customer Master'[Customer Name] ),",
              "    IF(",
              "        CALCULATE(",
              "            [WH SO Line Count (All)],",
              "            'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "        ) = BLANK(),",
              "        BLANK(),",
              "        IF(",
              "            CALCULATE(",
              "                [WH SO Line Count (All)],",
              "                'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "            ) = CALCULATE(",
              "                [WH SO Line Count (On Time)] + [WH SO Line Count (Exempt)],",
              "                'Sales Order'[Late Exclude Flag] <> \"Yes\"",
              "            ),",
              "            1,",
              "            0",
              "        )",
              "    )",
              ")"
            ],
            "formatString": "#,##0.00 %",
            "isHidden": true,
            "displayFolder": "Warehouse"
          },
          {
            "name": "SO Late Quantity",
            "description": "Difference between [SO Order Quantity] and [SO Shipped On Time Quantity]",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM( 'Sales Order'[LateQuantity] ),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Sales Order'[First Scheduled Ship Date] ),",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Report Date]",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Sales Orders"
          },
          {
            "name": "WH Days of Perfect On Time Shipment",
            "description": "Number of days when [WH Effective On Time Shipment] equals to 100%",
            "expression": [
              "",
              "VAR CountPerfectDays =",
              "    COUNTROWS(",
              "        FILTER(",
              "            VALUES('Calendar'[Date]),",
              "            [WH Effective On Time Shipment] >= 1",
              "        )",
              "    )",
              "",
              "RETURN",
              "CALCULATE(",
              "    CountPerfectDays + 0,",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] <= [Last Working Day]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Warehouse"
          },
          {
            "name": "Cost of Goods Sold",
            "description": "Sum of amount of line items posted under 0057020000 G/L account",
            "expression": "SUM( 'Cost of Goods Sold'[Amount] )",
            "formatString": "#,0",
            "displayFolder": "General"
          },
          {
            "name": "Operations Scrap Amount",
            "description": "Sum of non process order scrap from 'Scrap Usage Transactions' table, 'Scrap Confirmations'[Amount], and process order scrap and inventory reserve amount in 'Goods Issues and Receipts' table.",
            "expression": [
              "",
              "VAR PO_Scrap =",
              "    CALCULATE(",
              "        SUM( 'Goods Movement'[Amount] ) * -1,",
              "        KEEPFILTERS('Scrap Category'[Category] = \"Process Order Scrap\")",
              "    )",
              "",
              "-- Use G/L account items instead because it has correct debit/credit indicators",
              "VAR NonPO_Scrap =",
              "    CALCULATE(",
              "        SUM( 'Scrap Usage'[Amount] ),",
              "        kEEPFILTERS('Cost Element'[Category] = \"Scrap\"),",
              "        KEEPFILTERS('Cost Element'[Sub Category] = \"Non-PO Scrap\"),",
              "        KEEPFILTERS('Cost Element'[Operations Scrap Flag] = \"Yes\")",
              "    )",
              "",
              "-- ZSCRAP tcode only has confirmations on CAST orders",
              "VAR Scrap_Conf = SUM( 'Scrap Confirmation'[Amount] )",
              "",
              "VAR Inv_Reserve =",
              "    CALCULATE(",
              "        SUM( 'Goods Movement'[Amount] ) * -1,",
              "        KEEPFILTERS('Cost Element'[Category] = \"Inventory Reserve\"),",
              "        KEEPFILTERS('Cost Element'[Operations Scrap Flag] = \"Yes\")",
              "    )",
              "",
              "RETURN",
              "PO_Scrap + NonPO_Scrap + Scrap_Conf + Inv_Reserve"
            ],
            "formatString": "#,0",
            "displayFolder": "Operations Scrap"
          },
          {
            "name": "CONV Scrap Rate",
            "expression": [
              "CALCULATE(",
              "    [GI Scrap Rate],",
              "    KEEPFILTERS('Goods Movement'[Machine] IN {\"014\", \"088\", \"182\", \"733\", \"734\", \"735\", \"736\", \"737\", \"762\", \"MANUAL\"}),",
              "    KEEPFILTERS('Goods Movement'[Storage Bin] IN {\"600\", \"700\", \"733\", \"734\", \"735\", \"736\", \"737\", \"88\", \"182\", \"782\", \"555\"}),",
              "    USERELATIONSHIP( 'Calendar'[Date], 'Goods Movement'[Production Date] )",
              ")"
            ],
            "formatString": "#,0.00%;-#,0.00%;#,0.00%",
            "displayFolder": "Converting"
          },
          {

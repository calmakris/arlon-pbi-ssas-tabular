                "    \"Actual Footage\", SUM( 'Machine Status - Prep'[Actual Ft] ),",
                "    \"Plan Duration Hour\", SUM( 'Machine Status - Prep'[Plan Dur] ),",
                "    \"Actual Duration Hour\", SUM( 'Machine Status - Prep'[Actual Dur] ),",
                "    \"Status\",",
                "        SWITCH( TRUE(),",
                "            NOT(ISBLANK( MIN( 'Machine Status - Prep'[Actual Start] ) ) )",
                "                && ISBLANK( MIN( 'Machine Status - Prep'[Plan Start] ) )",
                "                && MAX( 'Machine Status - Prep'[Actual Stop] ) >= [Report Production Date]",
                "                && MIN( 'Machine Status - Prep'[Actual Start] ) <= [Now Date], -1,",
                "            ISBLANK( MIN( 'Machine Status - Prep'[Actual Start] ) )",
                "                && NOT(ISBLANK( MIN( 'Machine Status - Prep'[Plan Start] ) ) )",
                "                && MAX( 'Machine Status - Prep'[Plan Stop] ) >= [Report Production Date]",
                "                && MAX( 'Machine Status - Prep'[Plan Stop] ) <= [Now Date], 0,",
                "            NOT(ISBLANK( MIN( 'Machine Status - Prep'[Actual Start] ) ) )",
                "                && NOT(ISBLANK( MIN( 'Machine Status - Prep'[Plan Start] ) ) )",
                "                && MAX( 'Machine Status - Prep'[Actual Stop] ) >= [Report Production Date]",
                "                && MIN( 'Machine Status - Prep'[Actual Start] ) <= [Now Date], 1,",
                "            NOT( ISBLANK( MIN( 'Machine Status - Prep'[Actual Start] ) ) )",
                "                && MIN( 'Machine Status - Prep'[Actual Start] ) < [Report Production Date]",
                "                && MIN( 'Machine Status - Prep'[Actual Start] ) >= [Report Production Date] - 2, -2,",
                "            NOT( ISBLANK( MIN( 'Machine Status - Prep'[Plan Start] ) ) )",
                "                && MIN( 'Machine Status - Prep'[Plan Start] ) < [Report Production Date]",
                "                && MIN( 'Machine Status - Prep'[Plan Start] ) >= [Report Production Date] - 2, -2,",
                "            BLANK()",
                "        )",
                ")"
              ]
            }
          }
        ]
      },

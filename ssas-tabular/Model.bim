          {
            "type": "calculatedTableColumn",
            "name": "Actual Dur",
            "dataType": "decimal",
            "isNameInferred": true,
            "isHidden": true,
            "sourceColumn": "[Actual Dur]",
            "formatString": "#,0.00",
            "isAvailableInMdx": false,
            "summarizeBy": "sum"
          },
          {
            "type": "calculatedTableColumn",
            "name": "Process Order Number",
            "dataType": "string",
            "isNameInferred": true,
            "isDataTypeInferred": true,
            "sourceColumn": "Upstream Log[Process Order Number]",
            "isAvailableInMdx": false
          },
          {
            "type": "calculatedTableColumn",
            "name": "Machine",
            "dataType": "string",
            "isNameInferred": false,
            "isDataTypeInferred": true,
            "sourceColumn": "Upstream Log[Machine]",
            "isAvailableInMdx": false
          }
        ],
        "partitions": [
          {
            "name": "CalculatedTable 1",
            "source": {
              "type": "calculated",
              "expression": [
                "",
                "VAR Actuals =",
                "    SUMMARIZE(",
                "        FILTER(",
                "            'Upstream Log',",
                "            'Upstream Log'[Stop Time] >= [Report Production Date] - 14",
                "                && 'Upstream Log'[Start Time] <= [Now Date]",
                "                && 'Upstream Log'[Category] = \"Production\"",
                "                && 'Upstream Log'[Process Order Number] <> \"000000000000\"",
                "        ),",
                "        'Upstream Log'[Machine],",
                "        'Upstream Log'[Process Order Number],",
                "        \"Plan Start\", BLANK(),",
                "        \"Plan Stop\", BLANK(),",
                "        \"Actual Start\", MIN( 'Upstream Log'[Start Time] ),",
                "        \"Actual Stop\", MAX( 'Upstream Log'[Stop Time] ),",
                "        \"Plan Ft\", BLANK(),",
                "        \"Actual Ft\", SUM( 'Upstream Log'[Footage] ),",
                "        \"Plan Dur\", BLANK(),",
                "        \"Actual Dur\", SUM( 'Upstream Log'[Duration Minutes] ) / 60.0",
                "    )",
                "",
                "VAR Schedule =",
                "    SUMMARIZE(",
                "        FILTER(",
                "            'Upstream Schedule',",
                "            'Upstream Schedule'[Plan Finish Date] >= [Report Production Date] - 14",
                "                && 'Upstream Schedule'[Plan Start Date] < [Report Production Date] + 14",
                "                && 'Upstream Schedule'[Publish Date] <= [Now Date]",
                "                && 'Upstream Schedule'[Ranking Desc] = 1",
                "                && 'Upstream Schedule'[Target Quantity] > 0",
                "        ),",
                "        'Upstream Schedule'[Machine],",
                "        'Upstream Schedule'[Process Order Number],",
                "        \"Plan Start\", MIN( 'Upstream Schedule'[Plan Start Date] ),",
                "        \"Plan Stop\", MAX( 'Upstream Schedule'[Plan Finish Date] ),",
                "        \"Actual Start\", BLANK(),",
                "        \"Actual Stop\", BLANK(),",
                "        \"Plan Ft\", SUM( 'Upstream Schedule'[Target Quantity] ),",
                "        \"Actual Ft\", BLANK(),",
                "        \"Plan Dur\", SUM( 'Upstream Schedule'[Processing Hour] ),",
                "        \"Actual Dur\", BLANK()",
                "    )",
                "",
                "RETURN ",
                "    UNION(Actuals, Schedule)"
              ]
            }
          }
        ]
      },

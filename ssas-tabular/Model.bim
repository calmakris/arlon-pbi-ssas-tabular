            "formatString": "#,##0.0 \"hr\"",
            "displayFolder": "Machine Status"
          },
          {
            "name": "MStat Skipped S/O Duration Hour",
            "description": "Total duration of process orders scheduled between current production date and now. Skipped process orders have data in schedule files but none in machine data.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM('Machine Status'[Plan Duration Hour] ),",
              "    FILTER(",
              "        'Machine Status',",
              "        'Machine Status'[Actual Start Time] = BLANK()",
              "            && 'Machine Status'[Plan Stop Time] >= [Report Production Date]",
              "            && 'Machine Status'[Plan Stop Time] <= [Now Date]",
              "    )",
              ")"
            ],
            "formatString": "#,##0.0 \"hr\"",
            "displayFolder": "Machine Status"
          },
          {
            "name": "MStat Effective Net Hours Loss/Gain",
            "description": "Difference between [Net Hours Loss/Gain] and [Skipped S/O Duration Hours] measures.",
            "expression": "[MStat Net Hours Loss/Gain] - [MStat Skipped S/O Duration Hour]",
            "formatString": "#,##0.0 \"hr\"",
            "displayFolder": "Machine Status"
          },
          {
            "name": "MStat Net Footage Variance",
            "description": "Variance between the actual and scheduled footage between start of current production date and now.",
            "expression": [
              "",
              "VAR Actual = ",
              "    CALCULATE(",
              "        SUM( 'Machine Status'[Actual Footage] ),",
              "        FILTER(",
              "            'Machine Status',",
              "            'Machine Status'[Actual Stop Time] >= [Report Production Date]",
              "                && 'Machine Status'[Actual Stop Time] <= [Now Date]",
              "                && 'Machine Status'[Plan Start Time] <> BLANK()",
              "        )",
              "    )",
              "VAR Plan = ",
              "    CALCULATE(",
              "        SUM( 'Machine Status'[Plan Footage] ),",
              "        FILTER(",
              "            'Machine Status',",
              "            'Machine Status'[Actual Stop Time] >= [Report Production Date]",
              "                && 'Machine Status'[Actual Stop Time] <= [Now Date]",
              "                && 'Machine Status'[Plan Start Time] <> BLANK()",
              "        )",
              "    )",
              "RETURN",
              "    Actual - Plan",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Machine Status"
          },
          {
            "name": "MStat Data Integrity",
            "description": "Percentage of which the production log has data (has start and stop datetimes) within a given timeframe. The closer to 100% the better but it is expected the number to be not exactly 100%. Anything above 100% indicates the log has overlapping records which means there is an error in the data logging. When the value is low, see the last datetime stamp recorded then ask operators for more details.",
            "expression": [
              "",
              "VAR Required = DATEDIFF([Prior Production Date], [Now Date], SECOND) / 60",
              "VAR Actual = ",
              "    CALCULATE(",
              "        SUM( 'Upstream Logs - Simplified'[Duration Minutes] ),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] >= [Report Date] - 1",
              "        ),",
              "        FILTER(",
              "            'Upstream Logs - Simplified',",
              "            'Upstream Logs - Simplified'[Start Time] <= [Now Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "    DIVIDE( Actual, Required )"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Machine Status"
          },
          {
            "name": "Prior Production Date",
            "expression": " ([Report Date] - 1) + TIME(6, 0, 0)",
            "formatString": "MM/dd/yyyy HH:mm",
            "displayFolder": ".Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralShortDateTime\" FormatString=\"MM/dd/yyyy HH:mm\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Now Date",
            "expression": "NOW() + TIME(0,0,0)",
            "formatString": "MM/dd/yyyy HH:mm",
            "displayFolder": ".Dates",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralShortDateTime\" FormatString=\"MM/dd/yyyy HH:mm\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "Last Backorder Snapshot Amount",
            "expression": [
              "",
              "VAR RefDate =",
              "    CALCULATE(",
              "        MAX('Calendar'[Date]),",
              "        FILTER( 'Calendar', 'Calendar'[Date] <= [Report Date])",
              "    )",
              "",
              "RETURN",
              "    CALCULATE(",
              "        SUM('Backorder Snapshots'[Outgoing Amount]),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] = RefDate",
              "        )",
              "    )"
            ],
            "formatString": "\\$#,0;(\\$#,0);\\$#,0",
            "displayFolder": "Time Intelligence",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"Currency\"><Currency LCID=\"1033\" DisplayName=\"$ English (United States)\" Symbol=\"$\" PositivePattern=\"0\" NegativePattern=\"0\" /></Format>"
              }
            ]
          },
          {
            "name": "Last Shortage Amount% Snapshot",
            "expression": [
              "",
              "VAR RefDate =",
              "    CALCULATE(",
              "        MAX('Calendar'[Date]),",
              "        FILTER( 'Calendar', 'Calendar'[Date] <= [Report Date])",
              "    )",
              "",
              "VAR LastShortage = ",
              "    CALCULATE(",
              "        SUM( 'Backorder Snapshots'[Shortage Amount] ),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] = RefDate",
              "        )",
              "    )",
              "",
              "RETURN",
              "    DIVIDE( LastShortage, [Last Inventory Amount] )"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "MTD Accumulated Inventory Balance",
            "description": "Accumulated balance as of this month relative to [Report Date]",
            "expression": [
              "",
              "VAR LastVisibleDate =",
              "    MAX ( 'Calendar'[Date] )",
              "VAR FirstVisibleDate =",
              "    MIN ( 'Calendar'[Date] )",
              "VAR LastDateWithData =",
              "    CALCULATE (",
              "        MAX ( 'Inventory Balance (Totals)'[MonthDateKey] ),",
              "        REMOVEFILTERS ()",
              "    )",
              "VAR Result =",
              "    IF (",
              "        FirstVisibleDate <= LastDateWithData,",
              "        CALCULATE (",
              "            SUM( 'Inventory Balance (Totals)'[Amount (Group Currency)] ),",
              "            'Calendar'[Date] <= LastVisibleDate ",
              "        )",
              "    )",
              "RETURN",
              "    Result"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Prior Accumulated Inventory Balance",
            "description": "Accumulated balance as of end of last month relative to [Report Date]",
            "expression": [
              "",
              "CALCULATE(",
              "    [MTD Accumulated Inventory Balance],",
              "    DATEADD( 'Calendar'[Date], -1, MONTH )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "Inventory Balance",
            "expression": [
              "",
              "IF(",
              "    HASONEVALUE( '_Currency to Use'[Currency] ),",
              "    SWITCH(",
              "        VALUES( '_Currency to Use'[Currency] ),",
              "        \"USD\", SUM( 'Inventory Balance (Items)'[Amount (Local Currency 2)] ),",
              "        \"Local\", SUM( 'Inventory Balance (Items)'[Amount (Local Currency)] ),",
              "        SUM( 'Inventory Balance (Items)'[Amount (Local Currency 2)] )",
              "    ),",
              "    SUM( 'Inventory Balance (Items)'[Amount (Local Currency)] )  ",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Inventory Balance (Purchases)",
            "expression": [
              "",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"Purchases\"",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Inventory Balance (Produced)",
            "expression": [
              "",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"Produced\"",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Inventory Balance (Transfers)",
            "expression": [
              "",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"Transfers\"",
              ") +",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Document Type] IN {\"AB\", \"ZA\"},",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"\"",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Inventory Balance (Sales)",
            "expression": [
              "",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"Sales\"",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Inventory Balance (Scrap)",
            "expression": [
              "",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"Scrap\"",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Inventory Balance (Write-offs)",
            "expression": [
              "",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"Write-offs\"",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Inventory Balance (Accruals)",
            "expression": [
              "",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"Accruals\"",
              ") +",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Document Type] = \"SA\",",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"\"",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Inventory Balance (Reversals)",
            "expression": [
              "",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"Reversals\"",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Inventory Balance (PPV)",
            "expression": [
              "",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"PPV\"",
              ") +",
              "CALCULATE(",
              "    [MTD Inventory Balance],",
              "    'Inventory Balance (Items)'[Document Type] = \"PR\",",
              "    'Inventory Balance (Items)'[Movement Type Category] = \"\"",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "MTD Inventory Balance",
            "expression": "CALCULATE( [Inventory Balance], DATESMTD( 'Calendar'[Date] ) )",
            "formatString": "#,0.00",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "Check Inventory Balance",
            "description": "Value is 1 (or TRUE) if the [MTD Accumulated Inventory Balance] from 'Inventory Balance (Totals)' equals the sum of [MTD Prior Accumulated Inventory Balance] from 'Inventory Balance (Totals)' and [MTD Inventory Balance] from 'Inventory Balance (Items)'",
            "expression": [
              "",
              "VAR breakdown = ",
              "    [MTD Inventory Balance (Purchases)]",
              "    + [MTD Inventory Balance (Produced)]",
              "    + [MTD Inventory Balance (Transfers)]",
              "    + [MTD Inventory Balance (Sales)]",
              "    + [MTD Inventory Balance (Scrap)]",
              "    + [MTD Inventory Balance (Write-offs)]",
              "    + [MTD Inventory Balance (Accruals)]",
              "    + [MTD Inventory Balance (Reversals)]",
              "    + [MTD Inventory Balance (PPV)]",
              "",
              "RETURN",
              "    IF(",
              "        ISBLANK( [MTD Prior Accumulated Inventory Balance] ),",
              "        BLANK(),",
              "        IF(",
              "            ROUND([MTD Prior Accumulated Inventory Balance] + [MTD Inventory Balance], 2) = ROUND([MTD Accumulated Inventory Balance], 2),",
              "            1,",
              "            0",
              "        )",
              "    )"
            ],
            "formatString": "0",
            "displayFolder": "Inventory Balance"
          },
          {
            "name": "End of Next 3 Months",

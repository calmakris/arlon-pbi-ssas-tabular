                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralShortDateTime\" FormatString=\"MM/dd/yyyy HH:mm\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "MStat Last Process Order Number",
            "description": "Last process order ran in the machine. This measure is designed to be analyzed by machine.",
            "expression": [
              "",
              "VAR LastActDate = ",
              "    CALCULATE(",
              "        MAX('Machine Status'[Actual Start Time]),",
              "        FILTER(",
              "            'Machine Status',",
              "            'Machine Status'[Plan Start Time] <> BLANK()",
              "                && 'Machine Status'[Actual Start Time] <> BLANK()",
              "                && 'Machine Status'[Actual Start Time] >= [Report Production Date]",
              "        )",
              "    )",
              "RETURN",
              "    LOOKUPVALUE('Machine Status'[Process Order Number],'Machine Status'[Actual Start Time], LastActDate, BLANK() )"
            ],
            "formatString": "\"0\"",
            "displayFolder": "Machine Status"
          },
          {
            "name": "MStat Last Actual Start Time",
            "description": "Start time of the last process order ran in the machine. This measure is designed when analyzing by machine.",
            "expression": "LOOKUPVALUE('Machine Status'[Actual Start Time],'Machine Status'[Process Order Number], [MStat Last Process Order Number], BLANK() )",
            "formatString": "MM/dd/yyyy HH:mm",
            "displayFolder": "Machine Status",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralShortDateTime\" FormatString=\"MM/dd/yyyy HH:mm\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "MStat Last Plan Start Time",
            "description": "Scheduled start time of last process order ran in the machine. This measure is designed when analyzing by machine.",
            "expression": "LOOKUPVALUE('Machine Status'[Plan Start Time],'Machine Status'[Process Order Number], [MStat Last Process Order Number], BLANK() )",
            "formatString": "MM/dd/yyyy HH:mm",
            "displayFolder": "Machine Status",
            "annotations": [
              {
                "name": "Format",
                "value": "<Format Format=\"DateTimeCustom\"><DateTimes><DateTime LCID=\"1033\" Group=\"GeneralShortDateTime\" FormatString=\"MM/dd/yyyy HH:mm\" /></DateTimes></Format>"
              }
            ]
          },
          {
            "name": "MStat Net Hours Loss/Gain",
            "description": "Difference between [Last Actual Start Time] and [Last Plan Start Time] measures in hours.",
            "expression": [
              "",
              "CALCULATE(",
              "    DIVIDE( DATEDIFF( [MStat Last Actual Start Time], [MStat Last Plan Start Time], MINUTE), 60 ),",
              "    FILTER(",
              "        'Machine Status',",
              "        'Machine Status'[Plan Start Time] <> BLANK()",
              "            && 'Machine Status'[Actual Stop Time] >= [Report Production Date]",
              "            && 'Machine Status'[Actual Start Time] <= [Now Date]",
              "    )",
              ")"
            ],
            "formatString": "#,##0.0 \"hr\"",
            "displayFolder": "Machine Status"
          },
          {
            "name": "MStat Skipped S/O Duration Hour",
            "description": "Total duration of process orders scheduled between current production date and now. Skipped process orders have data in schedule files but none in machine data.",
            "expression": [
              "",
              "CALCULATE(",
              "    SUM('Machine Status'[Plan Duration Hour] ),",
              "    FILTER(",
              "        'Machine Status',",
              "        'Machine Status'[Actual Start Time] = BLANK()",
              "            && 'Machine Status'[Plan Stop Time] >= [Report Production Date]",
              "            && 'Machine Status'[Plan Stop Time] <= [Now Date]",
              "    )",
              ")"
            ],
            "formatString": "#,##0.0 \"hr\"",
            "displayFolder": "Machine Status"
          },
          {
            "name": "MStat Effective Net Hours Loss/Gain",
            "description": "Difference between [Net Hours Loss/Gain] and [Skipped S/O Duration Hours] measures.",
            "expression": "[MStat Net Hours Loss/Gain] - [MStat Skipped S/O Duration Hour]",
            "formatString": "#,##0.0 \"hr\"",
            "displayFolder": "Machine Status"
          },
          {
            "name": "MStat Net Footage Variance",
            "description": "Variance between the actual and scheduled footage between start of current production date and now.",
            "expression": [
              "",
              "VAR Actual = ",
              "    CALCULATE(",
              "        SUM( 'Machine Status'[Actual Footage] ),",
              "        FILTER(",
              "            'Machine Status',",
              "            'Machine Status'[Actual Stop Time] >= [Report Production Date]",
              "                && 'Machine Status'[Actual Stop Time] <= [Now Date]",
              "                && 'Machine Status'[Plan Start Time] <> BLANK()",
              "        )",
              "    )",
              "VAR Plan = ",
              "    CALCULATE(",
              "        SUM( 'Machine Status'[Plan Footage] ),",
              "        FILTER(",
              "            'Machine Status',",
              "            'Machine Status'[Actual Stop Time] >= [Report Production Date]",
              "                && 'Machine Status'[Actual Stop Time] <= [Now Date]",
              "                && 'Machine Status'[Plan Start Time] <> BLANK()",
              "        )",
              "    )",
              "RETURN",
              "    Actual - Plan",
              ""
            ],
            "formatString": "#,0",
            "displayFolder": "Machine Status"
          },
          {
            "name": "MStat Data Integrity",
            "description": "Percentage of which the production log has data (has start and stop datetimes) within a given timeframe. The closer to 100% the better but it is expected the number to be not exactly 100%. Anything above 100% indicates the log has overlapping records which means there is an error in the data logging. When the value is low, see the last datetime stamp recorded then ask operators for more details.",
            "expression": [
              "",
              "VAR Required = DIVIDE( DATEDIFF([Prior Production Date], [Now Date], SECOND), 60 )",
              "VAR Actual = ",
              "    CALCULATE(",
              "        SUM( 'Upstream Log Simplified'[DurationMinute] ),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] >= [Report Date] - 1",
              "        ),",
              "        FILTER(",
              "            'Upstream Log Simplified',",
              "            'Upstream Log Simplified'[Start Time] <= [Now Date]",
              "        )",
              "    )",
              "",
              "RETURN",
              "    DIVIDE( Actual, Required )"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Machine Status"
          },

          {
            "name": "MTD Intercompany Sales",
            "expression": " CALCULATE( [Intercompany Sales], DATESMTD( 'Calendar'[Date] ) )",
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "Net Inventory Quantity After Order Reservations",
            "description": "[Current Inventory Quantity] minus the cumulative total of 'Shop Order Reservations'[Requirement Quantity] by [Requirement Date]. If this measure is grouped by 'Calendar'[Date], the current day is the [Current Inventory Quantity] and the future dates are the net quantity.",
            "expression": [
              "",
              "VAR _Today = [Report Date] + 1",
              "VAR MinVisibleDate = MIN( 'Calendar'[Date] )",
              "VAR MaxReqDateWithData = ",
              "    CALCULATE(",
              "        MAX( 'Process Order Reservation'[Requirement Date] ),",
              "        ALL( 'Calendar' )",
              "    )",
              "VAR CumulativeTotal =",
              "    CALCULATE(",
              "        SUM( 'Process Order Reservation'[Requirement Quantity] ),",
              "        FILTER(",
              "            ALLSELECTED( 'Calendar'[Date] ),",
              "            'Calendar'[Date] > _Today",
              "                && 'Calendar'[Date] <= MAX( 'Calendar'[Date] )",
              "        )",
              "    )",
              "",
              "RETURN",
              "IF(",
              "    MinVisibleDate = _Today,",
              "    [Current Inventory Quantity],",
              "    IF(",
              "        MinVisibleDate > _Today,",
              "        [Current Inventory Quantity] + CumulativeTotal",
              "    )",
              ")"
            ],
            "formatString": "#,0.00",
            "displayFolder": "MRP"
          },
          {
            "name": "Operations Scrap Quantity",
            "description": "Sum of non process order scrap from 'Scrap Usage Transactions' table, 'Scrap Confirmations'[Quantity], and process order scrap and inventory reserve quantity in 'Goods Issues and Receipts' table.",
            "expression": [
              "",
              "VAR PO_Scrap =",
              "    CALCULATE(",
              "        SUM( 'Goods Movement'[Quantity] ) * -1,",
              "        KEEPFILTERS('Scrap Category'[Category] = \"Process Order Scrap\")",
              "    )",
              "",
              "-- Use G/L account items instead because it has correct debit/credit indicators",
              "VAR NonPO_Scrap =",
              "    CALCULATE(",
              "        SUM( 'Scrap Usage'[Quantity] ),",
              "        kEEPFILTERS('Cost Element'[Category] = \"Scrap\"),",
              "        KEEPFILTERS('Cost Element'[Sub Category] = \"Non-PO Scrap\"),",
              "        KEEPFILTERS('Cost Element'[Operations Scrap Flag] = \"Yes\")",
              "    )",
              "",
              "-- ZSCRAP tcode only has confirmations on CAST orders",
              "VAR Scrap_Conf = SUM( 'Scrap Confirmation'[Quantity] )",
              "",
              "VAR Inv_Reserve =",
              "    CALCULATE(",
              "        SUM( 'Goods Movement'[Quantity] ) * -1,",
              "        KEEPFILTERS('Cost Element'[Category] = \"Inventory Reserve\"),",
              "        KEEPFILTERS('Cost Element'[Operations Scrap Flag] = \"Yes\")",
              "    )",
              "",
              "RETURN",
              "PO_Scrap + NonPO_Scrap + Scrap_Conf + Inv_Reserve"
            ],
            "formatString": "#,0.00",
            "displayFolder": "Operations Scrap"
          },

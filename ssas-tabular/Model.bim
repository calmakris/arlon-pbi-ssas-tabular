            "displayFolder": "Inventory Balance"
          },
          {
            "name": "Check Inventory Balance",
            "description": "Value is 1 (or TRUE) if the [MTD Accumulated Inventory Balance] from 'Inventory Balance (Totals)' equals the sum of [MTD Prior Accumulated Inventory Balance] from 'Inventory Balance (Totals)' and [MTD Inventory Balance] from 'Inventory Balance (Items)'",
            "expression": [
              "",
              "VAR breakdown = ",
              "    [MTD Inventory Balance (Purchases)]",
              "    + [MTD Inventory Balance (Produced)]",
              "    + [MTD Inventory Balance (Transfers)]",
              "    + [MTD Inventory Balance (Sales)]",
              "    + [MTD Inventory Balance (Scrap)]",
              "    + [MTD Inventory Balance (Write-offs)]",
              "    + [MTD Inventory Balance (Accruals)]",
              "    + [MTD Inventory Balance (Reversals)]",
              "    + [MTD Inventory Balance (PPV)]",
              "",
              "RETURN",
              "    IF(",
              "        ISBLANK( [MTD Prior Accumulated Inventory Balance] ),",
              "        BLANK(),",
              "        IF(",
              "            ROUND([MTD Prior Accumulated Inventory Balance] + [MTD Inventory Balance], 2) = ROUND([MTD Accumulated Inventory Balance], 2),",
              "            1,",
              "            0",
              "        )",
              "    )"
            ],
            "formatString": "0",
            "displayFolder": "Inventory Balance"

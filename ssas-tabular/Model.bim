            ],
            "formatString": "#,##0.00 %",
            "displayFolder": "Warehouse"
          },
          {
            "name": "Last Storage Bin Utilization Snapshot",
            "description": "Last value of snapshot of storage bin utilitization (from 'Storage Bin Utilization (Snapshots)' table)",
            "expression": [
              "",
              "VAR RefDate =",
              "    CALCULATE(",
              "        MAX('Calendar'[Date]),",
              "        FILTER( 'Calendar', 'Calendar'[Date] <= [Report Date])",
              "    )",
              "",
              "RETURN",
              "    CALCULATE(",
              "        DIVIDE(",
              "            SUM( 'Storage Bin Utilization (Snapshots)'[Utilized] ),",
              "            SUM( 'Storage Bin Utilization (Snapshots)'[Capacity] )",
              "        ),",
              "        FILTER(",
              "            'Calendar',",
              "            'Calendar'[Date] = RefDate",
              "        )",
              "    )"
            ],
            "formatString": "0.00%;-0.00%;0.00%",
            "displayFolder": "Time Intelligence"
          },
          {
            "name": "WH Storage Bin Quantity",

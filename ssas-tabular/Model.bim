            "expression": [
              " ",
              "CALCULATE( ",
              "    SUM( 'Backorder Snapshots'[Incoming] ), ",
              "    FILTER(",
              "        'Calendar',",
              "        'Calendar'[Date] = MAX( 'Calendar'[Date] ) - 1",
              "    ),",
              "    TREATAS(",
              "        VALUES( 'Material Master'[Material Type] ),",
              "        'Backorder Snapshots'[Material Type]",
              "    ),",
              "    TREATAS(",
              "        VALUES( 'Purchase Orders'[Document Type] ),",
              "        'Backorder Snapshots'[Order Type]",
              "    )",
              ")"
            ],
            "formatString": "#,0",
            "displayFolder": "Time Intelligence"

{
  "name": "Audit 55 Score as % of Target MTD",
  "description": "Month-to-date 5S score. If there is no date context/filter, this value is the current month's 5S score.",
  "expression": [
    "",
    "CALCULATE(",
    "    CALCULATE(",
    "        [Audit 5S Score as % of Target],",
    "        DATESMTD( 'Calendar'[Date] )",
    "    ),",
    "    FILTER(",
    "        'Calendar',",
    "        'Calendar'[Date] <= [Report Date]",
    "    )",
    ")"
  ],
  "formatString": "#,0.0%;-#,0.0%;#,0.0%",
  "displayFolder": "5S\\_Time-Based",
  "annotations": [
    {
      "name": "TabularEditor_InPerspective",
      "value": "[\"CS Dashboard\",\"Engineering\",\"Operations\",\"Planning\",\"QA Dashboard\",\"Warehouse\"]"
    }
  ]
}
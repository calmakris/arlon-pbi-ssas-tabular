{
  "name": "Audit 5S Score as % of Target MTD PM Var",
  "description": "Difference between month-to-date and previous month's score as a % of target",
  "expression": [
    "",
    "VAR PM =",
    "    CALCULATE(",
    "        CALCULATE(",
    "            [Audit 5S Score as % of Target],",
    "            PREVIOUSMONTH( DATESMTD('Calendar'[Date]) )",
    "        ),",
    "        FILTER(",
    "            'Calendar',",
    "            'Calendar'[Date] <= [Report Date]",
    "        )",
    "    )",
    "",
    "RETURN",
    "IF(",
    "    ISBLANK( [Audit 55 Score as % of Target MTD] ) || ISBLANK( PM ),",
    "    BLANK(),",
    "    [Audit 55 Score as % of Target MTD] - PM",
    ")"
  ],
  "formatString": "#,0.0%;-#,0.0%;#,0.0%",
  "displayFolder": "5S\\_Time-Based",
  "annotations": [
    {
      "name": "TabularEditor_InPerspective",
      "value": "[\"CS Dashboard\",\"Engineering\",\"Operations\",\"Planning\",\"QA Dashboard\",\"Warehouse\"]"
    }
  ]
}
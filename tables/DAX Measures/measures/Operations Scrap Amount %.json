{
  "name": "Operations Scrap Amount %",
  "expression": [
    "VAR grand_total =",
    "    CALCULATE (",
    "        [Operations Scrap Amount],",
    "        ALL ( 'Scrap Category'[Category] ),",
    "        ALL('Scrap Category'[Sub Category]),",
    "        ALL('Reason for Movement'[Reason Text for Movement])",
    "        ",
    "    )",
    "VAR Percentge =",
    "    DIVIDE ( [Operations Scrap Amount], grand_total, 0 )",
    "RETURN",
    "    Percentge"
  ],
  "formatString": "#,##0.00 %",
  "displayFolder": "Operations Scrap",
  "annotations": [
    {
      "name": "TabularEditor_InPerspective",
      "value": "[\"Operations\"]"
    }
  ]
}
{
  "name": "Q-Block Amount",
  "description": "Total standard cost of inventory on Q block",
  "expression": [
    "CALCULATE(",
    "    SUM( 'Inventory'[AmountUSD] ),",
    "    KEEPFILTERS( 'Inventory'[Stock Category] = \"Q\" )",
    ")"
  ],
  "formatString": "#,0",
  "displayFolder": "Inventory\\_Blocked",
  "annotations": [
    {
      "name": "TabularEditor_InPerspective",
      "value": "[\"Finance\",\"Inventory\",\"Planning\",\"Product Management\",\"QA Dashboard\"]"
    }
  ]
}